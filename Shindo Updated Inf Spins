game:GetService("Players").LocalPlayer.startevent:FireServer("band", "\128")

local player = game.Players.LocalPlayer
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local TextButton = Instance.new("TextButton")
local TextButton_2 = Instance.new("TextButton")

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.Position = UDim2.new(0.36, 0, 0.20, 0)
Frame.Size = UDim2.new(0, 330, 0, 150)

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
TextLabel.Size = UDim2.new(0, 330, 0, 90)
TextLabel.Font = Enum.Font.AmaticSC
TextLabel.Text = "Do not press Rejoin or Auto Rejoin!"
TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.TextScaled = true
TextLabel.TextSize = 35.000
TextLabel.TextWrapped = true

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(0, 230, 30)
TextButton.Position = UDim2.new(0, 0, 0.600000006, 0)
TextButton.Size = UDim2.new(0, 330, 0, 60)
TextButton.Font = Enum.Font.AmaticSC
TextButton.Text = "OK!"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextScaled = true
TextButton.TextSize = 40.000
TextButton.TextWrapped = true

function OKSpinTip()
    for i,v in pairs(player.PlayerGui:GetChildren()) do
        if v.Name == "ScreenGui" then
            v:Destroy()
        end
    end
    local player = game.Players.LocalPlayer
    local ScreenGui = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local OpenButtonFrame = Instance.new("Frame")
    local OpenButton = Instance.new("TextButton")
    local MinButton = Instance.new("TextButton")
    local TextLabel = Instance.new("TextLabel")
    local TextButton = Instance.new("TextButton")
    local TextButton2 = Instance.new("TextButton")
    local TextButton3 = Instance.new("TextButton")

    OpenButtonFrame.Visible = false
    Frame.Visible = true

    ScreenGui.Name = "ScreenGui"
    ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    ScreenGui.Parent = game.CoreGui
    ScreenGui.ResetOnSpawn = false

    Frame.Parent = ScreenGui
    Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Frame.Position = UDim2.new(0.6745, 0, 0.6, 0)
    Frame.Size = UDim2.new(0, 200, 0, 245)
    Frame.Draggable = true

    OpenButtonFrame.Parent = ScreenGui
    OpenButtonFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    OpenButtonFrame.Position = UDim2.new(0.98, 0, 0.85, 0)
    OpenButtonFrame.Size = UDim2.new(0, 30, 0, 30)
    OpenButtonFrame.Draggable = true

    OpenButton.Parent = OpenButtonFrame
    OpenButton.BackgroundColor3 = Color3.fromRGB(220, 30, 30)
    OpenButton.Position = UDim2.new(0, 0, 0, 0)
    OpenButton.Size = UDim2.new(0, 30, 0, 30)
    OpenButton.Font = Enum.Font.Gotham
    OpenButton.Text = "+"
    OpenButton.TextColor3 = Color3.fromRGB(0, 0, 0)
    OpenButton.TextScaled = true
    OpenButton.TextSize = 80.000
    OpenButton.TextWrapped = true

    function open()
        OpenButtonFrame.Visible = false
        Frame.Visible = true
    end
    OpenButton.MouseButton1Click:connect(open)

    TextLabel.Parent = Frame
    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
    TextLabel.Size = UDim2.new(0, 200, 0, 60)
    TextLabel.Font = Enum.Font.AmaticSC
    TextLabel.Text = "Updated Functions"
    TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
    TextLabel.TextScaled = true
    TextLabel.TextSize = 35.000
    TextLabel.TextWrapped = true

    MinButton.Parent = Frame
    MinButton.BackgroundColor3 = Color3.fromRGB(220, 30, 30)
    MinButton.Position = UDim2.new(1.005, 0, 0, 0)
    MinButton.Size = UDim2.new(0, 30, 0, 30)
    MinButton.Font = Enum.Font.Gotham
    MinButton.Text = "x"
    MinButton.TextColor3 = Color3.fromRGB(0, 0, 0)
    MinButton.TextScaled = true
    MinButton.TextSize = 80.000
    MinButton.TextWrapped = true

    function min()
        OpenButtonFrame.Visible = true
        Frame.Visible = false
    end
    MinButton.MouseButton1Click:connect(min)

    TextButton.Parent = Frame
    TextButton.BackgroundColor3 = Color3.fromRGB(0, 180, 30)
    TextButton.Position = UDim2.new(0, 0, 0.2500000006, 0)
    TextButton.Size = UDim2.new(0, 200, 0, 60)
    TextButton.Font = Enum.Font.AmaticSC
    TextButton.Text = "Reset Spins"
    TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
    TextButton.TextScaled = true
    TextButton.TextSize = 40.000
    TextButton.TextWrapped = true

    function reset()
        game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
    end
    TextButton.MouseButton1Click:connect(reset)

    TextButton3.Parent = Frame
    TextButton3.BackgroundColor3 = Color3.fromRGB(0, 180, 30)
    TextButton3.Position = UDim2.new(0, 0, 0.600000006, 0)
    TextButton3.Size = UDim2.new(0, 200, 0, 99)
    TextButton3.Font = Enum.Font.AmaticSC
    TextButton3.Text = "..."
    TextButton3.TextColor3 = Color3.fromRGB(0, 0, 0)
    TextButton3.TextScaled = false
    TextButton3.TextSize = 500.000
    TextButton3.TextWrapped = true

    function WHY()
        wait()
        game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "Updated Inf Spin",Duration = 1, })
        wait(1)
        game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "Press Reset Spins when you run out of spins",Duration = 3, })
        wait(2)
        game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "Press Keep Data when you got Elements or Bloodlines or Kenjutsu you wanted",Duration = 3, })
        wait(2)
        game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "Do not press Reset Spins or Auto Rejoin from the Bruh Hub GUI",Duration = 3.7, })
    end
    TextButton3.MouseButton1Click:connect(WHY)

    TextButton2.Parent = Frame
    TextButton2.BackgroundColor3 = Color3.fromRGB(0, 180, 30)
    TextButton2.Position = UDim2.new(0, 0, 0.500000006, 0)
    TextButton2.Size = UDim2.new(0, 200, 0, 60)
    TextButton2.Font = Enum.Font.AmaticSC
    TextButton2.Text = "Keep Data"
    TextButton2.TextColor3 = Color3.fromRGB(0, 0, 0)
    TextButton2.TextScaled = true
    TextButton2.TextSize = 40.000
    TextButton2.TextWrapped = true

    function keep()
        game:GetService("Players").LocalPlayer.startevent:FireServer("band", "Eye")
        wait()
        game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "You may now enter villages or leave game to save your data",Duration = 3, })
        wait(1)
    end
    TextButton2.MouseButton1Click:connect(keep)
end
TextButton.MouseButton1Click:connect(OKSpinTip)
