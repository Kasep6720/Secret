local window2 = library:Window("Credits")

library:Keybind("RightControl")

window1:Label("Basic Functions", Color3.fromRGB(127, 143, 166))

window1:Toggle("Speed",false,function(bool)
    speed = bool
end)
game:GetService('RunService').Stepped:connect(function()
    if speed then
        pcall(function()
   game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = 50
        end)
    end
end)

window1:Label("AutoFarm Functions:", Color3.fromRGB(127, 143, 166))
