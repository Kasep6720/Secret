local player = game.Players.LocalPlayer
local VirtualUser=game:service'VirtualUser'
	game:service'Players'.LocalPlayer.Idled:connect(function()
	warn("anti-afk")
	VirtualUser:CaptureController()
	VirtualUser:ClickButton2(Vector2.new())
end)

local player = game.Players.LocalPlayer
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local TextButton_X = Instance.new("TextButton")
local TextButton = Instance.new("TextButton")
local TextButton_2 = Instance.new("TextButton")
local TextButton_3 = Instance.new("TextButton")
local TextButton_4 = Instance.new("TextButton")

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(117+50, 50+50, 168+50)
Frame.Position = UDim2.new(0, 0, 0.70, 0)
Frame.Size = UDim2.new(0, 200, 0, 185)
Frame.BackgroundTransparency = 0.98

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
TextLabel.Size = UDim2.new(0, 200, 0, 30)
TextLabel.Font = Enum.Font.AmaticSC
TextLabel.Text = "Loadout Changer         "
TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.TextScaled = true
TextLabel.TextSize = 40.000
TextLabel.TextWrapped = true

TextButton_X.Parent = Frame
TextButton_X.BackgroundColor3 = Color3.fromRGB(225, 45, 45)
TextButton_X.Position = UDim2.new(0.85, 0, 0.000000024, 0)
TextButton_X.Size = UDim2.new(0, 30, 0, 30)
TextButton_X.Font = Enum.Font.AmaticSC
TextButton_X.Text = "x"
TextButton_X.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton_X.TextScaled = true
TextButton_X.TextSize = 40.000
TextButton_X.TextWrapped = true

function X()
    for i,v in pairs(player.PlayerGui:GetChildren()) do
        if v.Name == "ScreenGui" then
            v:Destroy()
        end
    end
end
TextButton_X.MouseButton1Click:connect(X)

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(117, 50, 168)
TextButton.Position = UDim2.new(0., 0, 0.1650000006, 0)
TextButton.Size = UDim2.new(0, 200, 0, 25)
TextButton.Font = Enum.Font.AmaticSC
TextButton.Text = "Spam"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextScaled = true
TextButton.TextSize = 100.000
TextButton.TextWrapped = true

