TextButton_4.TextScaled = true
TextButton_4.TextSize = 40.000
TextButton_4.TextWrapped = true

function idk()
    game:GetService("StarterGui"):SetCore("SendNotification",{Title = "Loadout Changer",Text = ":)",Duration = 0.000000001, })
end
TextButton_4.MouseButton1Click:connect(idk)
