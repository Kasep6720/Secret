TextButton_4.BackgroundColor3 = Color3.fromRGB(117-50, 50-50, 168-50)
TextButton_4.Position = UDim2.new(0, 0, 0.660000024, 0)
TextButton_4.Size = UDim2.new(0, 200, 0, 65)
TextButton_4.Font = Enum.Font.AmaticSC
TextButton_4.Text = "..."
