end
TextButton_4.MouseButton1Click:connect(LAG)

local TextButton4Dot = Instance.new("TextButton")
TextButton4Dot.Parent = Frame
TextButton4Dot.BackgroundColor3 = Color3.fromRGB(117-20, 50-20, 168-20)
TextButton4Dot.Position = UDim2.new(0.85, 0, 0.5840000006, 0)
TextButton4Dot.Size = UDim2.new(0, 30, 0, 25)
TextButton4Dot.Font = Enum.Font.AmaticSC
TextButton4Dot.Text = "+"
TextButton4Dot.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton4Dot.TextScaled = false
TextButton4Dot.TextSize = 40.000
TextButton4Dot.TextWrapped = true

function LagLoadoutDot()
    local ScreenGui1 = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local TextLabel = Instance.new("TextLabel")

    ScreenGui1.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

    Frame.Parent = ScreenGui1
    Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Frame.Position = UDim2.new(0.16, 0, 0.70, 0)
    Frame.Size = UDim2.new(0, 200, 0, 185)

    TextLabel.Parent = Frame
    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
    TextLabel.Size = UDim2.new(0, 200, 0, 185)
    TextLabel.Font = Enum.Font.Ubuntu
    TextLabel.Text = [[Abilities Required:

Kenjutsu: 
Sun

Bloodlines: 
Borumaki, 
Satori Rengoku, 
Forged Rengoku, 
Sengoku, 
Alphirama Shizen, 
Eastwood Korashi, 
Menza, 
Typhoon

Other Abilities: 
Poison Style: Acidic Cloud, 
Earth Style: Winter Avalanche]]
    TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
    TextLabel.TextScaled = true
    TextLabel.TextSize = 40.000
    TextLabel.TextWrapped = true

    wait(5)

    ScreenGui1:Destroy()

end
TextButton4Dot.MouseButton1Click:connect(LagLoadoutDot)
