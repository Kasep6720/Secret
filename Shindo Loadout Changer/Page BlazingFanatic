--Properties:
local ScreenGui = Instance.new("ScreenGui")
--LoadoutChanger.Name = "LoadoutChanger"
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local MainBL = Instance.new("ImageLabel")
MainBL.Name = "MainBL"
MainBL.Parent = ScreenGui
MainBL.BackgroundColor3 = Color3.new(1, 1, 1)
MainBL.BackgroundTransparency = 1
MainBL.Position = UDim2.new(0.765, 0, 0.52, 0)
MainBL.Size = UDim2.new(0, 300, 0, 280)
MainBL.Visible = true
MainBL.Image = "rbxassetid://2260429633"
MainBL.ImageColor3 = Color3.new(0.117647, 0.117647, 0.117647)
MainBL.ScaleType = Enum.ScaleType.Slice
MainBL.SliceCenter = Rect.new(12, 12, 13, 13)

local X = Instance.new("TextButton")
X.Name = "X"
X.Parent = MainBL
X.BackgroundColor3 = Color3.new(0.0588235, 0.0588235, 0.0588235)
X.BorderSizePixel = 0
X.Position = UDim2.new(0.85, 0, 0, 0)
