function SpamLoadoutDot()
	local ScreenGui1 = Instance.new("ScreenGui")
	local Main = Instance.new("ImageLabel")
	local Frame = Instance.new("Frame")
	local TextLabel = Instance.new("TextLabel")
	local TextLabel1 = Instance.new("TextLabel")

	ScreenGui1.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

	Main.Name = "Main"
	Main.Parent = ScreenGui1
	Main.BackgroundColor3 = Color3.new(1, 1, 1)
	Main.BackgroundTransparency = 1
	Main.Position = UDim2.new(0.765, 0, 0.14, 0)
	Main.Size = UDim2.new(0, 300, 0, 230)
	Main.Visible = true
	Main.Image = "rbxassetid://2260429633"
	Main.ImageColor3 = Color3.new(0.117647, 0.117647, 0.117647)
	Main.ScaleType = Enum.ScaleType.Slice
	Main.SliceCenter = Rect.new(12, 12, 13, 13)

	TextLabel.Parent = Main
	TextLabel.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
	TextLabel.Size = UDim2.new(0, 300, 0, 230)
	TextLabel.Font = Enum.Font.Ubuntu
	TextLabel.BackgroundTransparency = 1
	TextLabel.Text = [[

Elements: 
Shiver, 
Acid, 
Chaos

Bloodlines: 
Bankai Akuma, 
Shindai Rengoku, 
Alphirama Shizen, 
Sengoku

Other Abilities: 
Snake Summon, 
Shindai Style: Sunflower Illusion, 
Earth Style: Winter Avalanche
]]
	TextLabel.TextColor3 = Color3.fromRGB(225, 225, 225)
	TextLabel.TextScaled = true
	TextLabel.TextSize = 40.000
	TextLabel.TextWrapped = true

	Frame.Parent = ScreenGui1
	Frame.BackgroundColor3 = Color3.fromRGB(255,163,26)
	Frame.Position = UDim2.new(0.765, 0, 0.11, 0)
	Frame.Size = UDim2.new(0, 300, 0, 30)
	Frame.Transparency = 0.1

	TextLabel1.Parent = Frame
	TextLabel1.BackgroundColor3 = Color3.fromRGB(255,163,26)
	TextLabel1.Size = UDim2.new(0, 300, 0, 30)
	TextLabel1.Font = Enum.Font.Ubuntu
	TextLabel1.BackgroundTransparency = 1
	TextLabel1.Text = [[Abilities Required]]
	TextLabel1.TextColor3 = Color3.fromRGB(1, 1, 1)
	TextLabel1.TextScaled = true
	TextLabel1.TextSize = 40.000
	TextLabel1.TextWrapped = true

	wait(4)

	ScreenGui1:Destroy()
end
SpamDesc.MouseButton1Click:connect(SpamLoadoutDot)

