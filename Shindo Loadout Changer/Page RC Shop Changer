clothslot1.Text = "Slot 1"
clothslot1.TextColor3 = Color3.new(1, 1, 1)
clothslot1.TextSize = 15
clothslot1.MouseButton1Click:connect(function()
	clothslot1.BackgroundColor3 = Color3.new(0.3, 0.8, 0.3)
	game:GetService("Workspace")["4hourshop"].item1.Value = Clothslotstextbox.Text
	wait(.5)
	clothslot1.BackgroundColor3 = Color3.new(0.2333333, 0.49, 0)
end)

