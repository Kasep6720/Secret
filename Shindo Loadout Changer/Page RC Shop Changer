local ShowRCUIButton = Instance.new("TextButton")
ShowRCUIButton.Name = "ShowRCUIButton"
ShowRCUIButton.Parent = ShowRCUIL
ShowRCUIButton.BackgroundColor3 = Color3.new(0.0745098, 0.0745098, 0.0745098)
ShowRCUIButton.BackgroundTransparency = 1
ShowRCUIButton.BorderSizePixel = 0
ShowRCUIButton.Position = UDim2.new(0.25, 0, 0, 0)
ShowRCUIButton.Size = UDim2.new(0, 60, 0, 20)
ShowRCUIButton.Font = Enum.Font.GothamBlack
ShowRCUIButton.Text = "Show RC Shop"
ShowRCUIButton.TextColor3 = Color3.new(1, 1, 1)
ShowRCUIButton.TextSize = 15
ShowRCUIButton.TextWrapped = false
ShowRCUIButton.TextStrokeColor3 = Color3.new(0, 0, 0)
ShowRCUIButton.ZIndex = 3
ShowRCUIButton.MouseButton1Click:connect(function()
	if game:GetService("Players").LocalPlayer.PlayerGui.Main.RYOshop.Visible == true then
		game:GetService("Players").LocalPlayer.PlayerGui.Main.RYOshop.Visible = false
		game:GetService("Players").LocalPlayer.PlayerGui.Main.Blur.Visible = true
		game:GetService("Players").LocalPlayer.PlayerGui.Main.ingame.Menu.Visible = true
	else
		game:GetService("Players").LocalPlayer.PlayerGui.Main.RYOshop.Visible = true
		game:GetService("Players").LocalPlayer.PlayerGui.Main.Blur.Visible = false
		game:GetService("Players").LocalPlayer.PlayerGui.Main.ingame.Menu.Visible = false
	end
end)

