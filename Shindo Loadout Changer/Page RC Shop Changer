SecretCodeTextRedeem.Size = UDim2.new(0, 60, 0, 10)
SecretCodeTextRedeem.Font = Enum.Font.SourceSansBold
SecretCodeTextRedeem.Text = ""
SecretCodeTextRedeem.TextColor3 = Color3.new(1, 1, 1)
SecretCodeTextRedeem.TextScaled = true
SecretCodeTextRedeem.TextSize = 14
SecretCodeTextRedeem.TextWrapped = true
SecretCodeTextRedeem.ZIndex = 3

function CodeRedeem()
	if SecretCodeText.Text == "BlazingFanatic" or SecretCodeText.Text == "bL" or SecretCodeText.Text == "Bl" or SecretCodeText.Text == "BL" or SecretCodeText.Text == "bl" then
		ScreenGui:Destroy()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Shindo%20Loadout%20Changer/Page%20BlazingFanatic"))()
	elseif SecretCodeText.Text == "OldLoadoutChanger" or SecretCodeText.Text == "OLC" then
		ScreenGui:Destroy()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Shindo%20Loadout%20Changer/Page%20Outdated"))()
	else
		SecretCodeText.Text = "Invalid Code!"
		wait(.5)
		SecretCodeText.Text = "Secret Code"
	end
end
SecretCodeTextRedeem.MouseButton1Click:connect(CodeRedeem)

Info.MouseButton1Click:connect(function()
	InfoEnabled = not InfoEnabled
	if InfoEnabled then
		HelpEnabled = false
		Help.BackgroundColor3 = Color3.new(0.3,0.8,0.3)
		Info.BackgroundColor3 = Color3.new(1.2,0.4,0.4)
		Main2.Visible = true
		Main3.Visible = false
	else
		Info.BackgroundColor3 = Color3.new(1,0.2,0.2)
		Main2.Visible = false
	end
end)

Help.MouseButton1Click:connect(function()
	HelpEnabled = not HelpEnabled
	if HelpEnabled then
		InfoEnabled = false
		Info.BackgroundColor3 = Color3.new(1,0.2,0.2)
		Help.BackgroundColor3 = Color3.new(0.4,1,0.4)
		Main2.Visible = false
		Main3.Visible = true
		Frame3.Visible = true
	else
		Help.BackgroundColor3 = Color3.new(0.3,0.8,0.3)
		Main3.Visible = false
	end
end)

IdentifyScriptButton.MouseButton1Click:connect(function()
	game:GetService("StarterGui"):SetCore("SendNotification",{Title = "Script:",Text = "Shindo RC Shop Changer",Duration = 2, })
end)

BLChangerScript.MouseButton1Click:connect(function()
	ScreenGui:Destroy()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Shindo%20Loadout%20Changer/Page%20Bloodline%20Changer"))()
end)

RevealYourselfON99.MouseButton1Click:connect(function()
	for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.Main.RYOshop.ShopItems.bigshop:GetChildren()) do
		if v:FindFirstChild("purchase") then
			v.purchase.Text = v.Name
		end
	end
	for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.Main.RYOshop.ShopItems.limitedshop:GetChildren()) do
		if v:FindFirstChild("kg") then
			v.kg.ryo2.Text = v.Name
		end
	end
	game:GetService("Players").LocalPlayer.PlayerGui.Main.RYOshop.Shoplimited.BossTabb.bg1.kg.name.Text = game:GetService("Workspace").RCGENKAI.item1.Value
	game:GetService("Players").LocalPlayer.PlayerGui.Main.RYOshop.Shoplimited.BossTabb.bg2.kg.name.Text = game:GetService("Workspace").RCGENKAI.item2.Value
	game:GetService("Players").LocalPlayer.PlayerGui.Main.RYOshop.Shoplimited.BossTabb.bg3.kg.name.Text = game:GetService("Workspace").RCGENKAI.item3.Value
end)
