s.Name = "Sound" 
s.SoundId = "http://www.roblox.com/asset/?id=4727752918" 
s.Volume = 99999999999999
s.Looped = false
s.archivable = false 
s.Parent = game.Workspace wait(.0000000000000000000000000000001) 
