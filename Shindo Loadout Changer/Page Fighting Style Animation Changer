local SecretCodeTextRedeem = Instance.new("TextButton")
SecretCodeTextRedeem.Name = "SecretCodeTextRedeem"
SecretCodeTextRedeem.Parent = FSChangerMain
SecretCodeTextRedeem.BackgroundColor3 = Color3.new(0.2333333, 0.49, 0)
SecretCodeTextRedeem.BorderColor3 = Color3.new(0,0,0)
SecretCodeTextRedeem.BorderSizePixel = 2
SecretCodeTextRedeem.Position = UDim2.new(0.115, 0, 0.87, 0)
SecretCodeTextRedeem.Size = UDim2.new(0, 60, 0, 10)
SecretCodeTextRedeem.Font = Enum.Font.SourceSansBold
SecretCodeTextRedeem.Text = ""
SecretCodeTextRedeem.TextColor3 = Color3.new(1, 1, 1)
SecretCodeTextRedeem.TextScaled = true
SecretCodeTextRedeem.TextSize = 14
SecretCodeTextRedeem.TextWrapped = true
SecretCodeTextRedeem.ZIndex = 3

function CodeRedeem()
	if SecretCodeText.Text == "BlazingFanatic" or SecretCodeText.Text == "bL" or SecretCodeText.Text == "Bl" or SecretCodeText.Text == "BL" or SecretCodeText.Text == "bl" then
		ScreenGui:Destroy()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Shindo%20Loadout%20Changer/Page%20BlazingFanatic"))()
	elseif SecretCodeText.Text == "OldLoadoutChanger" or SecretCodeText.Text == "OLC" then
		ScreenGui:Destroy()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Shindo%20Loadout%20Changer/Page%20Outdated"))()
	elseif SecretCodeText.Text == "ShindoFightingStyleAnimationChanger" or SecretCodeText.Text == "ShindoMartialArtAnimationChanger" or SecretCodeText.Text == "SFSAC" or SecretCodeText.Text == "SMAAC" or SecretCodeText.Text == "ORA" then
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = ":O",Text = "Shindo Fighting Style Animation Changer",Duration = 0.0001, })
		local s = Instance.new("Sound")
		s.Name = "Sound" 
		s.SoundId = "http://www.roblox.com/asset/?id=8701825353" 
		s.Volume = 99999999999999
		s.Looped = false
		s.archivable = false 
		s.Parent = game.Workspace wait(.0000000000000000000000000000001) 
		s:play()
		wait(0.8)
		local s = Instance.new("Sound")
		s.Name = "Sound" 
		s.SoundId = "http://www.roblox.com/asset/?id=6968100828" 
		s.Volume = 99999999999999
		s.Looped = false
		s.archivable = false 
		s.Parent = game.Workspace wait(.0000000000000000000000000000001) 
		s:play()
		wait(.15)
		local s = Instance.new("Sound")
		s.Name = "Sound" 
		s.SoundId = "http://www.roblox.com/asset/?id=6968100828" 
		s.Volume = 99999999999999
		s.Looped = false
		s.archivable = false 
		s.Parent = game.Workspace wait(.0000000000000000000000000000001) 
		s:play()
		wait(.15)
		local s = Instance.new("Sound")
		s.Name = "Sound" 
		s.SoundId = "http://www.roblox.com/asset/?id=6968100828" 
		s.Volume = 99999999999999
		s.Looped = false
		s.archivable = false 
		s.Parent = game.Workspace wait(.0000000000000000000000000000001) 
		s:play()
		wait(.15)
		local s = Instance.new("Sound")
		s.Name = "Sound" 
		s.SoundId = "http://www.roblox.com/asset/?id=6968100828" 
		s.Volume = 99999999999999
		s.Looped = false
		s.archivable = false 
		s.Parent = game.Workspace wait(.0000000000000000000000000000001) 
		s:play()
		wait(.15)
		local s = Instance.new("Sound")
		s.Name = "Sound" 
		s.SoundId = "http://www.roblox.com/asset/?id=6968100828" 
		s.Volume = 99999999999999
		s.Looped = false
		s.archivable = false 
		s.Parent = game.Workspace wait(.0000000000000000000000000000001) 
		s:play()
	else
		SecretCodeText.Text = "Invalid Code!"
		wait(.5)
		SecretCodeText.Text = "Secret Code"
	end
end
SecretCodeTextRedeem.MouseButton1Click:connect(CodeRedeem)

Info.MouseButton1Click:connect(function()
	InfoEnabled = not InfoEnabled
	if InfoEnabled then
		HelpEnabled = false
		Help.BackgroundColor3 = Color3.new(1.020,0.652,0.104)
		Info.BackgroundColor3 = Color3.new(0.5,0.5,1.5)
		Main2.Visible = true
		Main3.Visible = false
	else
		Info.BackgroundColor3 = Color3.new(0.3,0.3,1.2)
		Main2.Visible = false
	end
end)

Help.MouseButton1Click:connect(function()
	HelpEnabled = not HelpEnabled
	if HelpEnabled then
		InfoEnabled = false
		Info.BackgroundColor3 = Color3.new(0.3,0.3,1.2)
		Help.BackgroundColor3 = Color3.new(1.2,0.8,0.3)
		Main2.Visible = false
		Main3.Visible = true
		TextLabel2.TextSize = 15	
	else
		Help.BackgroundColor3 = Color3.new(1.020,0.652,0.104)
		Main3.Visible = false
	end
end)

IdentifyScriptButton.MouseButton1Click:connect(function()
	game:GetService("StarterGui"):SetCore("SendNotification",{Title = "Script:",Text = "Shindo Fighting Style Animation Changer",Duration = 2, })
end)

BLChangerScript.MouseButton1Click:connect(function()
	ScreenGui:Destroy()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Shindo%20Loadout%20Changer/Page%20Bloodline%20Changer"))()
end)

RCChangerScript.MouseButton1Click:connect(function()
	ScreenGui:Destroy()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Shindo%20Loadout%20Changer/Page%20RC%20Shop%20Changer"))()	
end)

RevealYourselfON9ZAI.MouseButton1Click:connect(function()
	game:GetService("Players").LocalPlayer.PlayerGui.Main.ingame.Menu.CombatTab.Element1.amt.Text = "Please enter these names"
	for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.Main.ingame.Menu.CombatTab.Element1:GetChildren()) do
		if v:FindFirstChild("level") then
			v.level.title.Text = v.Name
		end
	end
end)
