local ChangeAnimationbutton = Instance.new("TextButton")
ChangeAnimationbutton.Name = "ChangeAnimationbutton"
ChangeAnimationbutton.Parent = FSChangerMain
ChangeAnimationbutton.BackgroundColor3 = Color3.new(0.2333333, 0.49, 0)
ChangeAnimationbutton.BorderColor3 = Color3.new(0,0,0)
ChangeAnimationbutton.BorderSizePixel = 2
ChangeAnimationbutton.Position = UDim2.new(0.34, 0, 0.425, 0)
ChangeAnimationbutton.Font = Enum.Font.SourceSansBold
ChangeAnimationbutton.Size = UDim2.new(0, 180, 0, 15)
ChangeAnimationbutton.Text = "Change Animation"
ChangeAnimationbutton.TextColor3 = Color3.new(1, 1, 1)
ChangeAnimationbutton.TextSize = 15
ChangeAnimationbutton.MouseButton1Click:connect(function()
	ChangeAnimationbutton.BackgroundColor3 = Color3.new(0.3, 0.8, 0.3)
	game:GetService("Players").LocalPlayer.statz.fightingstyle.Value = ChangeAnimationtextbox.Text
	wait(.5)
	ChangeAnimationbutton.BackgroundColor3 = Color3.new(0.2333333, 0.49, 0)
end)

