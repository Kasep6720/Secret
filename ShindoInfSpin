	AutoF9 = bool
	end)

    local kgs = {}
    for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
        if v:FindFirstChild("KG") then
            table.insert(kgs, v.Name)
        end
    end
	window2:Label("Choose your BL and press SPIN", Color3.fromRGB(127, 143, 166))

    local a1
    window2:Dropdown("Bloodline Choice 1",kgs,function(KG1)
        print("Selected: " .. KG1)
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "1st Bloodline Selected: " .. KG1 ,Duration = 5, })
    	a1 = KG1
    end)
    local a2
    window2:Dropdown("Bloodline Choice 2",kgs,function(KG2)
        print("Selected: " .. KG2)		
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "2nd Bloodline Selected: " .. KG2 ,Duration = 5, })
