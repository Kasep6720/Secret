    window2:Dropdown("WHAT DO YOU WANT",kgs,function(KG2)
        print("Selected: " .. KG2)		
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "Selected: " .. KG2 ,Duration = 5, })
    	a2 = KG2
