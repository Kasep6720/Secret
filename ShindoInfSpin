	window2:Toggle("Spin Slot 1",false,function(bool)
		AutoSpinBL1 = bool
		if AutoSpinBL1 == true then
			if AutoF9 == true then
				game:GetService("StarterGui"):SetCore("DevConsoleVisible", true)
			end
			game.Players.LocalPlayer.statz.main.kg1.ChildAdded:Connect(function(yes)
				if yes.Name == "dontspin" then
					wait(.1)
					yes:Destroy()
				end
			end)
			local spins = game.Players.LocalPlayer.statz.spins.Value
			local des = game.Players.LocalPlayer.statz.spins
			spawn(function()
				for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
					if v:FindFirstChild("KG") then
						local a = Instance.new("StringValue")
						a.Name = v.Name
						a.Parent = game.Players.LocalPlayer.statz.genkailevel
					end
				end
			end)
			spawn(function()
				while wait() do
					if spins > 0 then
						spins = game.Players.LocalPlayer.statz.spins.Value
						kgvalue1 = game.Players.LocalPlayer.statz.main.kg1.Value
						if AutoSpinBL1 == true then
							if kgvalue1 ~= a1 and kgvalue1 ~= a2 and kgvalue1 ~= a3 and kgvalue1 ~= a4 and kgvalue1 ~= a5 then
								kgvalue1 = game.Players.LocalPlayer.statz.main.kg1.Value
								game.Players.LocalPlayer.startevent:FireServer("spin", "kg1")
								print("Rolled: " .. kgvalue1)
								wait(1)
								kgvalue1 = game.Players.LocalPlayer.statz.main.kg1.Value
							else
								print("You have got: " .. kgvalue1)
								game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "You have got: " .. kgvalue1,Duration = 0.00001, })
								if Autosave == true then
									game:GetService("Players").LocalPlayer.startevent:FireServer("band", "Eye")
								end
							end
						end
					else
						if Autorejoin == true then
							--player.statz.spins:Destroy()
							game:GetService('TeleportService'):Teleport(game.PlaceId, player)
						end
					end
				end
			end)
		else
		end
	end)

