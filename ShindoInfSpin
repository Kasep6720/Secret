	end)

	if game.Players.LocalPlayer.gamepasses:FindFirstChild("element3") then
		window3:Toggle("Spin Slot 3",false,function(bool)
			StartSpinElm3 = bool
			if StartSpinElm3 == true then
				if AutoF9 == true then
					game:GetService("StarterGui"):SetCore("DevConsoleVisible", true)
				end
				game.Players.LocalPlayer.statz.main.element3.ChildAdded:Connect(function(yes)
					if yes.Name == "dontspin" then
						wait(.1)
						yes:Destroy()
					end
				end)
				local spins = game.Players.LocalPlayer.statz.spins.Value
				local des = game.Players.LocalPlayer.statz.spins
				spawn(function()
					for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
						if v:FindFirstChild("KG") then
							local a = Instance.new("StringValue")
							a.Name = v.Name
							a.Parent = game.Players.LocalPlayer.statz.genkailevel
						end
					end
				end)
				spawn(function()
					while wait() do
						if spins > 0 then
							spins = game.Players.LocalPlayer.statz.spins.Value
							elmvalue3 = game.Players.LocalPlayer.statz.main.element3.Value
							if StartSpinElm3 == true then
								if elmvalue3 ~= a1 and elmvalue3 ~= a2 and elmvalue3 ~= a3 and elmvalue3 ~= a4 and elmvalue3 ~= a5 then
									elmvalue3 = game.Players.LocalPlayer.statz.main.element3.Value
									game.Players.LocalPlayer.startevent:FireServer("spin", "element3")
									print("Rolled: " .. elmvalue3)
									wait(1)
									elmvalue3 = game.Players.LocalPlayer.statz.main.element3.Value
								else
									print("You have got: " .. elmvalue3)
									game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "You have got: " .. elmvalue3,Duration = 0.00001, })
									if Autosave == true then
										game:GetService("Players").LocalPlayer.startevent:FireServer("band", "Eye")
									end
								end
							end
						else
							if Autorejoin == true then
								--player.statz.spins:Destroy()
								game:GetService('TeleportService'):Teleport(game.PlaceId, player)
							end
						end
					end
				end)
			else 
			end
		end)
	end

	if game.Players.LocalPlayer.gamepasses:FindFirstChild("element4") then
		window3:Toggle("Spin Slot 4",false,function(bool)
			StartSpinElm4 = bool
			if StartSpinElm4 == true then
				if AutoF9 == true then
					game:GetService("StarterGui"):SetCore("DevConsoleVisible", true)
				end
				game.Players.LocalPlayer.statz.main.element4.ChildAdded:Connect(function(yes)
					if yes.Name == "dontspin" then
						wait(.1)
						yes:Destroy()
					end
				end)
				local spins = game.Players.LocalPlayer.statz.spins.Value
				local des = game.Players.LocalPlayer.statz.spins
				spawn(function()
					for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
						if v:FindFirstChild("KG") then
							local a = Instance.new("StringValue")
							a.Name = v.Name
							a.Parent = game.Players.LocalPlayer.statz.genkailevel
						end
					end
				end)
				spawn(function()
					while wait() do
						if spins > 0 then
							spins = game.Players.LocalPlayer.statz.spins.Value
							elmvalue4 = game.Players.LocalPlayer.statz.main.element4.Value
							if StartSpinElm4 == true then
								if elmvalue4 ~= a1 and elmvalue4 ~= a2 and elmvalue4 ~= a3 and elmvalue4 ~= a4 and elmvalue4 ~= a5 then
									elmvalue4 = game.Players.LocalPlayer.statz.main.element4.Value
									game.Players.LocalPlayer.startevent:FireServer("spin", "element4")
									print("Rolled: " .. elmvalue4)
									wait(1)
									elmvalue4 = game.Players.LocalPlayer.statz.main.element4.Value
								else
									print("You have got: " .. elmvalue4)
									game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "You have got: " .. elmvalue4,Duration = 0.00001, })
									if Autosave == true then
										game:GetService("Players").LocalPlayer.startevent:FireServer("band", "Eye")
									end
								end
							end
						else
							if Autorejoin == true then
								--player.statz.spins:Destroy()
								game:GetService('TeleportService'):Teleport(game.PlaceId, player)
							end
						end
					end
				end)
			else 
			end
		end)
	end

	window4:Label("Choose your Kenjutsu and press SPIN", Color3.fromRGB(127, 143, 166))

    local ken = {}
    for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu.ALLKENSTYLES:GetChildren()) do
        if v:FindFirstChild("BREATHINGSTYLE") then
            table.insert(ken, v.Name)
        end
    end

	local kenselect1
		window4:Dropdown("Kenjutsu Choice 1",ken,function(KEN1)
		print("Selected: " .. KEN1)
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "1st Kenjutsu Selected: " .. KEN1 ,Duration = 5, })
		kenselect1 = KEN1
	end)
	local kenselect2
		window4:Dropdown("Kenjutsu Choice 2",ken,function(KEN2)
		print("Selected: " .. KEN2)
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "2nd Kenjutsu Selected: " .. KEN2 ,Duration = 5, })
		kenselect2 = KEN2
	end)
	local kenselect3
		window4:Dropdown("Kenjutsu Choice 3",ken,function(KEN3)
		print("Selected: " .. KEN3)
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "3rd Kenjutsu Selected: " .. KEN3 ,Duration = 5, })
		kenselect2 = KEN3
	end)

	window4:Toggle("Spin Kenjutsu",false,function(bool)
		StartSpinKen = bool
		if StartSpinKen == true then
			if AutoF9 == true then
				game:GetService("StarterGui"):SetCore("DevConsoleVisible", true)
			end
			game.Players.LocalPlayer.statz.breathingstyle.ChildAdded:Connect(function(yes)
				if yes.Name == "dontspin" then
					wait(.1)
					yes:Destroy()
				end
			end)
			local spins = game.Players.LocalPlayer.statz.spins.Value
			local des = game.Players.LocalPlayer.statz.spins
			spawn(function()
				for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
					if v:FindFirstChild("KG") then
						local a = Instance.new("StringValue")
						a.Name = v.Name
						a.Parent = game.Players.LocalPlayer.statz.genkailevel
					end
				end
			end)
			spawn(function()
				while wait() do
					if spins > 0 then
						spins = game.Players.LocalPlayer.statz.spins.Value
						kenvalue = game.Players.LocalPlayer.statz.breathingstyle.Value
						if StartSpinKen == true then
							if kenvalue ~= kenselect1 and kenvalue ~= kenselect2 and kenvalue ~= kenselect3 then
								kenvalue = game.Players.LocalPlayer.statz.breathingstyle.Value
								local args = {[1] = "spinBREATHING"}
								game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
								print("Rolled: " .. kenvalue)
								game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "Rolled: " .. kenvalue,Duration = 0.00001, })
								wait(1)
								kenvalue = game.Players.LocalPlayer.statz.breathingstyle.Value
							else
								print("You have got: " .. kenvalue)
								game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "You have got: " .. kenvalue,Duration = 0.00001, })
								if Autosave == true then
									game:GetService("Players").LocalPlayer.startevent:FireServer("band", "Eye")
								end
							end
						end
					else
						if Autorejoin == true then
							--player.statz.spins:Destroy()
							game:GetService('TeleportService'):Teleport(game.PlaceId, player)
						end
					end
				end
			end)
		else 
		end
	end)

	window5:Label("Made by NoobieDoge", Color3.fromRGB(127, 143, 166))
	window5:Label("https://discord.gg/P2CeRjvTDt", Color3.fromRGB(127, 143, 166))
	window5:Button("Copy Discord Link",function()
		setclipboard("https://discord.gg/P2CeRjvTDt")
		wait()
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "Discord link copied!",Duration = 1, })
	end)

Autosave = true
Autorejoin = true
AutoF9 = true
game:GetService("Players").LocalPlayer.startevent:FireServer("band", "\128")
spawn(function()
	while wait() do
		if datasavedandyoucanredeemcodes == true then
			
		else
			game.Players.LocalPlayer.PlayerGui.Main.Customization.Codes.Text = [[WARNING:
			DO NOT REDEEM CODES BEFORE SAVING DATA]]
		end
	end
end)
