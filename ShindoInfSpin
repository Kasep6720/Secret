    window2:Dropdown("BLOODLINE SLOT",{"kg1", "kg2", "kg3", "kg4"},function(kgS)
        b = kgS
    	kgslot = game.Players.LocalPlayer.statz.main:FindFirstChild(b)
    	kgvalue = kgslot.Value
    	print(kgslot)
    	print(kgvalue)
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "Selected: ".. kgS ,Duration = 7, })
    end)
    local a1
    window2:Dropdown("WHAT DO YOU WANT",kgs,function(KG1)
        print("Selected: " .. KG1)
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "DogeHub",Text = "Selected: " .. KG1 ,Duration = 5, })
    	a1 = KG1
    end)
