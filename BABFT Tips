--tips
local player = game.Players.LocalPlayer
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local TextLabel2 = Instance.new("TextLabel")
local TextLabel3 = Instance.new("TextLabel")
local TextButton = Instance.new("TextButton")

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.Position = UDim2.new(0.60, 0, 0.20, 0)
Frame.Size = UDim2.new(0, 330, 0, 300)

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(217, 112, 217)
TextLabel.Size = UDim2.new(0, 330, 0, 60)
TextLabel.Font = Enum.Font.AmaticSC
TextLabel.Text = "tips:"
TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.TextScaled = true
TextLabel.TextSize = 35.000
TextLabel.TextWrapped = true

TextLabel2.Parent = Frame
TextLabel2.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
TextLabel2.Position = UDim2.new(0, 0, 0.200000006, 0)
TextLabel2.Size = UDim2.new(0, 330, 0, 120)
TextLabel2.Font = Enum.Font.AmaticSC
TextLabel2.Text = "Do not reset or turn on other scripts while autofarm is enabled!"
TextLabel2.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel2.TextScaled = true
TextLabel2.TextSize = 35.000
TextLabel2.TextWrapped = true

TextLabel3.Parent = Frame
TextLabel3.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
TextLabel3.Position = UDim2.new(0, 0, 0.600000006, 0)
TextLabel3.Size = UDim2.new(0, 330, 0, 120)
TextLabel3.Font = Enum.Font.AmaticSC
TextLabel3.Text = "if you are stuck in the sky then please disable and enable autofarm again"
TextLabel3.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel3.TextScaled = true
TextLabel3.TextSize = 35.000
TextLabel3.TextWrapped = true

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(230, 30, 30)
TextButton.Position = UDim2.new(0.91, 0, 0.00000006, 0)
TextButton.Size = UDim2.new(0, 30, 0, 30)
TextButton.Font = Enum.Font.AmaticSC
TextButton.Text = "-"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextScaled = false
TextButton.TextSize = 60.000
TextButton.TextWrapped = true

function closetips()
    for i,v in pairs(player.PlayerGui:GetChildren()) do
        if v.Name == "ScreenGui" then
            v:Destroy()
        end
    end  
     local player = game.Players.LocalPlayer
    local ScreenGui = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local opentipsbutton = Instance.new("TextButton")

    ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

    Frame.Parent = ScreenGui
    Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Frame.Position = UDim2.new(0.960, 0, 0.60, 0)
    Frame.Size = UDim2.new(0, 50, 0, 30)

    opentipsbutton.Parent = Frame
    opentipsbutton.BackgroundColor3 = Color3.fromRGB(230, 30, 230)
    opentipsbutton.Position = UDim2.new(0, 0, 0, 0)
    opentipsbutton.Size = UDim2.new(0, 50, 0, 30)
    opentipsbutton.Font = Enum.Font.AmaticSC
    opentipsbutton.Text = "+"
    opentipsbutton.TextColor3 = Color3.fromRGB(0, 0, 0)
    opentipsbutton.TextScaled = false
    opentipsbutton.TextSize = 80.000
    opentipsbutton.TextWrapped = true

    function opentips()
        for i,v in pairs(player.PlayerGui:GetChildren()) do
            if v.Name == "ScreenGui" then
                v:Destroy()
            end
        end 
        loadstring(game:HttpGet("https://raw.githubusercontent.com/NoobieDoge/DogeHub/main/DogeHub"))()
    end
    TextButton.MouseButton1Click:connect(opentips)
end
TextButton.MouseButton1Click:connect(closetips)
