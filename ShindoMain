	for i,v in pairs(player.PlayerGui:GetChildren()) do
		if v.Name == "ScreenGui" then
			v:Destroy()
		end
	end
	NoneOA = true
	AutofarmPressed = true
end)

TextButton_9.MouseButton1Click:connect(function()
	Frame2.Visible = true
	Frame3.Visible = false
end)

game:GetService('RunService').Stepped:connect(function()
	if AutofarmPressed == true then
		AutofarmPressed = false
		if BeastNRods or BeastOnly or NoneOA == true then
			repeat wait() until game:IsLoaded()
			--Loading GUI
			local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/pornhub%20script%20gui"))()
			local window1 = library:Window("Auto Farm - DogeHub")
			local window2 = library:Window("Others")
			local window3 = library:Window("Settings")

			library:Keybind("RightControl")

			window1:Label("NIGHT = NO QUESTS", Color3.fromRGB(127, 143, 166))

			local autofarm
			window1:Toggle("Auto Farm",false, function(bool)
				autofarm = bool
				if BeastOnly or NoneOA then
					local platform = Instance.new("Part", player.Character)
					platform.Anchored = true
					platform.Transparency = 1
					platform.Size = Vector3.new(6, 1, 6)
			
					local connection
					connection = game:GetService("RunService").RenderStepped:connect(function()
						if player.Character:FindFirstChild("HumanoidRootPart") then
							platform.Position = player.Character.HumanoidRootPart.CFrame * Vector3.new(0, -1.8, 0)
						else
							connection:Disconnect()
						end
					end)
					game:GetService('RunService').Stepped:connect(function()
						if autofarm ~= true then
							platform:Destroy()
						end
					end)
				end
			end)

			local boss
			window1:Toggle("Boss Farm",false, function(bool)
				boss = bool
			end)

			local RANKUPP
			window1:Toggle("Auto Rank",false, function(bool)
				RANKUPP = bool
			end)

			local scrollfarm
			window1:Toggle("Scroll Snipe",false, function(bool)
				scrollfarm = bool
			end)

			if BeastNRods or BeastOnly then
				window1:Button("Infinite MD",function()
					INFMDON()
				end)
			end

			window1:Label("Boss farm settings", Color3.fromRGB(127, 143, 166))

			local bosslist = {}
			if game.PlaceId == 7923764447 then
				for i,v in pairs(workspace.sengokubossfight.missions:GetChildren()) do
					if v:FindFirstChild("missiongiver") then
						table.insert(bosslist, v.Name)
					end
				end
			end
			for i,v in pairs(game:GetService("Workspace").bossdropmission.missions:GetChildren()) do
				if v:FindFirstChild("missiongiver") then
					table.insert(bosslist, v.Name)
				end
			end

			local specific_boss
			window1:Toggle("Specific Boss",false, function(bool)
				specific_boss = bool
			end)

			local selected_boss
			window1:Dropdown("Specific boss to kill",bosslist,function(B)
				print("Selected: " .. B)
				notify("Boss Selected: " .. B)
				selected_boss = B
			end)

			local random_boss
			window1:Toggle("Random Bosses",true, function(bool)
				random_boss = bool
			end)
			random_boss = true

			window1:Label("If autofarm stuck:", Color3.fromRGB(127, 143, 166))
			window1:Label("In village = reset script", Color3.fromRGB(127, 143, 166))
			window1:Label("Outside village = rejoin", Color3.fromRGB(127, 143, 166))

			local function QUEST()
				for i,v in pairs(workspace.missiongivers:GetChildren()) do
					if v.Name == "" and v:FindFirstChild("Head") then
						local TALK = v:FindFirstChild("Talk")
						if v:FindFirstChild("Head") and v.Head:FindFirstChild("givemission") and v.Head.givemission.Enabled and v.Head.givemission:FindFirstChild("color").Image == "http://www.roblox.com/asset/?id=5459241648" --[[or v.Head.givemission.color.Image == "http://www.roblox.com/asset/?id=5459241799"]] then
							if v:WaitForChild("Talk"):WaitForChild("mobname") then
								local getmission = v:FindFirstChild("HumanoidRootPart")
								local clienttalk = v:FindFirstChild("CLIENTTALK")
								repeat
									player.Character.HumanoidRootPart.CFrame=getmission.CFrame+Vector3.new(0,3,0)
									clienttalk:FireServer()
									clienttalk:FireServer("accept")  
									wait()
								until mission.Visible or mission.title.text == "[Objective: Mission]" or not autofarm
							end
						end
					end
				end
			end

			local function FARM()
				if game:GetService("Players").LocalPlayer.statz.sword.throwable.Value == "BOSSDROPazimftg" or game:GetService("Players").LocalPlayer.statz.sword.throwable.Value == "kunaiwire" or game:GetService("Players").LocalPlayer.statz.sword.throwable.Value == "kunaiflash" then
					notify("Please change the throwable weapon!")
					autofarm = false
					boss = false
				else
					if BeastNRods then
						for i,v in pairs(workspace.npc:GetChildren()) do
							local mobname = string.split(mission.bg.name.Text,"Defeat ")[1]
							if(string.find(mobname,"(s)")) then
								mobname = string.gsub(mobname,"(s)","")
							end
							if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and not v:FindFirstChild("fullsusanoo") and v.Head.CFrame.Y > -1000 then
								repeat
									pcall(function()
										player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame+Vector3.new(0,-2,0)
										v.Humanoid.Health = 0
										game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
										game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
									end)
									pcall(function()
										game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
										player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame+Vector3.new(0,-2,-6.8)
										v.Humanoid.Health = 0
										game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
										wait()
									end)
									pcall(function()
										game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
										player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame+Vector3.new(-6.8,-2,0)
										v.Humanoid.Health = 0
										game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
										wait()
									end)
									pcall(function()
										game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
										player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame+Vector3.new(0,-2,6.8)
										v.Humanoid.Health = 0
										game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
										wait()
									end)
									pcall(function()
										game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
										player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame+Vector3.new(6.8,-2,0)
										v.Humanoid.Health = 0
										game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
										wait()
									end)
								until not v:FindFirstChild("Humanoid") or mission.Visible == false or not autofarm or not v
							end
						end
					elseif BeastOnly then
						for i,v in pairs(workspace.npc:GetChildren()) do
							local mobname = string.split(mission.bg.name.Text,"Defeat ")[1]
							if(string.find(mobname,"(s)")) then
								mobname = string.gsub(mobname,"(s)","")
							end
							if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and not v:FindFirstChild("fullsusanoo") and v.Head.CFrame.Y > -1000 then
								player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 15, 0)
								repeat wait()
									pcall(function()
										game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
										v.Humanoid.Health = 0
										pcall(function()
											player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 17, 10)
											game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
											game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
											game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
											game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
											game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
											game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
											game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
											game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
											v.Humanoid.Health = 0
										end)
									end)
								until not v:FindFirstChild("Humanoid") or mission.Visible == false or not autofarm or not v
							end
						end
					elseif NoneOA then
						for i,v in pairs(workspace.npc:GetChildren()) do
							local mobname = string.split(mission.bg.name.Text,"Defeat ")[1]
							if(string.find(mobname,"(s)")) then
								mobname = string.gsub(mobname,"(s)","")
							end
							if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and not v:FindFirstChild("fullsusanoo") and v.Head.CFrame.Y > -1000 then
								repeat
									pcall(function()
										v.Humanoid.Health = 0
										game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
										player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
										game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
										game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
										game.Players.LocalPlayer.Character.combat.update:FireServer("mouse1",true)
										wait(.000000000000001)
										v.Humanoid.Health = 0
									end)
								until not v:FindFirstChild("Humanoid") or mission.Visible == false or not autofarm or not v
							end
						end
					end
				end
			end

			local function BOSSFARM()
				if random_boss then
					if game.PlaceId == 7923764447 then
						for i,v in pairs(workspace.sengokubossfight.missions:GetChildren()) do
							if v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
								player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
								local args = {[1] = game:GetService("Players").LocalPlayer}
								v.missiongiver.CLIENTTALK:FireServer(unpack(args))
								wait()
							end
						end
					end
					for i,v in pairs(workspace.bossdropmission.missions:GetChildren()) do
						if v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
							player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
							local args = {[1] = game:GetService("Players").LocalPlayer}
							v.missiongiver.CLIENTTALK:FireServer(unpack(args))
							wait()
						end
					end
				elseif specific_boss then
					if game.PlaceId == 7923764447 then
						for i,v in pairs(workspace.sengokubossfight.missions:GetChildren()) do
							if v.Name == selected_boss and v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
								player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
								local args = {[1] = game:GetService("Players").LocalPlayer}
								v.missiongiver.CLIENTTALK:FireServer(unpack(args))
								wait()
							end
						end
					end
					for i,v in pairs(workspace.bossdropmission.missions:GetChildren()) do
						if v.Name == selected_boss and v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
							player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
							local args = {[1] = game:GetService("Players").LocalPlayer}
							v.missiongiver.CLIENTTALK:FireServer(unpack(args))
							wait()
						end
					end
				end
			end

			local count
			local function DISP()
				count = 0
				for i,v in pairs(workspace.missiongivers:GetChildren()) do
					if v.Name == "" and v:FindFirstChild("Head") then
						if v:FindFirstChild("Head") and v.Head:FindFirstChild("givemission") and v.Head.givemission.Enabled and v.Head.givemission:FindFirstChild("color") and v.Head.givemission.color.Image == "http://www.roblox.com/asset/?id=5459241648" then
							count = count + 1
						end
					end
				end
			end

			spawn(function()
				while wait() do
					if autofarm or boss then
						if mission.Visible == true then
							FARM()
							for i,v in pairs(workspace.npc:GetChildren()) do
								if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
									repeat 
										v.Humanoid.Health = 0 
										v.Humanoid.Health = 0
										v.Humanoid.Health = 0
										v.Humanoid.Health = 0
										v.Humanoid.Health = 0
									until (not autofarm and not boss) or v.Humanoid.Health == 0
								end
							end
						else
							if boss and autofarm then
								BOSSFARM()
								wait(.1)
								DISP()
								wait(.1)
								QUEST()
							elseif boss then
								BOSSFARM()
							elseif autofarm then
								DISP()
								wait(.1)
								QUEST()
							end
						end
					end
				end
			end)

			spawn(function()
				while wait() do
					if autofarm then
						if count == 0 then
							if QUEST() or FARM() or BOSSFARM() then
								wait(5.5)
							else
								repeat wait()
									notify("Currently no missions avaliable")
									wait(2)
								until count > 0 or QUEST or FARM or BOSSFARM() or not autofarm
							end
						end
					end
				end
			end)

			spawn(function()
				while wait() do
					if scrollfarm then
						repeat wait()
							SCROLLFARM()
							SCROLLFARM1()
						until not scrollfarm
					end
				end
			end)

			local PRES = player.statz.prestige.maxlvlpres
			local RANK = player.statz.prestige.rank
			local RANKNUM = player.statz.prestige.number

			spawn(function()
				while wait() do
					if RANKUPP and player.statz.lvl:FindFirstChild("lvl").Value == 1000 then
						repeat wait()
							game.Players.LocalPlayer.startevent:FireServer("rankup")
						until player.statz.lvl:FindFirstChild("lvl").Value == 1 or not RANKUPP
					end
				end
			end)

