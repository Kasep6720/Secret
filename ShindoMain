	window3:Button("Reset Script",function()
		DailyChi = false
		DailyRush = false
		DailyJump = false
		DailyM1 = false
		DailyBoss = false
		punches = false
		shoot = false
		block = false
		tree = false
		EnabledAutoMentor = false
		library:Destroy()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Shindo/ShindoMain"))()
	end)

