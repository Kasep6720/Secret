	if game:GetService("Workspace"):FindFirstChild("npc"):FindFirstChild("npc1") then
		dfarm = true
	end
	if game:GetService("Workspace"):FindFirstChild("warmode") then
		dfarm = true
	end

	for i,v in pairs(workspace.Terrain:GetChildren()) do
		if v.Name(player.Name) then
			v:Destroy()
			wait(30)
		end
	end

end)

