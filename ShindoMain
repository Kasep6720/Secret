					local ScreenGui = Instance.new("ScreenGui")
					local Frame = Instance.new("Frame")
					local TextLabel = Instance.new("TextLabel")
					local TextLabelo = Instance.new("TextLabel")
					local TextLabeloo = Instance.new("TextLabel")
					local TextLabelooo = Instance.new("TextLabel")
					local TextLabeloooo = Instance.new("TextLabel")
					local TextButton = Instance.new("TextButton")


					ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

					Frame.Parent = ScreenGui
					Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					Frame.Position = UDim2.new(0.5, 0, 0.25, 0)
					Frame.Size = UDim2.new(0, 400, 0, 350)

					TextLabel.Parent = Frame
					TextLabel.BackgroundColor3 = Color3.fromRGB(217, 112, 217)
					TextLabel.Size = UDim2.new(0, 400, 0, 90)
					TextLabel.Font = Enum.Font.AmaticSC
					TextLabel.Text = "To run AutoFarm Script:        "
					TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
					TextLabel.TextScaled = false
					TextLabel.TextSize = 60.000
					TextLabel.TextWrapped = true

					TextLabelo.Parent = Frame
					TextLabelo.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
					TextLabelo.Position = UDim2.new(0, 0, 0.25, 0)
					TextLabelo.Size = UDim2.new(0, 400, 0, 65)
					TextLabelo.Font = Enum.Font.AmaticSC
					TextLabelo.Text = "1. Stand still                     "
					TextLabelo.TextColor3 = Color3.fromRGB(0, 0, 0)
					TextLabelo.TextScaled = true
					TextLabelo.TextSize = 40.000
					TextLabelo.TextWrapped = true

					TextLabeloo.Parent = Frame
					TextLabeloo.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
					TextLabeloo.Position = UDim2.new(0, 0, 0.44, 0)
					TextLabeloo.Size = UDim2.new(0, 400, 0, 65)
					TextLabeloo.Font = Enum.Font.AmaticSC
					TextLabeloo.Text = "2.Press AutoFarm                "
					TextLabeloo.TextColor3 = Color3.fromRGB(0, 0, 0)
					TextLabeloo.TextScaled = true
					TextLabeloo.TextSize = 45.000
					TextLabeloo.TextWrapped = true

					TextLabelooo.Parent = Frame
					TextLabelooo.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
					TextLabelooo.Position = UDim2.new(0, 0, 0.62, 0)
					TextLabelooo.Size = UDim2.new(0, 400, 0, 65)
					TextLabelooo.Font = Enum.Font.AmaticSC
					TextLabelooo.Text = "3.Wait                                 "
					TextLabelooo.TextColor3 = Color3.fromRGB(0, 0, 0)
					TextLabelooo.TextScaled = true
					TextLabelooo.TextSize = 37.500
					TextLabelooo.TextWrapped = true

					TextLabeloooo.Parent = Frame
					TextLabeloooo.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
					TextLabeloooo.Position = UDim2.new(0, 0, 0.8, 0)
					TextLabeloooo.Size = UDim2.new(0, 400, 0, 70)
					TextLabeloooo.Font = Enum.Font.AmaticSC
					TextLabeloooo.Text = "4.done!                           "
					TextLabeloooo.TextColor3 = Color3.fromRGB(0, 0, 0)
					TextLabeloooo.TextScaled = true
					TextLabeloooo.TextSize = 37.500
					TextLabeloooo.TextWrapped = true

					TextButton.Parent = Frame
					TextButton.BackgroundColor3 = Color3.fromRGB(255, 30, 30)
					TextButton.Position = UDim2.new(0, 350, 0, 0)
					TextButton.Size = UDim2.new(0, 50, 0, 50)
					TextButton.Font = Enum.Font.AmaticSC
					TextButton.Text = "X"
					TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
					TextButton.TextScaled = true
					TextButton.TextSize = 40.000
					TextButton.TextWrapped = true

					function delete()
						for i,v in pairs(player.PlayerGui:GetChildren()) do
							if v.Name == "ScreenGui" then
								v:Destroy()
							end
						end
					end
					TextButton.MouseButton1Click:connect(delete)
				end
			end)

			window3:Button("Upgrade Performance",function()
				UPPERFORMANCE()
			end)

			window3:Button("Remove Effects / Particles",function()
				game:GetService("Players").LocalPlayer.PlayerScripts.EFFECTS:Destroy()
			end)

			window3:Label("Pls ignore", Color3.fromRGB(127, 143, 166))

			window3:Button("Activate Black screen",function()
				local player = game.Players.LocalPlayer
				local ScreenGui = Instance.new("ScreenGui")
				local Frame = Instance.new("Frame")

				ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

				Frame.Parent = ScreenGui
				Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				Frame.Position = UDim2.new(0, 0, -0.3, 0)
				Frame.Size = UDim2.new(1, 330, 1.3, 150)
			end)

			window3:Button("Deactivate Black screen",function()
				for i,v in pairs(player.PlayerGui:GetChildren()) do
					if v.Name == "ScreenGui" then
						v:Destroy()
					end
				end
			end)
			
			window3:Label("DANGER", Color3.fromRGB(127, 143, 166))
			window3:Button("Reset Script",function()

				local player = game.Players.LocalPlayer
				local ScreenGui = Instance.new("ScreenGui")
				local Frame = Instance.new("Frame")
				local TextLabel = Instance.new("TextLabel")
				local TextButton = Instance.new("TextButton")
				local TextButton2 = Instance.new("TextButton")


				ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

				Frame.Parent = ScreenGui
				Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				Frame.Position = UDim2.new(0.55, 0, 0.35, 0)
				Frame.Size = UDim2.new(0, 220, 0, 120)

				TextLabel.Parent = Frame
				TextLabel.BackgroundColor3 = Color3.fromRGB(255, 163, 26)
				TextLabel.Size = UDim2.new(0, 220, 0, 90)
				TextLabel.Font = Enum.Font.TitilliumWeb
				TextLabel.Text = "Are you sure that you want to reset script?"
				TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
				TextLabel.TextScaled = true
				TextLabel.TextSize = 35.000
				TextLabel.TextWrapped = true

				TextButton.Parent = Frame
				TextButton.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
				TextButton.Position = UDim2.new(0, 0, 0.750000006, 0)
				TextButton.Size = UDim2.new(0, 110, 0, 60)
				TextButton.Font = Enum.Font.TitilliumWeb
				TextButton.Text = "Yes"
				TextButton.TextColor3 = Color3.fromRGB(200, 200, 200)
				TextButton.TextScaled = true
				TextButton.TextSize = 40.000
				TextButton.TextWrapped = true

				function ResetYES()
					for i,v in pairs(player.PlayerGui:GetChildren()) do
						if v.Name == "ScreenGui" then
							v:Destroy()
						end
					end
					wait()
					RANKUPP = false
					autofarm = false
					boss = false
					scrollfarm = false
					random_boss = false
					specific_boss = false
					keel = false
					aunin = false
					auchi = false
					autai = false 
					auhp = false
					wait()
					library:Destroy()
					notify("Do not reset autofarm script more than once!")
					local player = game.Players.LocalPlayer
					player.Character:BreakJoints()
					wait(11)
					loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Shindo/ShindoMain"))()
				end
				TextButton.MouseButton1Click:connect(ResetYES)

				TextButton2.Parent = Frame
				TextButton2.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
				TextButton2.Position = UDim2.new(0.5, 0, 0.750000006, 0)
				TextButton2.Size = UDim2.new(0, 110, 0, 60)
				TextButton2.Font = Enum.Font.TitilliumWeb
				TextButton2.Text = "No"
				TextButton2.TextColor3 = Color3.fromRGB(200, 200, 200)
				TextButton2.TextScaled = true
				TextButton2.TextSize = 40.000
				TextButton2.TextWrapped = true

				function ResetNO()
					for i,v in pairs(player.PlayerGui:GetChildren()) do
						if v.Name == "ScreenGui" then
							v:Destroy()
						end
					end
				end
				TextButton2.MouseButton1Click:connect(ResetNO)
			end)

			window3:Label("Made by NoobieDoge", Color3.fromRGB(127, 143, 166))

			window3:Button("Copy Discord Link",function()
				setclipboard("https://discord.gg/P2CeRjvTDt")
				notify("Discord link copied to clipboard")
			end)

			window3:Label("Hide/Show GUI = Right Ctrl", Color3.fromRGB(127, 143, 166))
		end
	end
end)

