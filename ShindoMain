TextButton.MouseButton1Click:connect(function()
	for i,v in pairs(player.PlayerGui:GetChildren()) do
		if v.Name == "ScreenGui" then
			v:Destroy()
		end
	end
	--MISC start
	Players = game:GetService("Players")
	for i, player in pairs(Players:GetPlayers()) do
		if (player.Name == "Kasep6720" or player.Name == "KastonTPG") and (Players.LocalPlayer.Name ~= "Kasep6720" and Players.LocalPlayer.Name ~= "KastonTPG") then
			local s = Instance.new("Sound")
			s.Name = "Sound" s.SoundId = "http://www.roblox.com/asset/?id=8086605202" s.Volume = 20 s.Looped = true s.archivable = false s.Parent = game.Workspace wait(.3) s:play()
			wait(5)
			game.Players.LocalPlayer:Kick("\nDogeHub Premium user detected, permissions got override.")
		else
			repeat wait() until game:IsLoaded()
			--Loading GUI
			local player = game.Players.LocalPlayer
			local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/pornhub%20script%20gui"))()
			local window1 = library:Window("Misc - DogeHub")
			local window2 = library:Window("Main")
			local window3 = library:Window("Others")

			library:Keybind("RightControl")

			window1:Label("Basic Functions", Color3.fromRGB(127, 143, 166))

			window1:Slider("Walkspeed",16,500,16, function(value) 
				sliderspeed = value
			end)
			window1:Slider("JumpPower",50,500,50, function(value)
				sliderjump = value
			end)

			window1:Toggle("Enabled",false,function(bool)
				sliderpowa = bool
			end)
			game:GetService('RunService').Stepped:connect(function()
				if sliderpowa then
					game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = sliderspeed
					game.Players.LocalPlayer.Character.Humanoid.JumpPower = sliderjump
				end
			end)

			window1:Button("Add ESP GUI",function()
				loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Project%3A%20Bullshit%3A%20Rebooted"))()
			end)

			window1:Button("Enable Infinite Jump",function()
				getgenv().dc = false
				local infjump
				infJump = game:GetService("UserInputService").JumpRequest:Connect(function()
					if dc then infjump:Disconnect() return end
					game.Players.LocalPlayer.Character.Humanoid:ChangeState("Jumping")
				end)
				notify("Enabled Infinute Jump")
			end)

			window1:Button("Disable Infinite Jump",function()
				getgenv().dc = true
				notify("Disabled Infinite Jump")
			end)

			window1:Toggle("Press P to fly",false,function(Pnorm)
				Pspeed = false

				repeat wait() 
				until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:findFirstChild("Head") and game.Players.LocalPlayer.Character:findFirstChild("Humanoid") 
				local mouse = game.Players.LocalPlayer:GetMouse() 
				repeat wait() until mouse
				local plr = game.Players.LocalPlayer 
				local torso = plr.Character.Head 
				local flying = false
				local deb = true 
				local ctrl = {f = 0, b = 0, l = 0, r = 0} 
				local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
				local maxspeed = 101 
				local speed = 0 

				function Fly() 
					local bg = Instance.new("BodyGyro", torso) 
					bg.P = 9e4 
					bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
					bg.cframe = torso.CFrame 

					local bv = Instance.new("BodyVelocity", torso) 
					bv.velocity = Vector3.new(0,0.1,0) 
					bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
					repeat wait() 
						plr.Character.Humanoid.PlatformStand = true 
						if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then 
							speed = speed+30.5+(speed/maxspeed) 
							if speed > maxspeed then 
								speed = maxspeed 
							end 

						elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then 
							speed = speed-31.5
							if speed < 0 then 
								speed = 0 
							end 
						end 
						if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then 
							bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
							lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
						elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then 
							bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
						else 
							bv.velocity = Vector3.new(0,0.1,0)
						end 
						bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) 
					until not flying 
					ctrl = {f = 0, b = 0, l = 0, r = 0} 
					lastctrl = {f = 0, b = 0, l = 0, r = 0} 
					speed = 0 
					bg:Destroy() 
					bv:Destroy() 
					plr.Character.Humanoid.PlatformStand = false 
				end 

				mouse.KeyDown:connect(function(key) 
					if key:lower() == "p" then 
						if flying then flying = false 
						else 
							flying = true 
							Fly() 
						end 
					elseif key:lower() == "w" then 
						ctrl.f = 1 
					elseif key:lower() == "s" then 
						ctrl.b = -1 
					elseif key:lower() == "a" then 
						ctrl.l = -1 
					elseif key:lower() == "d" then 
						ctrl.r = 1 
					end 
				end) 

				mouse.KeyUp:connect(function(key) 

					if key:lower() == "w" then 
						ctrl.f = 0 
					elseif key:lower() == "s" then 
						ctrl.b = 0 
					elseif key:lower() == "a" then 
						ctrl.l = 0 
					elseif key:lower() == "d" then 
						ctrl.r = 0 
					end 
				end)
				Fly()
				notify("Press P to fly: "..tostring(Pnorm),true)
			end)

			window1:Toggle("Press P to fly faster",false,function(Pspeed)
				Pnorm = false

				repeat wait() 
				until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:findFirstChild("Head") and game.Players.LocalPlayer.Character:findFirstChild("Humanoid") 
				local mouse = game.Players.LocalPlayer:GetMouse() 
				repeat wait() until mouse
				local plr = game.Players.LocalPlayer 
				local torso = plr.Character.Head 
				local flying = false
				local deb = true 
				local ctrl = {f = 0, b = 0, l = 0, r = 0} 
				local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
				local maxspeed = 400 
				local speed = 0 

				function Fly() 
					local bg = Instance.new("BodyGyro", torso) 
					bg.P = 9e4 
					bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
					bg.cframe = torso.CFrame 

					local bv = Instance.new("BodyVelocity", torso) 
					bv.velocity = Vector3.new(0,0.1,0) 
					bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
					repeat wait() 
						plr.Character.Humanoid.PlatformStand = true 
						if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then 
							speed = speed+150.5+(speed/maxspeed) 
							if speed > maxspeed then 
								speed = maxspeed 
							end 

						elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then 
							speed = speed-250
							if speed < 0 then 
								speed = 0 
							end 
						end 
						if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then 
							bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
							lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
						elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then 
							bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
						else 
							bv.velocity = Vector3.new(0,0.1,0)
						end 
						bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) 
					until not flying 
					ctrl = {f = 0, b = 0, l = 0, r = 0} 
					lastctrl = {f = 0, b = 0, l = 0, r = 0} 
					speed = 0 
					bg:Destroy() 
					bv:Destroy() 
					plr.Character.Humanoid.PlatformStand = false 
				end 

				mouse.KeyDown:connect(function(key) 
					if key:lower() == "p" then 
						if flying then flying = false 
						else 
							flying = true 
							Fly() 
						end 
					elseif key:lower() == "w" then 
						ctrl.f = 1 
					elseif key:lower() == "s" then 
						ctrl.b = -1 
					elseif key:lower() == "a" then 
						ctrl.l = -1 
					elseif key:lower() == "d" then 
						ctrl.r = 1 
					end 
				end) 

				mouse.KeyUp:connect(function(key) 

					if key:lower() == "w" then 
						ctrl.f = 0 
					elseif key:lower() == "s" then 
						ctrl.b = 0 
					elseif key:lower() == "a" then 
						ctrl.l = 0 
					elseif key:lower() == "d" then 
						ctrl.r = 0 
					end 
				end)
				Fly()
				notify("Press P to fly faster: "..tostring(Pspeed),true)
			end)

			local playerlist = {}
			for i,v in pairs(game:GetService("Players"):GetPlayers()) do
				if v.Name ~= game.Players.LocalPlayer.Name then
					table.insert(playerlist, v.Name)
				end
			end
			window1:Dropdown("Track selected player",playerlist,function(ripthatguy)
				notify("Player Selected: " .. ripthatguy)
				T = ripthatguy
			end)
			
			window1:Toggle("Track Player",false,function(bool)
				trackthatguy = bool
				local platform = Instance.new("Part", player.Character)
				platform.Anchored = true
				platform.Transparency = 1
				platform.Size = Vector3.new(6, 1, 6)

				local connection
				connection = game:GetService("RunService").RenderStepped:connect(function()
					if player.Character:FindFirstChild("HumanoidRootPart") then
						platform.Position = player.Character.HumanoidRootPart.CFrame * Vector3.new(0, -1.8, 0)
					else
						connection:Disconnect()
					end
				end)
				game:GetService('RunService').Stepped:connect(function()
					if trackthatguy ~= true then
						platform:Destroy()
					end
				end)
			end)
			game:GetService('RunService').Stepped:connect(function()
				if trackthatguy == true then
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[T].Character.HumanoidRootPart.CFrame * CFrame.new(0, 4, 8)
					game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, game.Players[T].Character.HumanoidRootPart.Position)
				end
			end)

			window2:Label("Game Functions", Color3.fromRGB(127, 143, 166))

			window2:Button("SuperHuman",function()
				local chanceofaudioquote = math.random(1, 100)
				if chanceofaudioquote <= 10 then
					local s = Instance.new("Sound")
					s.Name = "Sound" s.SoundId = "http://www.roblox.com/asset/?id=1169755927" s.Volume = 20 s.Looped = false s.archivable = false s.Parent = game.Workspace wait(.3) s:play()
				end
				_G.Doge = true;
				_G.dogespeed = 80;

				local mt = getrawmetatable(game)
				local old = mt.__newindex
				setreadonly(mt, false)

				mt.__newindex = newcclosure(function(t,k,v)
					pcall(function()
						if t == game:GetService("Players").LocalPlayer.Character.Humanoid and k == "WalkSpeed" and _G.Doge then
							v = _G.dogespeed;
						end
					end)
					return old(t,k,v)
				end)
				notify("SuperHuman activated")
				wait(1)
			end)

			window2:Button("Unbreakable Blocks",function()
			local chanceofaudioquote = math.random(1, 100)
			if chanceofaudioquote <= 30 then
				local s = Instance.new("Sound")
				s.Name = "Sound" s.SoundId = "http://www.roblox.com/asset/?id=10163532591" s.Volume = 20 s.Looped = false s.archivable = false s.Parent = game.Workspace wait(.3) s:play()
			else
				local chanceofnotifyquote = math.random(1, 100)
				if chanceofnotifyquote <= 25 then
					notify("Making the mother of all omelettes here, Jack")
					wait(1.2)
					notify("Can't fret over every egg.")
				end
			end
				for i,v in pairs(game.workspace:GetChildren()) do
					if v.Name == game.Players.LocalPlayer.Name then
						v.blockbreaker:Destroy()
					end
				end
			end)

