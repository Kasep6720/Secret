						punches = true
						tree = false
						shoot = false
						block = false
						dfarm = false
					elseif mission.title.Text == "[Objective: Tree-Jump Training]" then
						punches = false
						tree = true
						shoot = false
						block = false
						dfarm = false
					elseif mission.title.Text == "[Objective: Accuracy Training]" then
						punches = false
						tree = false
						shoot = true
						block = false
						dfarm = false
					elseif mission.title.Text == "[Objective: Deflecting Training]" then
						punches = false
						tree = false
						shoot = false
						block = true
						dfarm = false
					elseif game.PlaceId == 8184506020 then
						punches = false
						tree = false
						shoot = false
						block = false
						dfarm = true
					end
				end
			end
		end	
	end)
	
