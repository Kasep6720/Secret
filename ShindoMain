function AUTOFARM()
    local player = game.Players.LocalPlayer
    local ScreenGui = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local TextLabel = Instance.new("TextLabel")
    local TextButton = Instance.new("TextButton")
    local TextButton_2 = Instance.new("TextButton")
    local TextButton_24 = Instance.new("TextButton")

    ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

    Frame.Parent = ScreenGui
    Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Frame.Position = UDim2.new(0.1, 0, 0.25, 0)
    Frame.Size = UDim2.new(0, 200, 0, 300)

    TextLabel.Parent = Frame
    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
    TextLabel.Size = UDim2.new(0, 200, 0, 60)
    TextLabel.Font = Enum.Font.AmaticSC
    TextLabel.Text = "Autofarms"
    TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
    TextLabel.TextScaled = true
    TextLabel.TextSize = 35.000
    TextLabel.TextWrapped = true

    TextButton.Parent = Frame
    TextButton.BackgroundColor3 = Color3.fromRGB(117, 50, 168)
    TextButton.Position = UDim2.new(0, 0, 0.20000006, 0)
    TextButton.Size = UDim2.new(0, 200, 0, 120)
    TextButton.Font = Enum.Font.AmaticSC
    TextButton.Text = "Mentors & dailies"
    TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
    TextButton.TextScaled = false
    TextButton.TextSize = 49.99999
    TextButton.TextWrapped = true

    function mentor()
        for i,v in pairs(player.PlayerGui:GetChildren()) do
            if v.Name == "ScreenGui" then
                v:Destroy()
            end
        end
        repeat wait() until game:IsLoaded()
        --old-antitp bypass
        if workspace:FindFirstChild("CCoff") then
            game:GetService("Workspace").CCoff:Destroy()
        end
        --antiafk
        local VirtualUser=game:service'VirtualUser'
            game:service'Players'.LocalPlayer.Idled:connect(function()
            warn("anti-afk")
            VirtualUser:CaptureController()
            VirtualUser:ClickButton2(Vector2.new())
        end)
        --variables
        local player = game.Players.LocalPlayer
        function toTarget(pos, targetPos, targetCFrame)
            local tween_s = game:service"TweenService"
            local info = TweenInfo.new((targetPos - pos).Magnitude/getgenv().speed, Enum.EasingStyle.Linear)
            local tween, err = pcall(function()
            local tween = tween_s:Create(game:GetService("Players").LocalPlayer.Character["HumanoidRootPart"], info, {CFrame = targetCFrame * CFrame.fromAxisAngle(Vector3.new(1,0,0), math.rad(90))})
            tween:Play()
            end)
            if not tween then return err end
        end

        --loading GUI
        local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/pornhub%20script%20gui"))()
        local window1 = library:Window("Mentor Farm - DogeHub")
        local window2 = library:Window("Others")

        library:Keybind("RightControl")

        --MENTORFARM
        getgenv().speed = 4000
        window1:Label("Teleport to Mentors", Color3.fromRGB(127, 143, 166))

        window1:Button("Tp to Narumaki Mentor",function()
            if game.PlaceId == 5451398863 then
                player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Narumaki"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
            else
                game:GetService("StarterGui"):SetCore("SendNotification",{     Title = "Wrong Map",     Text = "Narumaki mentor is not in this map!",Duration = 5, })
            end
        end)
        window1:Button("Tp to Jiso Mentor",function() 
            if game.PlaceId == 6901575446 then
                player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Jiso Seishin"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
            else
                game:GetService("StarterGui"):SetCore("SendNotification",{     Title = "Wrong Map",     Text = "Jiso Seishin mentor is not in this map!",Duration = 5, })
            end
        end)
        window1:Button("Tp to Shindai Mentor",function()
            if game.PlaceId == 5451401540 then
                player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Shindai Akuma"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
            else
                game:GetService("StarterGui"):SetCore("SendNotification",{     Title = "Wrong Map",     Text = "Shindai Akuma mentor is not in this map!",Duration = 5, })
            end
        end)
        window1:Button("Tp to Ryuji Mentor",function()
            if game.PlaceId == 5431071837 then
                player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Ryuji Kenichi"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
            else
