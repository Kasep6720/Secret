    auchi = bool
end)
game:GetService('RunService').Stepped:connect(function()
    if auchi then
        pcall(function()
local args = {[1] = "addstat",[2] = "chakra",[3] = 18}
game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
wait(.25)
        end)
    end
end)
