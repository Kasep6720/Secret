                TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                TextButton.TextScaled = true
                TextButton.TextSize = 40.000
                TextButton.TextWrapped = true

                function delete()
                    for i,v in pairs(player.PlayerGui:GetChildren()) do
                        if v.Name == "ScreenGui" then
                            v:Destroy()
                        end
                    end
                end
                TextButton.MouseButton1Click:connect(delete)
            end)

            window1:Button("Upgrade Performance",function()
                local decalsyeeted = true -- Leaving this on makes games look shitty but the fps goes up by at least 20.
                local g = game
                local w = g.Workspace
                local l = g.Lighting
                local t = w.Terrain
                t.WaterWaveSize = 0
                t.WaterWaveSpeed = 0
                t.WaterReflectance = 0
                t.WaterTransparency = 0
                l.GlobalShadows = false
                l.FogEnd = 9e9
                l.Brightness = 0
                settings().Rendering.QualityLevel = "Level01"
                for i,v in pairs(g:GetDescendants()) do
                    if v:IsA("Part") or v:IsA("Union") or v:IsA("MeshPart") then
                        v.Material = "Plastic"
                        v.Reflectance = 0
                    elseif v:IsA("Decal") and decalsyeeted then 
                        v.Transparency = 1
                    elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then 
                        v.Lifetime = NumberRange.new(0)
                    end
                end
            end)

            local function QUEST()
                for i,v in pairs(workspace.missiongivers:GetChildren()) do
                    if v.Name == "" and v:FindFirstChild("Head") then
                        local TALK = v:FindFirstChild("Talk")
                        if v:FindFirstChild("Head") and v.Head:FindFirstChild("givemission") and v.Head.givemission.Enabled and v.Head.givemission:FindFirstChild("color").Image == "http://www.roblox.com/asset/?id=5459241648" --[[or v.Head.givemission.color.Image == "http://www.roblox.com/asset/?id=5459241799"]] then
                            if v:WaitForChild("Talk"):WaitForChild("mobname") then
                                local getmission = v:FindFirstChild("HumanoidRootPart")
                                local clienttalk = v:FindFirstChild("CLIENTTALK")
                                repeat wait()
                                    pcall(function()
                                        player.Character.HumanoidRootPart.CFrame=getmission.CFrame+Vector3.new(0,-7,0)
                                        clienttalk:FireServer()
                                        clienttalk:FireServer("accept")
                                        wait()
                                    end)                
                                until mission.Visible or not autooofarm
                            end
                        end
                    end
                end
            end

            local function FARM()
                for i,v in pairs(workspace.npc:GetChildren()) do
                    local mobname = string.split(mission.bg.name.Text,"Defeat ")[1]
                    if(string.find(mobname,"(s)")) then
                        mobname = string.gsub(mobname,"(s)","")
                    end
                    if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
                        wait(.00000009)
                        repeat wait()
                            pcall(function()
                                wait(.0005)
                                game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
                                player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
                                game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
                                game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
                                game.Players.LocalPlayer.Character.combat.update:FireServer("mouse1",true)
                                wait(.000000000000001)
                                v.Humanoid.Health = 0
                            end)
                            pcall(function()
                                wait(.00035)
                                game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
                                player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame+Vector3.new(0,-3.9,-3.4)
                                game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
                                game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
                                game.Players.LocalPlayer.Character.combat.update:FireServer("mouse1",true)
                                wait(.000000000000001)
                                v.Humanoid.Health = 0
                            end)
                        until v.Humanoid.Health == 0 or mission.Visible == false or not autooofarm or not v
                    end
                end
            end

            spawn(function()
                while wait() do
                    if autooofarm then
                        if mission.Visible == false then
                            QUEST()
                        else
                            FARM()
                        end
                    end
                end
            end)

            local PRES = player.statz.prestige.maxlvlpres
            local RANK = player.statz.prestige.rank
            local RANKNUM = player.statz.prestige.number

            spawn(function()
                while wait() do
                    if RANKUPP and player.statz.lvl:FindFirstChild("lvl").Value == 1000 then
                        repeat wait()
                            game.Players.LocalPlayer.startevent:FireServer("rankup")
                        until player.statz.lvl:FindFirstChild("lvl").Value == 1 or not RANKUP
                    end
                end
            end)

            window2:Label("Auto stats", Color3.fromRGB(127, 143, 166))

            window2:Toggle("Auto Chi",false, function(bool)
                auchi = bool
            end)
            game:GetService('RunService').Stepped:connect(function()
                if auchi then
                    pcall(function()
            local args = {[1] = "addstat",[2] = "chakra",[3] = 18}
            game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
            wait(.25)
                    end)
                end
            end)
            window2:Toggle("Auto Nin",false, function(bool)
                aunin = bool
            end)
            game:GetService('RunService').Stepped:connect(function()
                if aunin then
                    pcall(function()
            local args = {[1] = "addstat",[2] = "ninjutsu",[3] = 18}
            game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
            wait(.25)
                    end)
                end
            end)
            window2:Toggle("Auto Tai",false, function(bool)
                autai = bool
            end)
            game:GetService('RunService').Stepped:connect(function()
                if autai then
                    pcall(function()
            local args = {[1] = "addstat",[2] = "taijutsu",[3] = 18}
            game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
            wait(.25)
                    end)
                end
            end)
            window2:Toggle("Auto Health",false, function(bool)
                auhp = bool
            end)
            game:GetService('RunService').Stepped:connect(function()
                if auhp then
                    pcall(function()
            local args = {[1] = "addstat",[2] = "health",[3] = 18}
            game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
            wait(.25)
                    end)
                end
            end)

            window2:Label("Pls ignore", Color3.fromRGB(127, 143, 166))

            window2:Button("Activate Black screen",function()
                local player = game.Players.LocalPlayer
                local ScreenGui = Instance.new("ScreenGui")
                local Frame = Instance.new("Frame")

                ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

                Frame.Parent = ScreenGui
                Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                Frame.Position = UDim2.new(0, 0, -0.3, 0)
                Frame.Size = UDim2.new(1, 330, 1.3, 150)
            end)

            window2:Button("Deactivate Black screen",function()
                for i,v in pairs(player.PlayerGui:GetChildren()) do
                    if v.Name == "ScreenGui" then
                        v:Destroy()
                    end
                end
            end)

            window2:Label("DANGER", Color3.fromRGB(127, 143, 166))
            window2:Button("Reset Script",function()

                local player = game.Players.LocalPlayer
                local ScreenGui = Instance.new("ScreenGui")
                local Frame = Instance.new("Frame")
                local TextLabel = Instance.new("TextLabel")
                local TextButton = Instance.new("TextButton")
                local TextButton2 = Instance.new("TextButton")


                ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

                Frame.Parent = ScreenGui
                Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Frame.Position = UDim2.new(0.36, 0, 0.20, 0)
                Frame.Size = UDim2.new(0, 330, 0, 150)

                TextLabel.Parent = Frame
                TextLabel.BackgroundColor3 = Color3.fromRGB(255, 232, 117)
                TextLabel.Size = UDim2.new(0, 330, 0, 90)
                TextLabel.Font = Enum.Font.AmaticSC
                TextLabel.Text = "Are you sure you want to reset script?"
                TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
                TextLabel.TextScaled = true
                TextLabel.TextSize = 35.000
                TextLabel.TextWrapped = true

                TextButton.Parent = Frame
                TextButton.BackgroundColor3 = Color3.fromRGB(0, 230, 30)
                TextButton.Position = UDim2.new(0, 0, 0.600000006, 0)
                TextButton.Size = UDim2.new(0, 165, 0, 60)
                TextButton.Font = Enum.Font.AmaticSC
                TextButton.Text = "Yes"
                TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                TextButton.TextScaled = true
                TextButton.TextSize = 40.000
                TextButton.TextWrapped = true

                function ResetYES()
                    for i,v in pairs(player.PlayerGui:GetChildren()) do
                        if v.Name == "ScreenGui" then
                            v:Destroy()
                        end
                    end
                    wait()
                    RANKUPP = false
                    autoofarm = false
                    aunin = false
                    auchi = false
                    autai = false 
                    auhp = false
                    wait()
                    library:Destroy()
                    game:GetService("StarterGui"):SetCore("SendNotification",{     Title = "Script Reset",     Text = "Do not reset autofarm script more than once!",Duration = 15, })
                    local player = game.Players.LocalPlayer
                    player.Character:BreakJoints()
                    wait(11)
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Shindo/ShindoMain"))()
                end
                TextButton.MouseButton1Click:connect(ResetYES)

                TextButton2.Parent = Frame
                TextButton2.BackgroundColor3 = Color3.fromRGB(260, 30, 30)
                TextButton2.Position = UDim2.new(0.5, 0, 0.600000006, 0)
                TextButton2.Size = UDim2.new(0, 165, 0, 60)
                TextButton2.Font = Enum.Font.AmaticSC
                TextButton2.Text = "no"
                TextButton2.TextColor3 = Color3.fromRGB(0, 0, 0)
                TextButton2.TextSize = 40.000
                TextButton2.TextWrapped = true

                function ResetNO()
                    for i,v in pairs(player.PlayerGui:GetChildren()) do
                        if v.Name == "ScreenGui" then
                            v:Destroy()
                        end
                    end
                end
                TextButton2.MouseButton1Click:connect(ResetNO)
            end)

            window3:Label("Made by NoobieDoge", Color3.fromRGB(127, 143, 166))

