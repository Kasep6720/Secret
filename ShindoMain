	BgColor = Color3.fromRGB(117, 50, 168);
}) 

local autofarm
b:Toggle("AutoFarm",function(bool)
	autofarm = bool
end)
local RANKUPP
b:Toggle("AutoRank",function(bool)
	RANKUPP = bool
end)
local KILL
b:Toggle("Boss Kill Aura",function(bool)
	KILL = bool
end)
