		end)
	end

	spawn(function()
		while wait() do
			if LairFarm then
				if Jotaro then
					while Jotaro and LairFarm and task.wait() do
						JotaroLair()
					end
				end
				if Diavolo then
					while Diavolo and LairFarm and task.wait() do
						DiavoloLair()
					end
				end
				if HomelessMan then
					while HomelessMan and LairFarm and task.wait() do
						HomelessManLair()
					end
				end
				if Dio then
					while Dio and LairFarm and task.wait() do
						DioLair()
					end
				end
				if BadGi then
					while BadGi and LairFarm and task.wait() do
						BadGiLair()
					end
				end
			end
		end
	end)

	game:service'Players'.LocalPlayer.Idled:connect(function()
		if LairFarm and (BadGi or Dio or HomelessMan or Diavolo or Jotaro) then
			player.Character.Humanoid.Health = 0 
		end
	end)
	
	-- seventh page
	local page7 = NoobieDoge:addPage("Boss Farm", 5927656531)
	local section1 = page7:addSection("Boss Farm")
	local section2 = page7:addSection("Boss Farm Settings")
	
	local FarmOHJotaro = false
	section1:addToggle("Farm Heaven Ascended Jotaro",false, function(bool)
		FarmOHJotaro = bool
	end)

	local FarmJohnny = false
	section1:addToggle("Farm Johnny Joestar",false, function(bool)
		FarmJohnny = bool
	end)

	local UndergroundModeB = true
	section2:addToggle("Underground Farming Mode", true, function(bool)
		UndergroundModeB = bool
	end)

	local TPBackB = true
	section2:addToggle("Teleport back after boss killed", true, function(bool)
		TPBackB = bool
	end)
	
	spawn(function()
		while wait() do
			if FarmOHJotaro then
				for i, v in pairs(game:GetService("Workspace").Living:GetChildren()) do
					if v.Name == "Jotaro Over Heaven" and v:FindFirstChild("Humanoid").Health ~= 0 then
						FARMINGJOTAROOH = true
						local URLASTPOS = player.Character.HumanoidRootPart.CFrame
						repeat task.wait()
							pcall(function()
								if player.Character:FindFirstChild("Stand").Head:FindFirstChild("BodyPart") then
									if player.Character:FindFirstChild("Stand").Head.BodyPart.Transparency == 1 then
										local args = {[1] = "InitiateMove", [2] = "SummonStand", [3] = "Start"}
										game:GetService("Players").LocalPlayer.Character.StandEvents:FireServer(unpack(args))
									end
									for i, v1 in pairs(game:GetService("Players").LocalPlayer.PlayerGui.CDgui.fortnite:GetChildren()) do
										if v1:IsA("Frame") and (v1.Textt.Text == "Punch" or v1.Textt.Text == "Normal Punches") then
											--// waiting
										else
											local args = {[1] = "InitiateMove", [2] = "NormalPunch", [3] = "Start"}
											game:GetService("Players").LocalPlayer.Character.StandEvents:FireServer(unpack(args))
										end
									end
								else
									if player.Character:FindFirstChild("Stand").Head.Transparency == 1 then
										player.Character.StandEvents.Summon:FireServer()
									end
									for i, v1 in pairs(game:GetService("Players").LocalPlayer.PlayerGui.CDgui.fortnite:GetChildren()) do
										if v1:IsA("Frame") and (v1.Textt.Text == "Punch" or v1.Textt.Text == "Normal Punches") then
											--// waiting
										else
											game:GetService("Players").LocalPlayer.Character.StandEvents.M1:FireServer()
										end
									end
								end
								if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Stand") then
									game:GetService("Players").LocalPlayer.Character.Stand:WaitForChild("HumanoidRootPart").CFrame = game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
								end
								if UndergroundModeB == true then
									game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -7, 0, 1, 0, 0, 1) -- -593.952698, 66.6276474, -783.119934, -0.707110405, -5.96460623e-08, -0.707103133, -8.71555841e-08, 1, 2.80376944e-09, 0.707103133, 6.36105568e-08, -0.707110405
								else
									game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, 0, 6)
								end
								game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
								if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Aura").Value == false then
									game:GetService("Players").LocalPlayer.Character.StandEvents.Summon:FireServer()
								end
							end)
						until v:FindFirstChild("Humanoid").Health == 0 or FarmOHJotaro == false
						
						if TPBackB == true and FarmOHJotaro then
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = URLASTPOS
						end
						if UndergroundModeB == true and FarmOHJotaro then
							game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 10, 0))
						end
					end
				end
				FARMINGJOTAROOH = false
			end
			if FarmJohnny then
				for i, v in pairs(game:GetService("Workspace").Living:GetChildren()) do
					if v.Name == "JohnnyJoestar" and v:FindFirstChild("Humanoid").Health ~= 0 then
						FARMINGJOHNNY = true
						local URLASTPOS = player.Character.HumanoidRootPart.CFrame
						repeat task.wait()
							pcall(function()
								if player.Character:FindFirstChild("Stand").Head:FindFirstChild("BodyPart") then
									if player.Character:FindFirstChild("Stand").Head.BodyPart.Transparency == 1 then
										local args = {[1] = "InitiateMove", [2] = "SummonStand", [3] = "Start"}
										game:GetService("Players").LocalPlayer.Character.StandEvents:FireServer(unpack(args))
									end
									for i, v1 in pairs(game:GetService("Players").LocalPlayer.PlayerGui.CDgui.fortnite:GetChildren()) do
										if v1:IsA("Frame") and (v1.Textt.Text == "Punch" or v1.Textt.Text == "Normal Punches") then
											--// waiting
										else
											local args = {[1] = "InitiateMove", [2] = "NormalPunch", [3] = "Start"}
											game:GetService("Players").LocalPlayer.Character.StandEvents:FireServer(unpack(args))
										end
									end
								else
									if player.Character:FindFirstChild("Stand").Head.Transparency == 1 then
										player.Character.StandEvents.Summon:FireServer()
									end
									for i, v1 in pairs(game:GetService("Players").LocalPlayer.PlayerGui.CDgui.fortnite:GetChildren()) do
										if v1:IsA("Frame") and (v1.Textt.Text == "Punch" or v1.Textt.Text == "Normal Punches") then
											--// waiting
										else
											game:GetService("Players").LocalPlayer.Character.StandEvents.M1:FireServer()
										end
									end
								end
								if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Stand") then
									game:GetService("Players").LocalPlayer.Character.Stand:WaitForChild("HumanoidRootPart").CFrame = game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
								end
								if UndergroundModeB == true then
									game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -7, 0, 1, 0, 0, 1) -- -593.952698, 66.6276474, -783.119934, -0.707110405, -5.96460623e-08, -0.707103133, -8.71555841e-08, 1, 2.80376944e-09, 0.707103133, 6.36105568e-08, -0.707110405
								else
									game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, 0, 6)
								end
								game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
								if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Aura").Value == false then
									game:GetService("Players").LocalPlayer.Character.StandEvents.Summon:FireServer()
								end
							end)
						until v:FindFirstChild("Humanoid").Health == 0 or FarmJohnny == false
						
						if TPBackB == true and FarmJohnny then
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = URLASTPOS
						end
						if UndergroundModeB == true and FarmJohnny then
							game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 10, 0))
						end
					end
				end
				FARMINGJOHNNY = false
			end
		end
	end)
	
	spawn(function()
		while wait() do
			if FARMINGJOTAROOH or FARMINGJOHNNY then
				FARMINGBOSSES = true
			else
				FARMINGBOSSES = false
			end
		end
	end)

	-- eighth page
	local page8 = NoobieDoge:addPage("Stand Farm", 5927656531)
	local section1 = page8:addSection("Stand Farm")
	local section2 = page8:addSection("Stand Selection")
	local section3 = page8:addSection("Attribute Selection")
	local section4 = page8:addSection("Others")

	local StandFarm = false
	section1:addToggle("Stand Farm",false, function(bool)
		StandFarm = bool
	end)

	local STANDSLIST = {}
	for i,v in pairs(game:GetService("ReplicatedStorage").StandNameConvert:GetChildren()) do
		if v.Name ~= "PackageLink" and v.Name ~= "EDTWOH" and v.Name ~= "SPTWR" and v.Name ~= "SGMIH" and v.Name ~= "HGR" and v.Name ~= "TA2" and v.Name ~= "TA4" and v.Name ~= "TA3" and v.Name ~= "SPOVAOH" and v.Name ~= "StarPlatinumOverHeaven" and v.Name ~= "CDOH" and v.Name ~= "MIH" and v.Name ~= "CMoon" and v.Name ~= "STW" and v.Name ~= "D4CLT" and v.Name ~= "THR" and v.Name ~= "StabPlatinumTheWorld" and v.Name ~= "JSPOH" and v.Name ~= "SCROVA" and v.Name ~= "TWOVAOH" and v.Name ~= "StarPlatinumTheWorld" and v.Name ~= "TWOH" and v.Name ~= "DTWOH" and v.Name ~= "KCR" and v.Name ~= "SCR" and v.Name ~= "GERR" and v.Name ~= "PMR" and v.Name ~= "GER" and v.Name ~= "KQR" then
			table.insert(STANDSLIST, v.Name)
		end
	end
	local ATTRIBUTESLIST = {"None","Cheerful","Strong","Powerful","Manic","Tough","Enrage","Sloppy","Lethargic","Godly","Daemon","Invincible","Tragic","Glass Cannon","Hacker","Legendary"}

	section2:addDropdown("Stand Choice 1",STANDSLIST,function(S1)
		print("Selected: " .. S1)
		notify("Selected: " .. S1)
		SC1 = S1
	end)
