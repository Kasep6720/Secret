	local SFSC = true
	section4:addToggle("Stop When Stand Matches",true, function(bool)
		SFSC = bool
	end)

	local SFAC = true
	section4:addToggle("Stop When Attribute Matches",true, function(bool)
		SFAC = bool
	end)

	local SFSCAC = false
	section4:addToggle("Stop When Both Stand and Attribute Matches",false, function(bool)
		SFSCAC = bool
	end)

	local SFuseSA = true
	section4:addToggle("Use Stand Arrow",true, function(bool)
		SFuseSA = bool
	end)

	local SFuseCA = true
	section4:addToggle("Use Charged Arrow",true, function(bool)
		SFuseCA = bool
	end)

	local SFRoka = false
	section4:addToggle("Auto Buy Rokakaka when run out of Fruit",false, function(bool)
		SFRoka = bool
	end)

	local function FARMSTANDS()
		pcall(function()
			if SFRoka and not player.Backpack:FindFirstChild("Rokakaka") and not player.Character:FindFirstChild("Rokakaka") then
				if player.Data.Level.Value < 15 then
					notify("You cannot use this function yet")
				else
					if player.Data.Level.Value < 40 then
						for i = 1, 5, 1 do
							for i,v in pairs(game:GetService("Workspace").Map.NPCs:GetChildren()) do
								if v:FindFirstChild("Head") and v.Head:FindFirstChild("Main") and v.Head.Main:FindFirstChild("Text") and v.Head.Main.Text.Text == "d0ge_316sBrother [Lvl. 15+]" then
									player.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame
								end
							end
							wait(.25)
							game:GetService("ReplicatedStorage").Events.BuyItem:FireServer("Merchant1", "Option2")
						end
					else
						for i = 1, 5, 1 do
							for i,v in pairs(game:GetService("Workspace").Map.NPCs:GetChildren()) do
								if v:FindFirstChild("Head") and v.Head:FindFirstChild("Main") and v.Head.Main:FindFirstChild("Text") and v.Head.Main.Text.Text == "d0ge_316sBrother [Lvl. 40+]" then
									player.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame
								end
							end
							wait(.25)
							game:GetService("ReplicatedStorage").Events.BuyItem:FireServer("MerchantAU", "Option2")
						end
					end
				end
			end
			if player.Data.Stand.Value ~= "None" then
				if player.Backpack:FindFirstChild("Rokakaka") and not player.Character:FindFirstChild("Rokakaka") then
					player.Character.Humanoid:EquipTool(player.Backpack:FindFirstChild("Rokakaka"))
				end
				game:GetService("ReplicatedStorage").Events.UseItem:FireServer()
				wait(1)
			else
				if SFuseCA and SFuseSA then
					local CASAprobit = math.random(1, 2)
					if CASAprobit == 1 then
						if player.Backpack:FindFirstChild("Stand Arrow") then
							if player.Backpack:FindFirstChild("Stand Arrow") and not player.Character:FindFirstChild("Stand Arrow") then
								player.Character.Humanoid:EquipTool(player.Backpack:FindFirstChild("Stand Arrow"))
							end
							game:GetService("ReplicatedStorage").Events.UseItem:FireServer()
							wait(4.6)
						end
					else
						if player.Backpack:FindFirstChild("Charged Arrow") then
							if player.Backpack:FindFirstChild("Charged Arrow") and not player.Character:FindFirstChild("Charged Arrow") then
								player.Character.Humanoid:EquipTool(player.Backpack:FindFirstChild("Charged Arrow"))
							end
							game:GetService("ReplicatedStorage").Events.UseItem:FireServer()
							wait(4.6)
						end
					end
				elseif SFuseCA then
					if player.Backpack:FindFirstChild("Charged Arrow") then
						if player.Backpack:FindFirstChild("Charged Arrow") and not player.Character:FindFirstChild("Charged Arrow") then
							player.Character.Humanoid:EquipTool(player.Backpack:FindFirstChild("Charged Arrow"))
						end
						game:GetService("ReplicatedStorage").Events.UseItem:FireServer()
						wait(4.6)
					end
				elseif SFuseSA then
					if player.Backpack:FindFirstChild("Stand Arrow") then
						if player.Backpack:FindFirstChild("Stand Arrow") then
							if player.Backpack:FindFirstChild("Stand Arrow") and not player.Character:FindFirstChild("Stand Arrow") then
								player.Character.Humanoid:EquipTool(player.Backpack:FindFirstChild("Stand Arrow"))
							end
							game:GetService("ReplicatedStorage").Events.UseItem:FireServer()
							wait(4.6)
						end
					end
				end
			end
		end)
	end

	spawn(function()
		while wait() do
			if StandFarm then
				if SFSC and SFAC then
					if player.Data.Stand.Value ~= SC1 and player.Data.Stand.Value ~= SC2 and player.Data.Stand.Value ~= SC3 and player.Data.Stand.Value ~= SC4 and player.Data.Stand.Value ~= SC5 and player.Data.Attri.Value ~= AC1 and player.Data.Attri.Value ~= AC2 and player.Data.Attri.Value ~= AC3 and player.Data.Attri.Value ~= AC4 and player.Data.Attri.Value ~= AC5 then
						FARMSTANDS()
					else
						notify("You've got "  .. player.Data.Stand.Value .. " ,Attribute: " .. player.Data.Attri.Value)
					end
				elseif SFSC then
					if player.Data.Stand.Value ~= SC1 and player.Data.Stand.Value ~= SC2 and player.Data.Stand.Value ~= SC3 and player.Data.Stand.Value ~= SC4 and player.Data.Stand.Value ~= SC5 then
						FARMSTANDS()
					else
						notify("You've got "  .. player.Data.Stand.Value)
					end
				elseif SFAC then
					if player.Data.Attri.Value ~= AC1 and player.Data.Attri.Value ~= AC2 and player.Data.Attri.Value ~= AC3 and player.Data.Attri.Value ~= AC4 and player.Data.Attri.Value ~= AC5 then
						FARMSTANDS()
					else
						notify("You've got a "  .. player.Data.Attri.Value .. " Stand")
					end
				elseif SFSCAC then
					if (player.Data.Stand.Value ~= SC1 and player.Data.Stand.Value ~= SC2 and player.Data.Stand.Value ~= SC3 and player.Data.Stand.Value ~= SC4 and player.Data.Stand.Value ~= SC5) or (player.Data.Attri.Value ~= AC1 and player.Data.Attri.Value ~= AC2 and player.Data.Attri.Value ~= AC3 and player.Data.Attri.Value ~= AC4 and player.Data.Attri.Value ~= AC5) then
						FARMSTANDS()
					else
						notify("You've got "  .. player.Data.Stand.Value .. " ,Attribute: " .. player.Data.Attri.Value)
					end
				else
					notify("Please scroll down bruh.")
				end
			end
		end
	end)

	-- ninth page
