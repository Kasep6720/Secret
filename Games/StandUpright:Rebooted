	-- forth page
	local page4 = NoobieDoge:addPage("Drop Items", 5927656531)
	local section1 = page4:addSection("Drop Item")
	local section2 = page4:addSection("Hand Item")
	local section3 = page4:addSection("Auto Drop")

	section1:addButton("Drop Item", function()
		player.PlayerGui.Chat.Frame.ChatBarParentFrame.Frame.BoxFrame.Frame.ChatBar:CaptureFocus()
		player.PlayerGui.Chat.Frame.ChatBarParentFrame.Frame.BoxFrame.Frame.ChatBar.Text = "/dropitem"
		VirtualInputManager:SendKeyEvent(true,"Return",false,uwu)
	end)

	section2:addTextbox("Hand Item to: ","(Player Name)", function(bool)
		PlayerToHandItem = bool
	end)

	section2:addButton("Hand Item", function()
		if PlayerToHandItem == player.Name then
			notify("Bruh.")
		else
			player.PlayerGui.Chat.Frame.ChatBarParentFrame.Frame.BoxFrame.Frame.ChatBar:CaptureFocus()
			player.PlayerGui.Chat.Frame.ChatBarParentFrame.Frame.BoxFrame.Frame.ChatBar.Text = "/handitem "..(PlayerToHandItem)
			VirtualInputManager:SendKeyEvent(true,"Return",false,uwu)
		end
	end)

	local letrash = {}
	for i,v in pairs(player.Backpack:GetChildren()) do
		if v:IsA("Tool") and v:FindFirstChild("Handle") and v.Name then
			table.insert(letrash, v.Name)
		end
	end

	section3:addDropdown("Items that you want to drop",letrash,function(Letrash)
		print("Selected: " .. Letrash)
		notify("Selected: " .. Letrash)
		LeTrash = Letrash
	end)
	LeTrash = "None"

	local AutoDrop = false
	section3:addToggle("Drop selected items",false, function(bool)
		AutoDrop = bool
	end)

	spawn(function()
		while wait() do
			if AutoDrop then
				if LeTrash == "None" then
					notify("Please select an item that you want to drop!")
				else
					if player.Backpack:FindFirstChild(LeTrash) then
						player.Character:FindFirstChildOfClass("Humanoid"):EquipTool(player.Backpack[LeTrash])
						player.PlayerGui.Chat.Frame.ChatBarParentFrame.Frame.BoxFrame.Frame.ChatBar:CaptureFocus()
						player.PlayerGui.Chat.Frame.ChatBarParentFrame.Frame.BoxFrame.Frame.ChatBar.Text = "/dropitem"
						VirtualInputManager:SendKeyEvent(true,"Return",false,uwu)
					else
						notify("There are no more "..LeTrash.."'s in your inventory.")
					end
				end
			end
		end
	end)

