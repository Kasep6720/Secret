							until v:FindFirstChild("Humanoid").Health == 0 or (Farmlvl200 == false and FarmDueLVL == false) or AutoFarm == false
							
							if AutoRespawn == true and (Farmlvl200 or FarmDueLVL) then
								game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 1000, 0))
							end
							if UndergroundMode == true and (Farmlvl200 or FarmDueLVL) then
								game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 10, 0))
							end
						else
							workspace.Map.NPCs.Zeppeli.QuestDone:FireServer()
						end
					end
				end
			end
		end)
	end

	function LVL275Farm()
		pcall(function()
			for i, v in pairs(game:GetService("Workspace").Living:GetChildren()) do
				if v.Name == "Pillerman" and v:FindFirstChild("Humanoid").Health ~= 0 then
					if not player.Data.Quests:FindFirstChild("KillPillermen") then
						workspace.Map.NPCs["Young Joseph"].Done:FireServer()
					else
						if not player.Data.Quests.KillPillermen.Completed.Value == true then
							repeat task.wait()
								pcall(function()
									if player.Character:FindFirstChild("Stand").Head:FindFirstChild("BodyPart") then
										if player.Character:FindFirstChild("Stand").Head.BodyPart.Transparency == 1 then
											local args = {[1] = "InitiateMove", [2] = "SummonStand", [3] = "Start"}
											game:GetService("Players").LocalPlayer.Character.StandEvents:FireServer(unpack(args))
										end
										for i, v1 in pairs(game:GetService("Players").LocalPlayer.PlayerGui.CDgui.fortnite:GetChildren()) do
											if v1:IsA("Frame") and (v1.Textt.Text == "Punch" or v1.Textt.Text == "Normal Punches") then
												--// waiting
											else
												local args = {[1] = "InitiateMove", [2] = "NormalPunch", [3] = "Start"}
												game:GetService("Players").LocalPlayer.Character.StandEvents:FireServer(unpack(args))
											end
										end
									else
										if player.Character:FindFirstChild("Stand").Head.Transparency == 1 then
											player.Character.StandEvents.Summon:FireServer()
										end
										for i, v1 in pairs(game:GetService("Players").LocalPlayer.PlayerGui.CDgui.fortnite:GetChildren()) do
											if v1:IsA("Frame") and (v1.Textt.Text == "Punch" or v1.Textt.Text == "Normal Punches") then
												--// waiting
											else
												game:GetService("Players").LocalPlayer.Character.StandEvents.M1:FireServer()
											end
										end
									end
									if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Stand") then
										game:GetService("Players").LocalPlayer.Character.Stand:WaitForChild("HumanoidRootPart").CFrame = game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
									end
									if UndergroundMode == true then
										game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -7, 0, 1, 0, 0, 1) -- -593.952698, 66.6276474, -783.119934, -0.707110405, -5.96460623e-08, -0.707103133, -8.71555841e-08, 1, 2.80376944e-09, 0.707103133, 6.36105568e-08, -0.707110405
									else
										game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, 0, 6)
									end
									game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
									if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Aura").Value == false then
										game:GetService("Players").LocalPlayer.Character.StandEvents.Summon:FireServer()
									end
								end)
							until v:FindFirstChild("Humanoid").Health == 0 or (Farmlvl275 == false and FarmDueLVL == false) or AutoFarm == false
							
							if AutoRespawn == true and (Farmlvl275 or FarmDueLVL) then
								game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 1000, 0))
							end
							if UndergroundMode == true and (Farmlvl275 or FarmDueLVL) then
								game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 10, 0))
							end
						else
							workspace.Map.NPCs["Young Joseph"].QuestDone:FireServer()
						end
					end
				end
			end
		end)
	end

	spawn(function()
		while wait() do
			if AutoFarm then
				if Farmlvl1 or (player.Data.Level.Value < 10 and FarmDueLVL) then
					while (Farmlvl1 or (player.Data.Level.Value < 10 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL1Farm()
					end
				elseif Farmlvl10 or (player.Data.Level.Value < 20 and FarmDueLVL) then
					while (Farmlvl10 or (player.Data.Level.Value < 20 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL10Farm()
					end
				elseif Farmlvl20 or (player.Data.Level.Value < 30 and FarmDueLVL) then
					while (Farmlvl20 or (player.Data.Level.Value < 30 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL20Farm()
					end
				elseif Farmlvl30 or (player.Data.Level.Value < 40 and FarmDueLVL) then
					while (Farmlvl30 or (player.Data.Level.Value < 40 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL30Farm()
					end
				elseif Farmlvl40 or (player.Data.Level.Value < 50 and FarmDueLVL) then
					while (Farmlvl40 or (player.Data.Level.Value < 50 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL40Farm()
					end
				elseif Farmlvl50 or (player.Data.Level.Value < 75 and FarmDueLVL) then
					while (Farmlvl50 or (player.Data.Level.Value < 75 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL50Farm()
					end
				elseif Farmlvl75 or (player.Data.Level.Value < 100 and FarmDueLVL) then
					while (Farmlvl75 or (player.Data.Level.Value < 100 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL75Farm()
					end
				elseif Farmlvl100 or (player.Data.Level.Value < 125 and FarmDueLVL) then
					while (Farmlvl100 or (player.Data.Level.Value < 125 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL100Farm()
					end
				elseif Farmlvl125 or (player.Data.Level.Value < 150 and FarmDueLVL) then
					while (Farmlvl125 or (player.Data.Level.Value < 150 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL125Farm()
					end
				elseif Farmlvl150 or (player.Data.Level.Value < 175 and FarmDueLVL) then
					while (Farmlvl150 or (player.Data.Level.Value < 175 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL150Farm()
					end
				elseif Farmlvl175 or (player.Data.Level.Value < 200 and FarmDueLVL) then
					while (Farmlvl175 or (player.Data.Level.Value < 200 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL175Farm()
					end
				elseif Farmlvl200 or (player.Data.Level.Value < 275 and FarmDueLVL) then
					while (Farmlvl200 or (player.Data.Level.Value < 275 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL200Farm()
					end
				elseif Farmlvl275 or (player.Data.Level.Value < 9999 and FarmDueLVL) then
					while (Farmlvl275 or (player.Data.Level.Value < 9999 and FarmDueLVL)) and AutoFarm and task.wait() do
						LVL275Farm()
					end
				end
			end
		end
	end)

	-- sixth page
	local page6 = NoobieDoge:addPage("Lair Farm", 5927656531)
	local section1 = page6:addSection("Lair Farm")
	local section2 = page6:addSection("Lair Selection")

	local LairFarm = false
	section1:addToggle("Lair Farm",false, function(bool)
		LairFarm = bool
	end)

	local BadGi = false
	section2:addToggle("Bad Gi Leader [Lvl. 15+]", false, function(bool)
		BadGi = bool
	end)

	local Dio = false
	section2:addToggle("Dio [Lvl. 40+]", false, function(bool)
		Dio = bool
	end)

	local HomelessMan = false
	section2:addToggle("Homeless Lord [Lvl. 80+]", false, function(bool)
		HomelessMan = bool
	end)

	local Diavolo = false
	section2:addToggle("Diavolo [Lvl. 100+]", false, function(bool)
		Diavolo = bool
	end)

	local Jotaro = false
	section2:addToggle("Jotaro [Lvl. 200+]", false, function(bool)
		Jotaro = bool
	end)

	local FARMINGBOSSES

	function BadGiLair()
		pcall(function()
			for i, v in pairs(game:GetService("Workspace").Map.LairSpawns:GetChildren()) do
				if not v:FindFirstChild("Lvl 15 Lair") then
					pcall(function()
						for i, v1 in pairs(game:GetService("Workspace").Map.NPCs:GetChildren()) do
							if LairFarm and BadGi and v1:FindFirstChild("Head") and v1.Head:FindFirstChild("Main") and v1.Head.Main:FindFirstChild("Text") and v1.Head.Main.Text.Text == "i_stabman [Lvl. 15+]" then
								v1.Done:FireServer()
							end
						end
					end)
					wait(.08)
				else
					if v:FindFirstChild("Lvl 15 Lair") then
						for i, fakeboss in pairs(game:GetService("Workspace").Living:GetChildren()) do
							if fakeboss.Name == "Boss" and fakeboss.Head.Display.Frame.t.Text ~= "Bad Gi Boss" then
								fakeboss:Destroy()
							end
						end
						wait()
						local Boss = game:GetService("Workspace").Living:WaitForChild("Boss")
						pcall(function()
							repeat task.wait()
								if not FARMINGBOSSES and v:FindFirstChild("Lvl 15 Lair") then
									pcall(function()
										if player.Character:FindFirstChild("Stand").Head:FindFirstChild("BodyPart") then
											if player.Character:FindFirstChild("Stand").Head.BodyPart.Transparency == 1 then
												local args = {[1] = "InitiateMove", [2] = "SummonStand", [3] = "Start"}
												game:GetService("Players").LocalPlayer.Character.StandEvents:FireServer(unpack(args))
											end
											player.Character.HumanoidRootPart.CFrame = Boss:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -7, 3, 1, 0, 0, 1)
											local args = {[1] = "InitiateMove", [2] = "NormalPunch", [3] = "Start"}
											game:GetService("Players").LocalPlayer.Character.StandEvents:FireServer(unpack(args))
										else
											if player.Character:FindFirstChild("Stand").Head.Transparency == 1 then
												player.Character.StandEvents.Summon:FireServer()
											end
											player.Character.HumanoidRootPart.CFrame = Boss:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -7, 3, 1, 0, 0, 1)
											player.Character.StandEvents.M1:FireServer()
										end
										player.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
										player.Character.Stand.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
									end)
								end
							until Boss.Humanoid.Health == 0 or BadGi == false or player.Character:FindFirstChild("Humanoid").Health == 0 or not v:FindFirstChild("Lvl 15 Lair") or FARMINGBOSSES
							wait()
							--player.CharacterAdded:Wait()
							repeat task.wait() until player.Character:FindFirstChild("HumanoidRootPart")
						end)
					end
				end
			end
		end)
	end

	function DioLair()
		pcall(function()
			for i, v in pairs(game:GetService("Workspace").Map.LairSpawns:GetChildren()) do
				if not v:FindFirstChild("DIODUNGEON") then
					pcall(function()
						for i, v1 in pairs(game:GetService("Workspace").Map.NPCs:GetChildren()) do
							if LairFarm and Dio and v1:FindFirstChild("Head") and v1.Head:FindFirstChild("Main") and v1.Head.Main:FindFirstChild("Text") and v1.Head.Main.Text.Text == "i_stabman [Lvl. 40+]" then
								v1.Done:FireServer()
							end
						end
					end)
					wait(.08)
				else
					if v:FindFirstChild("DIODUNGEON") then
						for i, fakeboss in pairs(game:GetService("Workspace").Living:GetChildren()) do
							if fakeboss.Name == "Boss" and fakeboss.Head.Display.Frame.t.Text ~= "Dio [Dungeon]" then
								fakeboss:Destroy()
							end
						end
						wait()
						local Boss = game:GetService("Workspace").Living:WaitForChild("Boss")
						pcall(function()
							repeat task.wait()
								if not FARMINGBOSSES and v:FindFirstChild("DIODUNGEON") then
									pcall(function()
										if player.Character:FindFirstChild("Stand").Head:FindFirstChild("BodyPart") then
											if player.Character:FindFirstChild("Stand").Head.BodyPart.Transparency == 1 then
												local args = {[1] = "InitiateMove", [2] = "SummonStand", [3] = "Start"}
												game:GetService("Players").LocalPlayer.Character.StandEvents:FireServer(unpack(args))
											end
											player.Character.HumanoidRootPart.CFrame = Boss:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -7, 3, 1, 0, 0, 1)
											local args = {[1] = "InitiateMove", [2] = "NormalPunch", [3] = "Start"}
											game:GetService("Players").LocalPlayer.Character.StandEvents:FireServer(unpack(args))
										else
											if player.Character:FindFirstChild("Stand").Head.Transparency == 1 then
												player.Character.StandEvents.Summon:FireServer()
											end
											player.Character.HumanoidRootPart.CFrame = Boss:FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, -7, 3, 1, 0, 0, 1)
											player.Character.StandEvents.M1:FireServer()
										end
										player.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
										player.Character.Stand.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
									end)
								end
							until Boss.Humanoid.Health == 0 or Dio == false or player.Character:FindFirstChild("Humanoid").Health == 0 or not v:FindFirstChild("DIODUNGEON") or FARMINGBOSSES
							--wait()
							--player.CharacterAdded:Wait()
							repeat task.wait() until player.Character:FindFirstChild("HumanoidRootPart")
						end)
					end
				end
			end
		end)
	end

