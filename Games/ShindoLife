		spawn(function()
			while wait() do
				if EnabledAutoMentor then
					if mission.Visible == false then
						if game.PlaceId == 5451398863 then
							wait(.5)
							player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Narumaki"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
							local args = {[1] = game:GetService("Players").LocalPlayer}
							workspace.sensei.missions:FindFirstChild("Narumaki").missiongiver.CLIENTTALK:FireServer(unpack(args))
							wait(1)
						elseif game.PlaceId == 6901575446 then
							wait(.5)
							player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Jiso Seishin"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(5,0,-1.7)
							local args = {[1] = game:GetService("Players").LocalPlayer}
							workspace.sensei.missions:FindFirstChild("Jiso Seishin").missiongiver.CLIENTTALK:FireServer(unpack(args))
							wait(1)
						elseif game.PlaceId == 5451401540 then
							wait(.5)
							player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Shindai Akuma"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
							local args = {[1] = game:GetService("Players").LocalPlayer}
							workspace.sensei.missions:FindFirstChild("Shindai Akuma").missiongiver.CLIENTTALK:FireServer(unpack(args))
							wait(1)
						elseif game.PlaceId == 5431071837 then
							wait(.5)
							player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Ryuji Kenichi"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,4,3)
							local args = {[1] = game:GetService("Players").LocalPlayer}
							workspace.sensei.missions:FindFirstChild("Ryuji Kenichi").missiongiver.CLIENTTALK:FireServer(unpack(args))
							wait(1)
						elseif game.PlaceId == 4601350656 then
							for v = 1,8 do
								wait()
								game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, game:GetService("Workspace")["Ken Mentor"].HumanoidRootPart.Position)
							end
							game.Workspace.HumanoidRootPart.CFrame=game:GetService("Workspace")["Ken Mentor"].HumanoidRootPart.CFrame+Vector3.new(-6,4,0)
						elseif game.PlaceId == 8184506020 then
							punches = false
							tree = false
							shoot = false
							block = false
							dfarm = true
						else
							notify("You cannot do mentor missions in this map!")
							punches = false
							tree = false
							shoot = false
							block = false
							dfarm = false
						end
					else
						if mission.title.Text == "[Objective: Push-Up Training]" then
							punches = true
							tree = false
							shoot = false
							block = false
							dfarm = false
						elseif mission.title.Text == "[Objective: Tree-Jump Training]" then
							punches = false
							tree = true
							shoot = false
							block = false
							dfarm = false
						elseif mission.title.Text == "[Objective: Accuracy Training]" then
							punches = false
							tree = false
							shoot = true
							block = false
							dfarm = false
						elseif mission.title.Text == "[Objective: Deflecting Training]" then
							punches = false
							tree = false
							shoot = false
							block = true
							dfarm = false
						elseif game.PlaceId == 8184506020 then
							punches = false
							tree = false
							shoot = false
							block = false
							dfarm = true
						end
					end
				end
			end	
		end)
	end

	-- fifth page
	local page5 = NoobieDoge:addPage("Daily Missions", 5012544693)
	local section1 = page5:addSection("Claim Missions")
	local section2 = page5:addSection("Auto Daily Missions")
	local section3 = page5:addSection("Teleports")

	section1:addButton("Claim daily missions rewards",function()
