				workspace.sensei.missions.Narumaki.missiongiver.CLIENTTALK:FireServer(unpack(args))
			else
				notify("Narumaki Mentor is not in this map!")
			end
		end)
		section1:addButton("Tp to Jiso Mentor",function() 
			if game.PlaceId == 6901575446 then
