	local function QUEST()
		for i,v in pairs(workspace.missiongivers:GetChildren()) do
			if v.Name == "" and v:FindFirstChild("Head") then
				local TALK = v:FindFirstChild("Talk")
				if (v:FindFirstChild("Head") and v.Head:FindFirstChild("givemission") and v.Head.givemission.Enabled and v.Head.givemission:FindFirstChild("color").Image == "http://www.roblox.com/asset/?id=5459241648") or v.Talk.typ.Value == "defeat" then
					if v:WaitForChild("Talk"):WaitForChild("mobname") then
						local getmission = v:FindFirstChild("HumanoidRootPart")
						local clienttalk = v:FindFirstChild("CLIENTTALK")
						repeat
							player.Character.HumanoidRootPart.CFrame=getmission.CFrame+Vector3.new(0,3,0)
							clienttalk:FireServer()
							clienttalk:FireServer("accept")  
							wait()
						until mission.Visible or mission.title.text == "[Objective: Mission]" or not autofarm
					end
				end
			end
		end
	end

	local function BOSSFARM()
		if random_boss then
			if game.PlaceId == 7923764447 then
				for i,v in pairs(workspace.sengokubossfight.missions:GetChildren()) do
					if v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
						player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
						local args = {[1] = game:GetService("Players").LocalPlayer}
						v.missiongiver.CLIENTTALK:FireServer(unpack(args))
						wait()
					end
				end
			end
			for i,v in pairs(workspace.bossdropmission.missions:GetChildren()) do
				if v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
					player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
					local args = {[1] = game:GetService("Players").LocalPlayer}
					v.missiongiver.CLIENTTALK:FireServer(unpack(args))
					wait()
				end
			end
		elseif specific_boss then
			if game.PlaceId == 7923764447 then
				for i,v in pairs(workspace.sengokubossfight.missions:GetChildren()) do
					if v.Name == selected_boss and v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
						player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
						local args = {[1] = game:GetService("Players").LocalPlayer}
						v.missiongiver.CLIENTTALK:FireServer(unpack(args))
						wait()
					end
				end
			end
			for i,v in pairs(workspace.bossdropmission.missions:GetChildren()) do
				if v.Name == selected_boss and v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
					player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
					local args = {[1] = game:GetService("Players").LocalPlayer}
					v.missiongiver.CLIENTTALK:FireServer(unpack(args))
					wait()
				end
			end
		else
			wait(10)
			notify("Bruh.")
		end
	end

	local function BEASTFARM()
		if random_beasts then
			if game.PlaceId == 5451405681 then
				for i,v in pairs(workspace.tentailBOSS.missions:GetChildren()) do
