	for i,v in pairs(game:GetService("Workspace").RAREKGS:GetChildren()) do
		if v.Name ~= "KGTEMPLATE" and v.tim.Value ~= "N/A" and not player.statz.genkailevel:FindFirstChild(v.Name) and game:GetService("ReplicatedStorage").alljutsu[v.Name]:FindFirstChild("KG") then
			table.insert(kgs, v.Name)
		end
