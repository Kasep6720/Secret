	section5:addButton("External Shindo Bloodline Changer",function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/NoobieDoge/DogeHub/Separated/ShindoBloodlineChanger"))()
	end)

	-- fifth page
	local page5 = NoobieDoge:addPage("Mentor Farm", 5012544693)
	local MyRank = player.statz.prestige.rank.Value
	if MyRank == "F" or MyRank == "D" or MyRank == "C" or MyRank == "B" or MyRank == "A" then
		local section1 = page5:addSection("You need to be at least S rank to achieve this function")
	else
		local section1 = page5:addSection("Teleport to mentors")
		local section2 = page5:addSection("Mission Process")

		section1:addButton("Tp to Narumaki Mentor",function()
			if game.PlaceId == 5451398863 then
				player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Narumaki"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
				local args = {[1] = player}
				workspace.sensei.missions.Narumaki.missiongiver.CLIENTTALK:FireServer(unpack(args))
			else
				notify("Narumaki Mentor is not in this map!")
			end
		end)
		section1:addButton("Tp to Jiso Mentor",function()
			if game.PlaceId == 6901575446 then
				local args = {[1] = player}
				workspace.sensei.missions:FindFirstChild("Jiso Seishin").missiongiver.CLIENTTALK:FireServer(unpack(args))
				player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Jiso Seishin"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(5,0,-1.7)
			else
				notify("Jiso Seishin Mentor is not in this map!")
			end
		end)
		section1:addButton("Tp to Shindai Mentor",function()
			if game.PlaceId == 5451401540 then
				local args = {[1] = player}
				workspace.sensei.missions:FindFirstChild("Shindai Akuma").missiongiver.CLIENTTALK:FireServer(unpack(args))
				player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Shindai Akuma"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
			else
				notify("Shindai Akuma Mentor is not in this map!")
			end
		end)
		section1:addButton("Tp to Ryuji Mentor",function()
			if game.PlaceId == 5431071837 then
				local args = {[1] = player}
				workspace.sensei.missions:FindFirstChild("Ryuji Kenichi").missiongiver.CLIENTTALK:FireServer(unpack(args))
				player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Ryuji Kenichi"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,4,3)
			else
				notify("Ryuji Kenichi Mentor is not in this map!")
			end
		end)
		section1:addButton("Tp to Bruce Mentor",function()
			if game.PlaceId == 5431071837 then
				local args = {[1] = player}
				workspace.sensei.missions:FindFirstChild("Bruce Kenichi").missiongiver.CLIENTTALK:FireServer(unpack(args))
				player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Bruce Kenichi"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
			else
				notify("Bruce Kenichi Mentor is not in this map!")
			end
		end)
		section1:addButton("Tp to Kenjutsu Mentor",function()
			if game.PlaceId == 4601350656 then
				for v = 1,8 do
					wait()
					game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, game:GetService("Workspace")["Ken Mentor"].HumanoidRootPart.Position)
				end
				player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace")["Ken Mentor"].HumanoidRootPart.CFrame+Vector3.new(-6,4,0)
			else
				notify("Kenjutsu Mentor is not in this map!")
			end
		end)

		section2:addToggle("Auto Mentor Quests",false, function(bool)
			EnabledAutoMentor = bool
			notify("Auto Mentor Quests: "..tostring(EnabledAutoMentor),true)
		end)

		section2:addToggle("1/5",false, function(bool)
			punches = bool
			notify("1/5: "..tostring(punches),true)
		end)
		game:GetService('RunService').Stepped:connect(function()
			if punches then
				player.Character.combat.update:FireServer("mouse1",true)
			end
		end)

		section2:addToggle("2/5",false, function(bool)
			tree = bool
			notify("2/5: "..tostring(tree),true)
		end)
		game:GetService('RunService').Stepped:connect(function()
			if tree then
				player.Character.combat.update:FireServer("takemovement2")
			end
		end)

		section2:addToggle("3/5",false, function(bool)
			shoot = bool
			notify("3/5: "..tostring(shoot),true)
		end)

		spawn(function()
			while wait(5) do
				if shoot then
					if game.PlaceId == 5451398863 then
						player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Narumaki"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(-22,0,-45)
					elseif game.PlaceId == 6901575446 then
						player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Jiso Seishin"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(50,0,10)
					elseif game.PlaceId == 5451401540 then
						player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Shindai Akuma"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(-100,7,-170)
					elseif game.PlaceId == 5431071837 then
						player.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(19300, 935, -12780))
					end
				end
			end
		end)
		game:GetService('RunService').Stepped:connect(function()
			if shoot then
				repeat wait()
					game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, game:GetService("Workspace").Terrain[player.Name].kunaiaim.Position)
					player.Character.combat.update:FireServer("mouse1",true)
				until mission.Visible == false or not shoot
			end
		end)

		section2:addToggle("4/5",false, function(bool)
			block = bool
			notify("4/5: "..tostring(block),true)
		end)

		spawn(function()
			while wait(5) do
				if block then
					if game.PlaceId == 5451398863 then
						player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Narumaki"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(-22,0,-45)
					elseif game.PlaceId == 6901575446 then
						player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Jiso Seishin"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(50,0,10)
					elseif game.PlaceId == 5451401540 then
						player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Shindai Akuma"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(-100,7,-170)
					elseif game.PlaceId == 5431071837 then
						player.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(19300, 935, -12780))
					end
				end
			end
		end)
		game:GetService('RunService').Stepped:connect(function()
			if block then
				repeat wait()
					game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, game:GetService("Workspace").Terrain[player.Name].kunaiaim.Position)
				until mission.Visible == false or not block
			end
		end)

		section2:addToggle("5/5",false, function(bool)
			keel = bool
			notify("5/5: "..tostring(keel),true)
		end)

		section2:addToggle("Kenjutsu Training",false, function(bool)
			if game.PlaceId ~= 4601350656 then
				keel = bool
				notify("Kenjutsu Training: "..tostring(keel),true)
			else
				local args = {[1] = "trainkenmaster"}
				player.startevent:FireServer(unpack(args))
			end
		end)

		game:GetService('RunService').Stepped:connect(function()
			if keel or DailyBoss then
				NPCKEELAURA()
			end
		end)

		spawn(function()
			while wait() do
				if EnabledAutoMentor then
					if mission.Visible == false then
						if game.PlaceId == 5451398863 then
							wait(.5)
							player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Narumaki"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
							local args = {[1] = player}
							workspace.sensei.missions:FindFirstChild("Narumaki").missiongiver.CLIENTTALK:FireServer(unpack(args))
							wait(1)
						elseif game.PlaceId == 6901575446 then
							wait(.5)
							player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Jiso Seishin"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(5,0,-1.7)
							local args = {[1] = player}
							workspace.sensei.missions:FindFirstChild("Jiso Seishin").missiongiver.CLIENTTALK:FireServer(unpack(args))
							wait(1)
						elseif game.PlaceId == 5451401540 then
							wait(.5)
							player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Shindai Akuma"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
							local args = {[1] = player}
							workspace.sensei.missions:FindFirstChild("Shindai Akuma").missiongiver.CLIENTTALK:FireServer(unpack(args))
							wait(1)
						elseif game.PlaceId == 5431071837 then
							if player.statz.currentsensei.Value == "Ryuji Kenichi" then
								wait(.5)
								player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Ryuji Kenichi"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,4,3)
								local args = {[1] = player}
								workspace.sensei.missions:FindFirstChild("Ryuji Kenichi").missiongiver.CLIENTTALK:FireServer(unpack(args))
								wait(1)
							elseif player.statz.currentsensei.Value == "Bruce Kenichi" then
								wait(.5)
								player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Bruce Kenichi"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,4,3)
								local args = {[1] = player}
								workspace.sensei.missions:FindFirstChild("Bruce Kenichi").missiongiver.CLIENTTALK:FireServer(unpack(args))
								wait(1)
							end
						elseif game.PlaceId == 4601350656 then
							for v = 1,8 do
								wait()
								game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, game:GetService("Workspace")["Ken Mentor"].HumanoidRootPart.Position)
							end
							game.Workspace.HumanoidRootPart.CFrame=game:GetService("Workspace")["Ken Mentor"].HumanoidRootPart.CFrame+Vector3.new(-6,4,0)
						elseif game.PlaceId == 8184506020 then --Mentor Boss Battle Map
							punches = false
							tree = false
							shoot = false
							block = false
							dfarm = true
						else
							notify("You cannot do mentor missions in this map!")
							punches = false
							tree = false
							shoot = false
							block = false
							dfarm = false
						end
					else
						if mission.title.Text == "[Objective: Push-Up Training]" then
							punches = true
							tree = false
							shoot = false
							block = false
							dfarm = false
						elseif mission.title.Text == "[Objective: Tree-Jump Training]" then
							punches = false
							tree = true
							shoot = false
							block = false
							dfarm = false
						elseif mission.title.Text == "[Objective: Accuracy Training]" then
							punches = false
							tree = false
							shoot = true
							block = false
							dfarm = false
						elseif mission.title.Text == "[Objective: Deflecting Training]" then
							punches = false
							tree = false
							shoot = false
							block = true
							dfarm = false
						elseif game.PlaceId == 8184506020 then
							punches = false
							tree = false
							shoot = false
							block = false
							dfarm = true
						end
					end
				end
			end
		end)
	end

	-- sixth page
	local page6 = NoobieDoge:addPage("Daily Missions", 5012544693)
	local section1 = page6:addSection("Claim Missions")
	local section2 = page6:addSection("Auto Daily Missions")
	local section3 = page6:addSection("Teleports")

	section1:addButton("Claim daily missions rewards",function()
		local args = {[1] = "claim",[2] = player.statz.mission.dailyquest1}
		player.startevent:FireServer(unpack(args))
		local args = {[1] = "claim",[2] = player.statz.mission.dailyquest2}
		player.startevent:FireServer(unpack(args))
		local args = {[1] = "claim",[2] = player.statz.mission.dailyquest3}
		player.startevent:FireServer(unpack(args))
		local args = {[1] = "claim",[2] = player.statz.mission.dailyquest4}
		player.startevent:FireServer(unpack(args))
