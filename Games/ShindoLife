	section1:addDropdown("Boss Missions Location",allbosslist,function(B1)
		notify("Teleported to: " .. B1 .. " Boss Mission Location")
		bossmissionlocation = B1
		wait()
		if game.PlaceId == 7923764447 then
			for i,v in pairs(workspace.sengokubossfight.missions:GetChildren()) do
				if v.Name == bossmissionlocation and v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
					player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
				end
			end
		end
		if game:GetService("Workspace"):FindFirstChild("bossdropmission") then
			for i,v in pairs(workspace.bossdropmission.missions:GetChildren()) do
				if v.Name == bossmissionlocation and v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
					player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
				end
			end
		end
		for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
			if v.Name ~= "sensei" and v:FindFirstChild("missions") and v.Name ~= "bossdropmission" and v.Name ~= "sengokubossfight" then
				for i,v1 in pairs(game:GetService("Workspace")[v.Name].missions:GetChildren()) do
					if v1.Name == bossmissionlocation and v1:FindFirstChild("missiongiver") and v1.missiongiver:FindFirstChild("timer") and v1.missiongiver.timer:FindFirstChild("time") and v1.missiongiver.timer.time:FindFirstChild("time") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
						player.Character.HumanoidRootPart.CFrame=v1.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
					end
				end
			end
		end
	end)

	local maps = {}
	for i,v in pairs(player.PlayerGui.Main.ingame.Menu.ArenaTab.List:GetChildren()) do
		if v:FindFirstChild("place") then
			table.insert(maps, v.Name)
		end
	end
	local mapselect
	section2:addDropdown("Map Selection",maps,function(MAPSed)
		print("Selected: " .. MAPSed)
		notify("Selected: " .. MAPSed)
		mapselect = MAPSed
	end)

	section2:addButton("Teleport",function()
		local args = {[1] = "rpgteleport",[2] = player.PlayerGui.Main.ingame.Menu.ArenaTab.List[mapselect].place.Value}
		player.startevent:FireServer(unpack(args))
	end)

	section2:addTextbox("Private Server Code", "DDrugs", function(PScodevalue, focusLost)
		print("Input", PScodevalue)
		if focusLost then
			notify("Private Server Code: " .. PScodevalue)
			PScodevalue1 = PScodevalue
		end
	end)

	section2:addButton("Private Server Teleport",function()
		local args = {[1] = "teleporttoprivate",[2] = PScodevalue1}
		player.startevent:FireServer(unpack(args))
	end)

	-- last page
	local pagelast = NoobieDoge:addPage("Settings", 5012544693)
	local settings = pagelast:addSection("Settings")
	local colors = pagelast:addSection("Colors")
	local credits = pagelast:addSection("Credits")

	settings:addButton("Upgrade Performance",function()
		UPPERFORMANCE()
		settings:updateButton("Upgrade Performance","Upgraded Performance")
	end)

	settings:addButton("Remove Effects / Particles",function()
		if player.PlayerScripts:FindFirstChild("EFFECTS") then
			player.PlayerScripts.EFFECTS:Destroy()
		end
		settings:updateButton("Remove Effects / Particles","Removed Effects / Particles")
	end)

	settings:addButton("Reset Script",function()
		local player = game.Players.LocalPlayer
		local ScreenGuiK = Instance.new("ScreenGui")
		local Frame = Instance.new("ImageLabel")
		local TextLabelFrame = Instance.new("ImageLabel")
		local TextLabel = Instance.new("TextLabel")
		local TextButton = Instance.new("ImageButton")
		local TextButtonText = Instance.new("TextButton")
		local TextButton_2 = Instance.new("ImageButton")
		local TextButton_2Text = Instance.new("TextButton")

		ScreenGuiK.Parent = player:WaitForChild("PlayerGui")

		Frame.Parent = ScreenGuiK
		Frame.Name = "Main"
		Frame.BackgroundTransparency = 1
		Frame.Position = UDim2.new(0.66, 0, 0.055, 0)
		Frame.Size = UDim2.new(0, 220, 0, 120)
