		player.startevent:FireServer(unpack(args))
		local args = {[1] = "claim",[2] = player.statz.mission.dailyquest14}
		player.startevent:FireServer(unpack(args))
	end)

	section1:addButton("Claim main missions rewards",function()
		local args = {[1] = "claim2",[2] = player.statz.mainmission.arena}
		player.startevent:FireServer(unpack(args))
		local args = {[1] = "claim2",[2] = player.statz.mainmission.arenax}
		player.startevent:FireServer(unpack(args))
		local args = {[1] = "claim2",[2] = player.statz.mainmission.conquest}
