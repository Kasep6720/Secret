	-- last page
	local pagelast = NoobieDoge:addPage("Settings", 5176336983)
	local settings = pagelast:addSection("Settings")
	local colors = pagelast:addSection("Colors")
	local credits = pagelast:addSection("Credits")

	settings:addButton("Save Script Data (unfinished)",function()
		local HTTP = game:GetService("HttpService")
		local temp = { }
		local succ, out = pcall(function()
			temp.Autorejoin = Doge.Autorejoin
			temp.Autosave = Doge.Autosave
			temp.resetspinswhenthisnumberreached = Doge.resetspinswhenthisnumberreached
			temp.savegomyhome = Doge.savegomyhome
			temp.savegomainmenu = Doge.savegomainmenu
			temp.a1 = Doge.a1
			temp.a2 = Doge.a2
			temp.a3 = Doge.a3
			temp.a4 = Doge.a4
			temp.a5 = Doge.a5
			temp.A1 = Doge.A1
			temp.A2 = Doge.A2
			temp.A3 = Doge.A3
			temp.A4 = Doge.A4
			temp.A5 = Doge.A5
			makefolder("DogeHubV2/ShindoLife")
			writefile("DogeHubV2/ShindoLife/MainMenu.txt", HTTP:JSONEncode(temp))
		end)
		if not succ then
			error(out)
		end
	end)

	settings:addButton("Upgrade Performance",function()
		UPPERFORMANCE()
		settings:updateButton("Upgrade Performance","Upgraded Performance")
	end)

	settings:addKeybind("Toggle Keybind", Enum.KeyCode.RightControl, function()
	print("Activated Keybind")
	NoobieDoge:toggle()
	end, function()
	print("Changed Keybind")
	end)

	for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
		colors:addColorPicker(theme, color, function(color3)
			NoobieDoge:setTheme(theme, color3)
		end)
	end

	credits:addButton("Made by NoobieDoge")
	credits:addButton("https://discord.gg/P2CeRjvTDt")
	credits:addButton("Copy Discord Link",function()
		setclipboard("https://discord.gg/P2CeRjvTDt")
		wait()
		notify("Discord link copied to clipboard!")
	end)

	game:GetService('RunService').Stepped:connect(function()
		if datasavedandyoucanredeemcodes == true then
			AutoSpinBL1 = false
			AutoSpinBL2 = false
			AutoSpinBL3 = false
			AutoSpinBL4 = false
			StartSpinElm1 = false
			StartSpinElm2 = false
			StartSpinElm3 = false
			StartSpinElm4 = false
			StartSpinKen = false
			StartSpinKen2 = false
			datasavedandyoucanredeemcodes = false
			player.startevent:FireServer("band", "Eye")
			if savegomyhome then
				notify("You will be teleported to My Home in order to save data")
				wait(1)
				local args = {[1] = "arenateleport",[2] = 5451410109}
				player.startevent:FireServer(unpack(args))
			elseif savegomainmenu then
				notify("You will rejoin in order to save data")
				wait(1)
				game:GetService('TeleportService'):Teleport(game.PlaceId, player)
			end
		else
			player.PlayerGui.Main.Customization.Codes.Text = [[WARNING:
			DO NOT REDEEM CODES BEFORE SAVING DATA]]
		end
	end)

	if game:GetService("Workspace").npc:FindFirstChild("DogeHubNPC") then
		notify("Spam execute won't do anything m8")
	else
		local DogeHubNPC = Instance.new("Part")
		DogeHubNPC.Parent = game:GetService("Workspace").npc
		DogeHubNPC.Name = "DogeHubNPC"
		DogeHubNPC.Anchored = true
		DogeHubNPC.Transparency = 1
		DogeHubNPC.Size = Vector3.new(6, 1, 6)
		--some functions that are important af
		Autorejoin = true
		player.startevent:FireServer("band", "\128")
		player.PlayerGui.Main.RYOshop.ShopItems.HB.int.Text = "[DO NOT PURCHASE ANY ITEMS WHILE USING THE SCRIPT]"
		player.PlayerGui.Main.RYOshop.ShopItems.HB.rellcloak.Text = "[DO NOT PURCHASE ANY ITEMS WHILE USING THE SCRIPT]"
		player.PlayerGui.Main.RYOshop.ShopItems.HB.limited.Text = "[DO NOT PURCHASE ANY ITEMS WHILE USING THE SCRIPT]"
		player.PlayerGui.Main.Customization.CharacterTab.Frame1.hair.color.Visible = false
		player.PlayerGui.Main.Customization.CharacterTab.Frame1.hair.arrowleft.Visible = false
		player.PlayerGui.Main.Customization.CharacterTab.Frame1.hair.arrowright.Visible = false
		player.PlayerGui.Main.Customization.CharacterTab.Frame1.hair.custom.Visible = false
		player.PlayerGui.Main.Customization.CharacterTab.Frame1.band.Visible = false
		player.PlayerGui.Main.Customization.CharacterTab.Frame1.outfit.Visible = false
		player.PlayerGui.Main.Customization.CharacterTab.Frame1.shoe.Visible = false
		player.PlayerGui.Main.Customization.CharacterTab.Frame1.beard.Visible = false
		player.PlayerGui.Main.Customization.CharacterTab.Frame1.face.Visible = false
		player.PlayerGui.Main.Customization.CharacterTab.Frame2.Visible = false
		game:GetService('RunService').Stepped:connect(function()
			for i,v in pairs(player.PlayerGui.Main.Customization.CharacterTab.Frame1:GetChildren()) do
				if v:FindFirstChild("text") then
					v.text.Text = "You cannot customize character while inf spin"
				end
			end
		end)
		repeat wait() until player.PlayerGui.Main.Customization.Genkaibag.Frame2.BossTab
		for i,v in pairs(player.PlayerGui.Main.Customization.Genkaibag.Frame2.BossTab:GetChildren()) do
			if v:FindFirstChild("name") then
				v.name.Text = ("("..v.Name ..")".. " " .. v.name.Text)
			end
		end
		--Random bullshit lol
		for i,v in pairs(game.Workspace.Camera.startup:GetChildren()) do
			if v.Name == "" and v.Model.hair.BrickColor == BrickColor.new(0,255,0) then
				print("")
			else
				for i,v in pairs(game.Workspace.Camera.startup:GetChildren()) do
					if v.Name == "" then
						v.Name = "NPC"
					end
				end
				game.Workspace.Camera.startup.NPC.Name = "DioSenko"
				game.Workspace.Camera.startup.NPC.Name = ""
				game.Workspace.Camera.startup.NPC.Name = "Narumaki"
				game:GetService("Workspace").Camera.startup.DioSenko.Model.hair.BrickColor = BrickColor.new(0,255,0)
				game:GetService("Workspace").Camera.startup.DioSenko.Model.hair.Material = Enum.Material.Neon
				game:GetService("Workspace").Camera.startup.DioSenko.Head.Decal.Texture = [[rbxassetid://9792123944]]
				game:GetService("Workspace").Camera.startup.DioSenko.Head.Mesh:Destroy()
				game:GetService("Workspace").Camera.startup.DioSenko.Head.Shape = Enum.PartType.Ball
				game:GetService("Workspace").Camera.startup.DioSenko.Head.Size = Vector3.new(2, 1.5, 2)
				if game:GetService("Workspace").Camera.startup.Narumaki.uniform:FindFirstChild("ShirtTemplate") then
					game:GetService("Workspace").Camera.startup.Narumaki.uniform.ShirtTemplate = [[rbxassetid://7384140987]]
				end
				if game:GetService("Workspace").Camera.startup.Narumaki.uniform:FindFirstChild("PantsTemplate") then
					game:GetService("Workspace").Camera.startup.Narumaki.uniform.PantsTemplate = [[rbxassetid://7384140866]]
				end
				local leface = math.random(1, 6)
				if leface == 1 then
					game:GetService("Workspace").Camera.startup.Narumaki.Head.face.Texture = [[rbxassetid://7336910703]]
				elseif leface == 2 then
					game:GetService("Workspace").Camera.startup.Narumaki.Head.face.Texture = [[rbxassetid://7389509483]]
				elseif leface == 3 then
					game:GetService("Workspace").Camera.startup.Narumaki.Head.face.Texture = [[rbxassetid://7331429249]]
				elseif leface == 4 then
					game:GetService("Workspace").Camera.startup.Narumaki.Head.face.Texture = [[rbxassetid://10993872952]]
				elseif leface == 5 then
					game:GetService("Workspace").Camera.startup.Narumaki.Head.face.Texture = [[rbxassetid://9792123944]]

				end
				for i,v in pairs(game:GetService("Workspace").Camera.startup.Narumaki.Customize.Model:GetChildren()) do
					if v.Name ~= "headhair" and v.Name ~= "torsoscroll" then
						v:Destroy()
					end
				end
				game:GetService("Workspace").Camera.startup.Narumaki.Name = ""
				game:GetService("Workspace").Camera.startup.DioSenko.Name = ""
			end
		end
	end

end

--Shindo My Home
local function ShindoMyHome()
	--DogeHub V2 GUI
	local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/pornhub%20page%20format%20gui"))()
	local NoobieDoge = library.new("DogeHub V2", 10567878127)

	-- themes
	local themes = {
	Background = Color3.fromRGB(24, 24, 24),
	Glow = Color3.fromRGB(0, 0, 0),
	Accent = Color3.fromRGB(10, 10, 10),
	LightContrast = Color3.fromRGB(20, 20, 20),
	DarkContrast = Color3.fromRGB(14, 14, 14),
	TextColor = Color3.fromRGB(255, 255, 255)
	}

	-- first page
	local page1 = NoobieDoge:addPage("Universal", 10567878127)
	local section1 = page1:addSection("Walk Speed & Jump Power & Gravity")
	local section2 = page1:addSection("Other Functions")

	section1:addSlider("Walk Speed", originalwalkspeed, 0, 500, function(value)
		sliderspeed = value
	end)
	section1:addSlider("Jump Power", originaljumppower, 0, 500, function(value)
		sliderjump = value
	end)
	section1:addSlider("Gravity", originalgravity, 0, 500, function(value)
		slidergravity = value
	end)
	sliderspeed = 16
	sliderjump = 50
	slidergravity = 196.2

	section1:addToggle("Enabled", nil, function(value)
		sliderpowa = value
	end)
	game:GetService('RunService').Stepped:connect(function()
		if sliderpowa then
			player.Character.Humanoid.WalkSpeed = sliderspeed
			player.Character.Humanoid.JumpPower = sliderjump
			game:GetService("Workspace").Gravity = slidergravity
		end
	end)

	section1:addButton("Reset to Default", function()
		player.Character.Humanoid.WalkSpeed = originalwalkspeed
		player.Character.Humanoid.JumpPower = originaljumppower
		game:GetService("Workspace").Gravity = originalgravity
		section1:updateSlider("Walk Speed", "Walk Speed", originalwalkspeed, 0, 500)
		section1:updateSlider("Jump Power", "Jump Power", originaljumppower, 0, 500)
		section1:updateSlider("Gravity", "Gravity", originalgravity, 0, 500)
	end)

	section2:addButton("Add ESP GUI", function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Project%3A%20Bullshit%3A%20Rebooted"))()
	end)

	section2:addButton("Reset Character",function()
		player.Character:BreakJoints()
	end)

	-- second page
	local page2 = NoobieDoge:addPage("Misc", 10567878127)
	local section1 = page2:addSection("Kenjutsu Spin")

	section1:addDropdown("Custom Display Ranks",{"RELL", "F 0", "D 1", "D 2", "D 3", "C 1", "C 2", "C 3", "B 1", "B 2", "B 3", "A 1", "A 2", "A 3", "S 1", "S 2", "S 3", "V 1", "V 2", "V 3", "X 1", "X 2", "X 3", "Y 1", "Y 2", "Y 3", "Z 1", "Z 2", "Z 3", "MAX-69", "MAX-420", "MAX-666", "MAX-999", "GOD", "Dragon", "Hacker","???"},function(rektrank)
		player.leaderstats.Rank.Value =  (rektrank)
		notify("Rank Displayed: " .. rektrank)
	end)

	local elm = {}
	for i,v in pairs(game:GetService("Workspace").RAREKGS:GetChildren()) do
		if v.Name ~= "KGTEMPLATE" and game:GetService("ReplicatedStorage").alljutsu[v.Name]:FindFirstChild("ELEMENT") then
			table.insert(elm, v.Name)
		end
	end
	for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
		if v:FindFirstChild("ELEMENT") and not v:FindFirstChild("limited") then
			table.insert(elm, v.Name)
		end
	end

	local kgs = {}
	for i,v in pairs(game:GetService("Workspace").RAREKGS:GetChildren()) do
		if v.Name ~= "KGTEMPLATE" and game:GetService("ReplicatedStorage").alljutsu[v.Name]:FindFirstChild("KG") then
			table.insert(kgs, v.Name)
		end
	end
	for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
		if (v:FindFirstChild("KG") and not v:FindFirstChild("cantspin") and not v:FindFirstChild("limited")) then
			table.insert(kgs, v.Name)
		end
	end

	local ken = {}
	for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu.ALLKENSTYLES:GetChildren()) do
		if v:FindFirstChild("BREATHINGSTYLE") and not v:FindFirstChild("limited") then
			table.insert(ken, v.Name)
		end
	end

	section1:addButton("Reset Spins NOW",function()
		player.startevent:FireServer("band", "\128")
		wait()
		game:GetService("TeleportService"):Teleport(game.PlaceId, player)
	end)

	section1:addButton("Save Spins Data",function()
		SAVEDATA()
	end)

	section2:addToggle("Auto Rejoin",true,function(bool)
		Autorejoin = bool
	end)

	section2:addToggle("Auto Save",false,function(bool)
		Autosave = bool
	end)

	if player.statz.unlocked:FindFirstChild("kenjutsuslot2") then
		local kenselect1
			section1:addDropdown("Kenjutsu Choice 1",ken,function(KEN1)
			print("Selected: " .. KEN1)
			notify("Selected: " .. KEN1)
			FrameK.Visible = true
			kenselect1 = KEN1
		end)
		local kenselect2
			section1:addDropdown("Kenjutsu Choice 2",ken,function(KEN2)
			print("Selected: " .. KEN2)
			notify("Selected: " .. KEN2)
			FrameK.Visible = true
			kenselect2 = KEN2
		end)
		local kenselect3
			section1:addDropdown("Kenjutsu Choice 3",ken,function(KEN3)
			print("Selected: " .. KEN3)
			notify("Selected: " .. KEN3)
			FrameK.Visible = true
			kenselect3 = KEN3
		end)
		local kenselect4
			section1:addDropdown("Kenjutsu Choice 4",ken,function(KEN4)
			print("Selected: " .. KEN4)
			notify("Selected: " .. KEN4)
			FrameK.Visible = true
			kenselect4 = KEN4
		end)
		local kenselect5
			section1:addDropdown("Kenjutsu Choice 3",ken,function(KEN5)
			print("Selected: " .. KEN5)
			notify("Selected: " .. KEN5)
			FrameK.Visible = true
			kenselect5 = KEN5
		end)

		section1:addToggle("Spin Slot 1",false,function(bool)
			player.startevent:FireServer("band", "\128")
			StartSpinKen = bool
			if StartSpinKen == true then
				local spins = player.statz.spins.Value
				game:GetService('RunService').Stepped:connect(function()
					if spins > 9 then
						spins = player.statz.spins.Value
						kenvalue = player.statz.breathingstyle.Value
						kenvalue2 = player.statz.breathingstyle2.Value
						if StartSpinKen == true then
							if kenvalue ~= kenselect1 and kenvalue ~= kenselect2 and kenvalue ~= kenselect3 and kenvalue ~= kenselect4 and kenvalue ~= kenselect5 and kenvalue2 ~= kenselect1 and kenvalue2 ~= kenselect2 and kenvalue2 ~= kenselect3 and kenvalue2 ~= kenselect4 and kenvalue2 ~= kenselect5 then
								local args = {[1] = "spinBREATHING"}
								player.startevent:FireServer(unpack(args))
								print("Rolled: " .. kenvalue)
								wait(1)
							elseif kenvalue2 ~= kenselect1 and kenvalue2 ~= kenselect2 and kenvalue2 ~= kenselect3 and kenvalue2 ~= kenselect4 and kenvalue2 ~= kenselect5 then
								if Autosave == true then
									datasavedandyoucanredeemcodes = true
								end
							else
								print("You have got: " .. kenvalue)
								notify("You have got " .. kenvalue .. " in slot 1")
								if Autosave == true then
									datasavedandyoucanredeemcodes = true
								end
							end
						end
					else
						if Autorejoin == true then
							--player.statz.spins:Destroy()
							player.startevent:FireServer("band", "\128")
							wait()
							game:GetService('TeleportService'):Teleport(game.PlaceId, player)
						end
					end
				end)
			else
			end
		end)

		section1:addToggle("Spin Slot 2",false,function(bool)
			player.startevent:FireServer("band", "\128")
			StartSpinKen2 = bool
			if StartSpinKen2 == true then
				local spins = player.statz.spins.Value
				game:GetService('RunService').Stepped:connect(function()
					if spins > 9 then
						spins = player.statz.spins.Value
						kenvalue = player.statz.breathingstyle.Value
						kenvalue2 = player.statz.breathingstyle2.Value
						if StartSpinKen2 == true then
							if kenvalue ~= kenselect1 and kenvalue ~= kenselect2 and kenvalue ~= kenselect3 and kenvalue ~= kenselect4 and kenvalue ~= kenselect5 and kenvalue2 ~= kenselect1 and kenvalue2 ~= kenselect2 and kenvalue2 ~= kenselect3 and kenvalue2 ~= kenselect4 and kenvalue2 ~= kenselect5 then
								local args = {[1] = "spinBREATHING",[2] = "breathingstyle2"}
								player.startevent:FireServer(unpack(args))
								print("Rolled: " .. kenvalue2)
								wait(1)
							elseif kenvalue ~= kenselect1 and kenvalue ~= kenselect2 and kenvalue ~= kenselect3 and kenvalue ~= kenselect4 and kenvalue ~= kenselect5 then
								if Autosave == true then
									datasavedandyoucanredeemcodes = true
								end
							else
								print("You have got: " .. kenvalue2)
								notify("You have got " .. kenvalue2 .. " in slot 2")
								if Autosave == true then
									datasavedandyoucanredeemcodes = true
								end
							end
						end
					else
						if Autorejoin == true then
							--player.statz.spins:Destroy()
							player.startevent:FireServer("band", "\128")
							wait()
							game:GetService('TeleportService'):Teleport(game.PlaceId, player)
						end
					end
				end)
			else
			end
		end)
	else
		local kenselect1
			section1:addDropdown("Kenjutsu Choice 1",ken,function(KEN1)
			print("Selected: " .. KEN1)
			notify("Selected: " .. KEN1)
			kenselect1 = KEN1
		end)
		local kenselect2
			section1:addDropdown("Kenjutsu Choice 2",ken,function(KEN2)
			print("Selected: " .. KEN2)
			notify("Selected: " .. KEN2)
			kenselect2 = KEN2
		end)
		local kenselect3
			section1:addDropdown("Kenjutsu Choice 3",ken,function(KEN3)
			print("Selected: " .. KEN3)
			notify("Selected: " .. KEN3)
			kenselect3 = KEN3
		end)

		section1:addToggle("Spin Kenjutsu",false,function(bool)
			player.startevent:FireServer("band", "\128")
			StartSpinKen = bool
			if StartSpinKen == true then
				local spins = player.statz.spins.Value
				game:GetService('RunService').Stepped:connect(function()
					if spins > 9 then
						spins = player.statz.spins.Value
						kenvalue = player.statz.breathingstyle.Value
						if StartSpinKen == true then
							if kenvalue ~= kenselect1 and kenvalue ~= kenselect2 and kenvalue ~= kenselect3 then
								local args = {[1] = "spinBREATHING"}
								player.startevent:FireServer(unpack(args))
								print("Rolled: " .. kenvalue)
								notify("Rolled: " .. kenvalue)
								wait(1)
							else
								print("You have got: " .. kenvalue)
								notify("You have got: " .. kenvalue)
								if Autosave == true then
									datasavedandyoucanredeemcodes = true
								end
							end
						end
					else
						if Autorejoin == true then
							--player.statz.spins:Destroy()
							player.startevent:FireServer("band", "\128")
							wait()
							game:GetService('TeleportService'):Teleport(game.PlaceId, player)
						end
					end
				end)
			else
			end
		end)
	end

	game:GetService('RunService').Stepped:connect(function()
		if datasavedandyoucanredeemcodes == true then
			StartSpinKen = false
			StartSpinKen2 = false
			datasavedandyoucanredeemcodes = false
			player.startevent:FireServer("band", "Eye")
			notify("You will be teleported to My Home in order to save data")
			wait(1)
			local args = {[1] = "arenateleport",[2] = 5451410109}
			player.startevent:FireServer(unpack(args))
		end
	end)

	-- third page
	local page3 = NoobieDoge:addPage("Customs", 10567878127)
	local section1 = page3:addSection("Display Ranks")
	local section2 = page3:addSection("Element Changer")
	local section3 = page3:addSection("Bloodline Changer")
	local section4 = page3:addSection("Extra")

	section1:addDropdown("Custom Display Ranks",{"RELL", "F 0", "D 1", "D 2", "D 3", "C 1", "C 2", "C 3", "B 1", "B 2", "B 3", "A 1", "A 2", "A 3", "S 1", "S 2", "S 3", "V 1", "V 2", "V 3", "X 1", "X 2", "X 3", "Y 1", "Y 2", "Y 3", "Z 1", "Z 2", "Z 3", "MAX-69", "MAX-420", "MAX-666", "MAX-999", "GOD", "Dragon", "Hacker","???"},function(rektrank)
		player.leaderstats.Rank.Value =  (rektrank)
		notify("Rank Displayed: " .. rektrank)
	end)

	section2:addDropdown("Element Slot 1",elm,function(aELM1)
		print("Element Slot 1: " .. aELM1)
		notify("Element Slot 1: " .. aELM1)
		player.statz.main.element1.Value = aELM1
	end)
	section2:addDropdown("Element Slot 2",elm,function(aELM2)
		print("Element Slot 2: " .. aELM2)
		notify("Element Slot 2: " .. aELM2)
		player.statz.main.element2.Value = aELM2
	end)
	section2:addDropdown("Element Slot 3",elm,function(aELM3)
		print("Element Slot 3: " .. aELM3)
		notify("Element Slot 3: " .. aELM3)
		player.statz.main.element3.Value = aELM3
	end)
	section2:addDropdown("Element Slot 4",elm,function(aELM4)
		print("Element Slot 4: " .. aELM4)
		notify("Element Slot 4: " .. aELM4)
		player.statz.main.element4.Value = aELM4
	end)

	section3:addDropdown("Bloodline Slot 1",kgs,function(aKG1)
		print("Bloodline Slot 1: " .. aKG1)
		notify("Bloodline Slot 1: " .. aKG1)
		player.statz.main.kg1.Value = aKG1
	end)
	section3:addDropdown("Bloodline Slot 2",kgs,function(aKG2)
		print("Bloodline Slot 2: " .. aKG2)
		notify("Bloodline Slot 2: " .. aKG2)
		player.statz.main.kg2.Value = aKG2
	end)
	section3:addDropdown("Bloodline Slot 3",kgs,function(aKG3)
		print("Bloodline Slot 3: " .. aKG3)
		notify("Bloodline Slot 3: " .. aKG3)
		player.statz.main.kg3.Value = aKG3
	end)
	section3:addDropdown("Bloodline Slot 4",kgs,function(aKG4)
		print("Bloodline Slot 4: " .. aKG4)
		notify("Bloodline Slot 4: " .. aKG4)
		player.statz.main.kg4.Value = aKG4
	end)

	section4:addButton("External Shindo Bloodline Changer",function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/NoobieDoge/DogeHub/Separated/ShindoBloodlineChanger"))()
	end)

	-- forth page
	local page4 = NoobieDoge:addPage("AFK", 10567878127)
	local section1 = page4:addSection("AFK Places")
	local section2 = page4:addSection("Auto Rank")
	local section3 = page4:addSection("Auto Rank Settings")

	for i,v in pairs(game:GetService("Workspace").map:GetChildren()) do
		if v.Name == (player.Name .. "apartment") then
			if v:FindFirstChild("Model1") then
			else
				v.Model.Name = "Model1"
				v.Model.Seat.Name = "Seat1"
				v.Model.Seat.Name = "Seat2"
				v.Model.Seat.Name = "Seat3"
				v.Model.Seat.Name = "Seat4"
			end
		end
	end

	section1:addButton("Bed",function()
		for i,v in pairs(game:GetService("Workspace").map:GetChildren()) do
			if v.Name == (player.Name .. "apartment") then
				player.Character.HumanoidRootPart.CFrame=v.afkreward.CFrame
			end
		end
	end)

	section1:addButton("Seat 1 (Left)",function()
		for i,v in pairs(game:GetService("Workspace").map:GetChildren()) do
			if v.Name == (player.Name .. "apartment") then
				player.Character.HumanoidRootPart.CFrame=v.Model.Seat3.CFrame
			end
		end
	end)

	section1:addButton("Seat 1 (Right)",function()
		for i,v in pairs(game:GetService("Workspace").map:GetChildren()) do
			if v.Name == (player.Name .. "apartment") then
				player.Character.HumanoidRootPart.CFrame=v.Model.Seat4.CFrame
			end
		end
	end)

	section1:addButton("Seat 2 (Left)",function()
		for i,v in pairs(game:GetService("Workspace").map:GetChildren()) do
			if v.Name == (player.Name .. "apartment") then
				player.Character.HumanoidRootPart.CFrame=v.Model.Seat1.CFrame
			end
		end
	end)

	section1:addButton("Seat 2 (Right)",function()
		for i,v in pairs(game:GetService("Workspace").map:GetChildren()) do
			if v.Name == (player.Name .. "apartment") then
				player.Character.HumanoidRootPart.CFrame=v.Model.Seat2.CFrame
			end
		end
	end)

	section1:addButton("Ramen",function()
		for i,v in pairs(game:GetService("Workspace").map:GetChildren()) do
			if v.Name == (player.Name .. "apartment") then
				player.Character.HumanoidRootPart.CFrame=v.Ramen.CFrame
			end
		end
	end)

	local RANKUPP
	section2:addToggle("Auto Rank",false, function(bool)
		RANKUPP = bool
		notify("Auto Rank: "..tostring(RANKUPP),true)
	end)

	local MAXRANKUPP
	section2:addToggle("Rank Up after MAX",false, function(bool)
		MAXRANKUPP = bool
		notify("Rank Up after MAX: "..tostring(MAXRANKUPP),true)
	end)

	local RANKUPPBYLVL
	section3:addToggle("Rank Up with Levels",true, function(bool)
		RANKUPPBYLVL = bool
		notify("Rank Up with levels: "..tostring(RANKUPPBYLVL),true)
	end)
	RANKUPPBYLVL = true

	local RANKUPPBYRC
	section3:addToggle("Rank Up with RELL Coins",false, function(bool)
		RANKUPPBYRC = bool
		notify("Rank Up with RELL Coins: "..tostring(RANKUPPBYRC),true)
	end)

	local NUMTHENSTOPRANKUPP
	section3:addToggle("Stop when spins reached specific amount",false, function(bool)
		NUMTHENSTOPRANKUPP = bool
		notify("Stop when spins reached specific amount: "..tostring(NUMTHENSTOPRANKUPP),true)
	end)

	local stopautorankwhenthisnumberreached
	if player.gamepasses:FindFirstChild("spinsbag") then
		section3:addSlider("Stop Auto Rank when spins reached", 1000, 50, 1000, function(value)
			stopautorankwhenthisnumberreached = value
		end)
		stopautorankwhenthisnumberreached = 1000
	else
		section3:addSlider("Stop Auto Rank when spins reached", 500, 50, 500, function(value)
			stopautorankwhenthisnumberreached = value
		end)
		stopautorankwhenthisnumberreached = 500
	end

	local PRES = player.statz.prestige.maxlvlpres
	local RANK = player.statz.prestige.rank
	local RANKNUM = player.statz.prestige.number

	spawn(function()
		while wait() do
			if RANKUPP then
				if NUMTHENSTOPRANKUPP and player.statz.spins.Value > (stopautorankwhenthisnumberreached - 1) then
					RANKUPP = false
					section2:updateToggle("Auto Rank", false)
					notify("Spins amount has currently reached " .. stopautorankwhenthisnumberreached)
				else
					if MAXRANKUPP then
						if RANKUPPBYRC then
							if player.PlayerGui.notifui:FindFirstChild("keymove") and player.PlayerGui.notifui.keymove.Tex.text == "Not Enough RELL Coin." then
								wait()
								notify("Out of RELL Coins")
								wait()
								RANKUPP = false
								section2:updateToggle("Auto Rank", false)
								RANKUPPBYRC = false
								section3:updateToggle("Rank Up with RELL Coins", false)
								--patched --loadstring(game:HttpGet('https://raw.githubusercontent.com/KylnDantas/InfRellCoinsSL/main/file.lua',true))()
							else
								player.startevent:FireServer("maxlvlpres2")
							end
						end
						if RANKUPPBYLVL then
							if player.statz.lvl:FindFirstChild("lvl").Value == 1000 then
								repeat wait()
									player.startevent:FireServer("maxlvlpres")
								until player.statz.lvl:FindFirstChild("lvl").Value == 1 or not RANKUPP or not MAXRANKUPP
							end
						else
							notify("Bruh.")
						end
					else
						if RANKUPPBYRC then
							if player.PlayerGui.notifui:FindFirstChild("keymove") and player.PlayerGui.notifui.keymove.Tex.text == "Not Enough RELL Coin." then
								wait()
								notify("Out of RELL Coins")
								wait()
								RANKUPP = false
								section2:updateToggle("Auto Rank", false)
								RANKUPPBYRC = false
								section3:updateToggle("Rank Up with RELL Coins", false)
								--patched --loadstring(game:HttpGet('https://raw.githubusercontent.com/KylnDantas/InfRellCoinsSL/main/file.lua',true))()
							else
								player.startevent:FireServer("maxlvlpres2")
							end
						end
						if RANKUPPBYLVL then
							if player.statz.lvl:FindFirstChild("lvl").Value == 1000 then
								repeat wait()
									player.startevent:FireServer("rankup")
								until player.statz.lvl:FindFirstChild("lvl").Value == 1 or not RANKUPP
							end
						else
							notify("Bruh.")
						end
					end
				end
			end
		end
	end)

	-- load
	NoobieDoge:SelectPage(NoobieDoge.pages[4], true)

	-- fifth page
	local page5 = NoobieDoge:addPage("Teleports", 10567878127)
	local section1 = page5:addSection("Server Teleports")

	local maps = {}
	for i,v in pairs(player.PlayerGui.Main.ingame.Menu.ArenaTab.List:GetChildren()) do
		if v:FindFirstChild("place") then
			table.insert(maps, v.Name)
		end
	end
	local mapselect
	section1:addDropdown("Map Selection",maps,function(MAPSed)
		print("Selected: " .. MAPSed)
		notify("Selected: " .. MAPSed)
		mapselect = MAPSed
	end)

	section1:addButton("Teleport",function()
		local args = {[1] = "rpgteleport",[2] = player.PlayerGui.Main.ingame.Menu.ArenaTab.List[mapselect].place.Value}
		player.startevent:FireServer(unpack(args))
	end)

	section1:addTextbox("Private Server Code", "DDrugs", function(PScodevalue, focusLost)
		print("Input", PScodevalue)
		if focusLost then
			notify("Private Server Code: " .. PScodevalue)
			PScodevalue1 = PScodevalue
		end
	end)

	section1:addButton("Private Server Teleport",function()
		local args = {[1] = "teleporttoprivate",[2] = PScodevalue1}
		player.startevent:FireServer(unpack(args))
	end)

	-- last page
	local pagelast = NoobieDoge:addPage("Settings", 10567878127)
	local settings = pagelast:addSection("Settings")
	local colors = pagelast:addSection("Colors")
	local credits = pagelast:addSection("Credits")

	settings:addButton("Upgrade Performance",function()
		UPPERFORMANCE()
		settings:updateButton("Upgrade Performance","Upgraded Performance")
	end)

	settings:addButton("Remove Effects / Particles",function()
		if player.PlayerScripts:FindFirstChild("EFFECTS") then
			player.PlayerScripts.EFFECTS:Destroy()
		end
		settings:updateButton("Remove Effects / Particles","Removed Effects / Particles")
	end)

	settings:addButton("Reset Script",function()
		local player = game.Players.LocalPlayer
		local ScreenGuiK = Instance.new("ScreenGui")
		local Frame = Instance.new("ImageLabel")
		local TextLabelFrame = Instance.new("ImageLabel")
		local TextLabel = Instance.new("TextLabel")
		local TextButton = Instance.new("ImageButton")
		local TextButtonText = Instance.new("TextButton")
		local TextButton_2 = Instance.new("ImageButton")
		local TextButton_2Text = Instance.new("TextButton")

		ScreenGuiK.Parent = player:WaitForChild("PlayerGui")

		Frame.Parent = ScreenGuiK
		Frame.Name = "Main"
		Frame.BackgroundTransparency = 1
		Frame.Position = UDim2.new(0.66, 0, 0.055, 0)
		Frame.Size = UDim2.new(0, 220, 0, 120)
		Frame.Image = "rbxassetid://4641149554"
		Frame.ImageColor3 = Color3.fromRGB(24, 24, 24)
		Frame.ScaleType = Enum.ScaleType.Slice
		Frame.SliceCenter = Rect.new(4, 4, 296, 296)
		Frame.Active = true
		Frame.Draggable = true

		TextLabelFrame.Parent = Frame
		TextLabelFrame.Name = "TopBar"
		TextLabelFrame.BackgroundTransparency = 1
		TextLabelFrame.ClipsDescendants = true
		TextLabelFrame.Size = UDim2.new(1, 0, 0, 38)
		TextLabelFrame.ZIndex = 2
		TextLabelFrame.Image = "rbxassetid://4595286933"
		TextLabelFrame.ImageColor3 = Color3.fromRGB(255, 163, 26)
		TextLabelFrame.ScaleType = Enum.ScaleType.Slice
		TextLabelFrame.SliceCenter = Rect.new(4, 4, 296, 296)

		TextLabel.Parent = Frame
		TextLabel.BackgroundColor3 = Color3.fromRGB(255, 163, 26)
		TextLabel.Position = UDim2.new(0, 0, -0.1, 0)
		TextLabel.BackgroundTransparency = 1
		TextLabel.Size = UDim2.new(0, 220, 0, 60)
		TextLabel.ZIndex = 3
		TextLabel.Font = Enum.Font.TitilliumWeb
		TextLabel.Text = "Are you sure that you want to reset script?"
		TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
		TextLabel.TextScaled = false
		TextLabel.TextSize = 21.000
		TextLabel.TextWrapped = true

		TextButton.Parent = Frame
		TextButton.Name = "Button"
		TextButton.BackgroundTransparency = 1
		TextButton.Position = UDim2.new(0.075, 0, 0.500000006, 0)
		TextButton.BorderSizePixel = 0
		TextButton.Size = UDim2.new(0, 82.5, 0, 45)
		TextButton.ZIndex = 2
		TextButton.Image = "rbxassetid://5028857472"
		TextButton.ImageColor3 = Color3.fromRGB(41, 41, 41)
		TextButton.ScaleType = Enum.ScaleType.Slice
		TextButton.SliceCenter = Rect.new(2, 2, 298, 298)

		TextButtonText.Parent = TextButton
		TextButtonText.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
		TextButtonText.BackgroundTransparency = 1
		TextButtonText.ZIndex = 3
		TextButtonText.Size = UDim2.new(0, 82.5, 0, 45)
		TextButtonText.Font = Enum.Font.TitilliumWeb
		TextButtonText.Text = "Yes"
		TextButtonText.TextColor3 = Color3.fromRGB(200, 200, 200)
		TextButtonText.TextScaled = true
		TextButtonText.TextSize = 40.000
		TextButtonText.TextWrapped = true

		function ResetYES()
			for i,v in pairs(player.PlayerGui:GetChildren()) do
				if v.Name == "ScreenGui" then
					v:Destroy()
				end
			end
			wait()
			sliderpowa = false
			player.Character.Humanoid.WalkSpeed = originalwalkspeed
			player.Character.Humanoid.JumpPower = originaljumppower
			game:GetService("Workspace").Gravity = originalgravity
			RANKUPP = false
			MAXRANKUPP = false
			NUMTHENSTOPRANKUPP = false
			RANKUPPBYLVL = true
			RANKUPPBYRC = false
			wait()
			for i,library in pairs(game:GetService("CoreGui"):GetChildren()) do
				if library.Name == "DogeHub V2" then
					library:Destroy()
				end
			end
			notify("Do not reset script too many times!")
			wait(.1)
			ShindoMyHome()
		end
		TextButton.MouseButton1Click:connect(ResetYES)
		TextButtonText.MouseButton1Click:connect(ResetYES)

		TextButton_2.Parent = Frame
		TextButton_2.Name = "Button"
		TextButton_2.BackgroundTransparency = 1
		TextButton_2.Position = UDim2.new(0.55, 0, 0.500000006, 0)
		TextButton_2.BorderSizePixel = 0
		TextButton_2.Size = UDim2.new(0, 82.5, 0, 45)
		TextButton_2.ZIndex = 2
		TextButton_2.Image = "rbxassetid://5028857472"
		TextButton_2.ImageColor3 = Color3.fromRGB(41, 41, 41)
		TextButton_2.ScaleType = Enum.ScaleType.Slice
		TextButton_2.SliceCenter = Rect.new(2, 2, 298, 298)

		TextButton_2Text.Parent = TextButton_2
		TextButton_2Text.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
		TextButton_2Text.BackgroundTransparency = 1
		TextButton_2Text.ZIndex = 3
		TextButton_2Text.Size = UDim2.new(0, 82.5, 0, 45)
		TextButton_2Text.Font = Enum.Font.TitilliumWeb
		TextButton_2Text.Text = "No"
		TextButton_2Text.TextColor3 = Color3.fromRGB(200, 200, 200)
		TextButton_2Text.TextScaled = true
		TextButton_2Text.TextSize = 40.000
		TextButton_2Text.TextWrapped = true

		function ResetNO()
			for i,v in pairs(player.PlayerGui:GetChildren()) do
				if v.Name == "ScreenGui" then
					v:Destroy()
				end
			end
		end
		TextButton_2.MouseButton1Click:connect(ResetNO)
		TextButton_2Text.MouseButton1Click:connect(ResetNO)
	end)

	settings:addButton("Rejoin Server",function()
		REJOINSERVER()
	end)

	settings:addKeybind("Toggle Keybind", Enum.KeyCode.RightControl, function()
	print("Activated Keybind")
	NoobieDoge:toggle()
	end, function()
	print("Changed Keybind")
	end)

	for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
		colors:addColorPicker(theme, color, function(color3)
			NoobieDoge:setTheme(theme, color3)
		end)
	end

	credits:addButton("Made by NoobieDoge")
	credits:addButton("https://discord.gg/P2CeRjvTDt")
	credits:addButton("Copy Discord Link",function()
		setclipboard("https://discord.gg/P2CeRjvTDt")
		wait()
		notify("Discord link copied to clipboard!")
	end)

	notify("Turned on anti-AFK")

end

--Shindo Dungeon Selection
local function ShindoDungeonSelection()
	notify("Dungeon will be chosen randomly")
	wait()
	local DungeonsRandom = math.random(1, 12)
	wait(2)
	notify("Dungeon chosen: " .. player.PlayerGui.teleport.serverframe.top.BossTab[DungeonsRandom].icon.lvl.Text)
	wait(1)
	local args = {[1] = "dungeon" .. DungeonsRandom}
	player.pressplay:FireServer(unpack(args))
	wait()
end

--Shindo Dungeons
local function ShindoDungeons()
	--DogeHub V2 GUI
	local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/pornhub%20page%20format%20gui"))()
	local NoobieDoge = library.new("DogeHub V2", 5013109572)

	-- themes
	local themes = {
	Background = Color3.fromRGB(24, 24, 24),
	Glow = Color3.fromRGB(0, 0, 0),
	Accent = Color3.fromRGB(10, 10, 10),
	LightContrast = Color3.fromRGB(20, 20, 20),
	DarkContrast = Color3.fromRGB(14, 14, 14),
	TextColor = Color3.fromRGB(255, 255, 255)
	}

	-- first page
	local page1 = NoobieDoge:addPage("Universal", 5012544693)
	local section1 = page1:addSection("Walk Speed & Jump Power & Gravity")
	local section2 = page1:addSection("Other Functions")

	section1:addSlider("Walk Speed", originalwalkspeed, 0, 500, function(value)
		sliderspeed = value
	end)
	section1:addSlider("Jump Power", originaljumppower, 0, 500, function(value)
		sliderjump = value
	end)
	section1:addSlider("Gravity", originalgravity, 0, 500, function(value)
		slidergravity = value
	end)
	sliderspeed = 16
	sliderjump = 50
	slidergravity = 196.2

	section1:addToggle("Enabled", nil, function(value)
		sliderpowa = value
	end)
	game:GetService('RunService').Stepped:connect(function()
		if sliderpowa then
			player.Character.Humanoid.WalkSpeed = sliderspeed
			player.Character.Humanoid.JumpPower = sliderjump
			game:GetService("Workspace").Gravity = slidergravity
		end
	end)

	section1:addButton("Reset to Default", function()
		player.Character.Humanoid.WalkSpeed = originalwalkspeed
		player.Character.Humanoid.JumpPower = originaljumppower
		game:GetService("Workspace").Gravity = originalgravity
		section1:updateSlider("Walk Speed", "Walk Speed", originalwalkspeed, 0, 500)
		section1:updateSlider("Jump Power", "Jump Power", originaljumppower, 0, 500)
		section1:updateSlider("Gravity", "Gravity", originalgravity, 0, 500)
	end)

	section2:addButton("Add ESP GUI", function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Project%3A%20Bullshit%3A%20Rebooted"))()
	end)

	section2:addButton("Reset Character",function()
		player.Character:BreakJoints()
	end)

	section2:addToggle("Ctrl + Click TP",false,function(bool)
		CtrlClickTeePee = bool
		CTRLCLICKTP()
		if CtrlClickTeePee == true then
			notify("Press Ctrl + Click to Teleport")
		end
	end)

	local infjumptoggle = false
	section2:addToggle("Infinite Jump",false,function()
		infjumptoggle = not infjumptoggle
		notify("Infinite Jump: "..tostring(infjumptoggle),true)
		if infjumptoggle then
			getgenv().dc = false
			local infjump
			infJump = game:GetService("UserInputService").JumpRequest:Connect(function()
				if dc then infjump:Disconnect() return end
				player.Character.Humanoid:ChangeState("Jumping")
			end)
		else
			getgenv().dc = true
		end
	end)

	player.CharacterAdded:Connect(function()
		if infjumptoggle then
			wait(2.5)
			getgenv().dc = false
			local infjump
			infJump = game:GetService("UserInputService").JumpRequest:Connect(function()
				if dc then infjump:Disconnect() return end
				player.Character.Humanoid:ChangeState("Jumping")
			end)
		end
	end)

	section2:addToggle("Noclip", false, function(bool)
		noclips = bool
		game:GetService("RunService").Stepped:connect(function()
			if noclips then
				for i, v in pairs(player.Character:GetChildren()) do
					if v:IsA("BasePart") then
						v.CanCollide = false
					end
				end
			end
		end)
	end)

	section2:addSlider("Fly Speed", 101, 0, 999, function(value)
		sliderfly = value
	end)
	sliderfly = 101
	section2:addToggle("Press P to fly",false,function(bool)
		Pfly = bool
		repeat wait() 
		until player and player.Character:findFirstChild("Head") player.Character:findFirstChild("Humanoid") 
		local mouse = player:GetMouse() 
		repeat wait() until mouse
		local torso = player.Character.Head 
		local flying = false
		local deb = true 
		local ctrl = {f = 0, b = 0, l = 0, r = 0} 
		local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
		local maxspeed = sliderfly 
		local speed = 0 

		function Fly() 
			local bg = Instance.new("BodyGyro", torso) 
			bg.P = 9e4 
			bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
			bg.cframe = torso.CFrame 

			local bv = Instance.new("BodyVelocity", torso) 
			bv.velocity = Vector3.new(0,0.1,0) 
			bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
			repeat wait() 
				player.Character.Humanoid.PlatformStand = true 
				if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then 
					speed = speed+(sliderfly/1.7)+(speed/maxspeed) 
					if speed > maxspeed then 
						speed = maxspeed 
					end 

				elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then 
					speed = speed-(sliderfly/1.7)
					if speed < 0 then 
						speed = 0 
					end 
				end 
				if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then 
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
					lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
				elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then 
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
				else 
					bv.velocity = Vector3.new(0,0.1,0)
				end 
				bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) 
			until not flying or not Pfly
			ctrl = {f = 0, b = 0, l = 0, r = 0} 
			lastctrl = {f = 0, b = 0, l = 0, r = 0} 
			speed = 0 
			bg:Destroy() 
			bv:Destroy() 
			player.Character.Humanoid.PlatformStand = false 
		end 

		mouse.KeyDown:connect(function(key) 
			if key:lower() == "p" then 
				if flying then flying = false 
				else 
					flying = true 
					Fly() 
				end 
			elseif key:lower() == "w" then 
				ctrl.f = 1 
			elseif key:lower() == "s" then 
				ctrl.b = -1 
			elseif key:lower() == "a" then 
				ctrl.l = -1 
			elseif key:lower() == "d" then 
				ctrl.r = 1 
			end 
		end) 

		mouse.KeyUp:connect(function(key) 

			if key:lower() == "w" then 
				ctrl.f = 0 
			elseif key:lower() == "s" then 
				ctrl.b = 0 
			elseif key:lower() == "a" then 
				ctrl.l = 0 
			elseif key:lower() == "d" then 
				ctrl.r = 0 
			end 
		end)
		Fly()
		notify("Press P to fly: "..tostring(Pfly),true)
	end)

	player.CharacterAdded:Connect(function()
		if Pfly then
			repeat wait() 
			until player and player.Character:findFirstChild("Head") player.Character:findFirstChild("Humanoid") 
			local mouse = player:GetMouse() 
			repeat wait() until mouse
			local player = game.Players.LocalPlayer 
			local torso = player.Character.Head 
			local flying = false
			local deb = true 
			local ctrl = {f = 0, b = 0, l = 0, r = 0} 
			local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
			local maxspeed = sliderfly 
			local speed = 0 
			
			function Fly() 
				local bg = Instance.new("BodyGyro", torso) 
				bg.P = 9e4 
				bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
				bg.cframe = torso.CFrame 
	
				local bv = Instance.new("BodyVelocity", torso) 
				bv.velocity = Vector3.new(0,0.1,0) 
				bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
				repeat wait() 
					player.Character.Humanoid.PlatformStand = true 
					if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then 
						speed = speed+(sliderfly/1.7)+(speed/maxspeed) 
						if speed > maxspeed then 
							speed = maxspeed 
						end 
	
					elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then 
						speed = speed-(sliderfly/1.7)
						if speed < 0 then 
							speed = 0 
						end 
					end 
					if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then 
						bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
						lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
					elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then 
						bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
					else 
						bv.velocity = Vector3.new(0,0.1,0)
					end 
					bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) 
				until not flying or not Pfly
				ctrl = {f = 0, b = 0, l = 0, r = 0} 
				lastctrl = {f = 0, b = 0, l = 0, r = 0} 
				speed = 0 
				bg:Destroy() 
				bv:Destroy() 
				player.Character.Humanoid.PlatformStand = false 
			end 
	
			mouse.KeyDown:connect(function(key) 
				if key:lower() == "p" then 
					if flying then flying = false 
					else 
						flying = true 
						Fly() 
					end 
				elseif key:lower() == "w" then 
					ctrl.f = 1 
				elseif key:lower() == "s" then 
					ctrl.b = -1 
				elseif key:lower() == "a" then 
					ctrl.l = -1 
				elseif key:lower() == "d" then 
					ctrl.r = 1 
				end 
			end) 
	
			mouse.KeyUp:connect(function(key) 
	
				if key:lower() == "w" then 
					ctrl.f = 0 
				elseif key:lower() == "s" then 
					ctrl.b = 0 
				elseif key:lower() == "a" then 
					ctrl.l = 0 
				elseif key:lower() == "d" then 
					ctrl.r = 0 
				end 
			end)
			Fly()
		end
	end)

	-- second page
	local page2 = NoobieDoge:addPage("Misc", 5012544693)
	local section1 = page2:addSection("Game Functions")
	local mission = player.PlayerGui:WaitForChild("Main"):WaitForChild("ingame"):WaitForChild("Missionstory")

	section1:addButton("SuperHuman",function()
		section1:updateButton("SuperHuman","SuperHuman (Activated)")
		SUPERHUMAN()
	end)

	section1:addButton("Infinite MD",function()
		INFMDON()
	end)

	section1:addButton("God Mode",function()
		GODMODE()
	end)

	game:GetService('RunService').Stepped:connect(function()
		if not player.Character:FindFirstChild("fakehealth") then
			if game:GetService("CoreGui").DevConsoleMaster.DevConsoleWindow.Visible == true then
				notify("Just ignore the errors lol")
			end
		end
	end)

	local disableincombattoggle = true
	section1:addToggle("Disable In Combat",true,function(bool)
		disableincombattoggle = not disableincombattoggle
		if disableincombattoggle then
			DisabledInCombat = bool
			notify("Disabled In Combat")
		else
			notify("In Combat will be re-activated after a while")
		end
	end)
	DisabledInCombat = true

	game:GetService('RunService').Stepped:connect(function()
		if DisabledInCombat then
			player.incombat.Value = false
		end
	end)

	local unbreakableblockstoggle = false
	section1:addToggle("Unbreakable Blocks",false,function()
		unbreakableblockstoggle = not unbreakableblockstoggle
		notify("Unbreakable Blocks: "..tostring(unbreakableblockstoggle),true)
		if unbreakableblockstoggle then
			UNBREAKABLE_BLOCKS()
		else
			DISABLE_UNBREAKABLE_BLOCKS()
		end
	end)

	player.CharacterAdded:Connect(function()
		if unbreakableblockstoggle then
			wait(2.5)
			UNBREAKABLE_BLOCKS()
		end
	end)

	section1:addToggle("NPC Kill Aura (May cause lag issues)",false,function(bool)
		keel = bool
		notify("NPC Kill Aura: "..tostring(keel),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if keel then
			NPCKEELAURA()
		end
	end)

	local scrollfarm
	section1:addToggle("Scroll Snipe",false, function(bool)
		scrollfarm = bool
	end)

	spawn(function()
		while wait() do
			if scrollfarm then
				repeat wait()
					SCROLLFARM()
					SCROLLFARM1()
					SCROLLFARM2()
					SCROLLFARM3()
				until not scrollfarm
			end
		end
	end)

	section1:addButton("Old No CD Method",function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/HDTerebi/Shindo-Life/main/No%20CD%20Method"))()
		wait()
		notify("Equip a beast mode, mode up, type '!spirit', press the ']' button, done!")
	end)

	-- third page
	local page3 = NoobieDoge:addPage("Spam Keys", 5012544693)
	local section1 = page3:addSection("Configurations")
	local section2 = page3:addSection("Keys")

	section1:addToggle("Enabled",false,function(bool)
		SpamGOBRRRRR = bool
		notify("Spam Keys: "..tostring(SpamGOBRRRRR),true)
	end)

	section2:addToggle("Spam Click",false,function(bool)
		ClickRATATA = bool
		notify("Spam Click: "..tostring(ClickRATATA),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if ClickRATATA and SpamGOBRRRRR then
			SpamClick()
		end
	end)
	section2:addToggle("Spam Q",false,function(bool)
		Q = bool
		notify("Spam Q: "..tostring(Q),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if Q and SpamGOBRRRRR then
			SpamQ()
		end
	end)
	section2:addToggle("Spam E",false, function(bool)
		E = bool
		notify("Spam E: "..tostring(E),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if E and SpamGOBRRRRR then
			SpamE()
		end
	end)
	section2:addToggle("Spam R",false, function(bool)
		R = bool
		notify("Spam R: "..tostring(R),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if R and SpamGOBRRRRR then
			SpamR()
		end
	end)
	section2:addToggle("Spam T",false, function(bool)
		T = bool
		notify("Spam T: "..tostring(T),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if T and SpamGOBRRRRR then
			SpamT()
		end
	end)
	section2:addToggle("Spam Y",false, function(bool)
		Y = bool
		notify("Spam Y: "..tostring(Y),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if Y and SpamGOBRRRRR then
			SpamY()
		end
	end)
	section2:addToggle("Spam F",false, function(bool)
		F = bool
		notify("Spam F: "..tostring(F),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if F and SpamGOBRRRRR then
			SpamF()
		end
	end)
	section2:addToggle("Spam G",false, function(bool)
		G = bool
		notify("Spam G: "..tostring(G),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if G and SpamGOBRRRRR then
			SpamG()
		end
	end)
	section2:addToggle("Spam H",false, function(bool)
		H = bool
		notify("Spam H: "..tostring(H),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if H and SpamGOBRRRRR then
			SpamH()
		end
	end)
	section2:addToggle("Spam V",false, function(bool)
		V = bool
		notify("Spam V: "..tostring(V),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if V and SpamGOBRRRRR then
			SpamV()
		end
	end)
	section2:addToggle("Spam B",false, function(bool)
		B = bool
		notify("Spam B: "..tostring(B),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if B and SpamGOBRRRRR then
			SpamB()
		end
	end)
	section2:addToggle("Spam N",false, function(bool)
		N = bool
		notify("Spam N: "..tostring(N),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if N and SpamGOBRRRRR then
			SpamN()
		end
	end)
	section2:addToggle("Spam Z",false, function(bool)
		Z = bool
		notify("Spam Z: "..tostring(Z),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if Z and SpamGOBRRRRR then
			SpamZ()
		end
	end)
	section2:addToggle("Spam C",false, function(bool)
		C = bool
		notify("Spam C: "..tostring(C),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if C and SpamGOBRRRRR then
			SpamC()
		end
	end)

	-- forth page
	local page4 = NoobieDoge:addPage("Customs", 5012544693)
	local section1 = page4:addSection("Display Ranks")
	local section2 = page4:addSection("Combat Art Animation")
	local section3 = page4:addSection("Element Changer")
	local section4 = page4:addSection("Bloodline Changer")
	local section5 = page4:addSection("Extra")

	section1:addDropdown("Custom Display Ranks",{"RELL", "F 0", "D 1", "D 2", "D 3", "C 1", "C 2", "C 3", "B 1", "B 2", "B 3", "A 1", "A 2", "A 3", "S 1", "S 2", "S 3", "V 1", "V 2", "V 3", "X 1", "X 2", "X 3", "Y 1", "Y 2", "Y 3", "Z 1", "Z 2", "Z 3", "MAX-69", "MAX-420", "MAX-666", "MAX-999", "GOD", "Dragon", "Hacker","???"},function(rektrank)
		player.leaderstats.Rank.Value =  (rektrank)
		notify("Rank Displayed: " .. rektrank)
	end)

	local elm = {}
	for i,v in pairs(game:GetService("Workspace").RAREKGS:GetChildren()) do
		if v.Name ~= "KGTEMPLATE" and game:GetService("ReplicatedStorage").alljutsu[v.Name]:FindFirstChild("ELEMENT") then
			table.insert(elm, v.Name)
		end
	end
	for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
		if v:FindFirstChild("ELEMENT") and not v:FindFirstChild("limited") then
			table.insert(elm, v.Name)
		end
	end

	local kgs = {}
	for i,v in pairs(game:GetService("Workspace").RAREKGS:GetChildren()) do
		if v.Name ~= "KGTEMPLATE" and game:GetService("ReplicatedStorage").alljutsu[v.Name]:FindFirstChild("KG") then
			table.insert(kgs, v.Name)
		end
	end
	for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
		if (v:FindFirstChild("KG") and not v:FindFirstChild("cantspin") and not v:FindFirstChild("limited")) then
			table.insert(kgs, v.Name)
		end
	end

	local ken = {}
	for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu.ALLKENSTYLES:GetChildren()) do
		if v:FindFirstChild("BREATHINGSTYLE") and not v:FindFirstChild("limited") then
			table.insert(ken, v.Name)
		end
	end

	local CAA = {}
	for i,v in pairs(player.PlayerGui.Main.ingame.Menu.CombatTab.Element1:GetChildren()) do
		if v:FindFirstChild("level") then
			table.insert(CAA, v.Name)
		end
	end
	section2:addDropdown("Combat Art",CAA,function(CAA1)
		print("Combat Art: " .. CAA1)
		notify("Combat Art: " .. CAA1)
		player.statz.fightingstyle.Value = CAA1
	end)

	section3:addDropdown("Element Slot 1",elm,function(aELM1)
		print("Element Slot 1: " .. aELM1)
		notify("Element Slot 1: " .. aELM1)
		player.statz.main.element1.Value = aELM1
	end)
	section3:addDropdown("Element Slot 2",elm,function(aELM2)
		print("Element Slot 2: " .. aELM2)
		notify("Element Slot 2: " .. aELM2)
		player.statz.main.element2.Value = aELM2
	end)
	section3:addDropdown("Element Slot 3",elm,function(aELM3)
		print("Element Slot 3: " .. aELM3)
		notify("Element Slot 3: " .. aELM3)
		player.statz.main.element3.Value = aELM3
	end)
	section3:addDropdown("Element Slot 4",elm,function(aELM4)
		print("Element Slot 4: " .. aELM4)
		notify("Element Slot 4: " .. aELM4)
		player.statz.main.element4.Value = aELM4
	end)

	section4:addDropdown("Bloodline Slot 1",kgs,function(aKG1)
		print("Bloodline Slot 1: " .. aKG1)
		notify("Bloodline Slot 1: " .. aKG1)
		player.statz.main.kg1.Value = aKG1
	end)
	section4:addDropdown("Bloodline Slot 2",kgs,function(aKG2)
		print("Bloodline Slot 2: " .. aKG2)
		notify("Bloodline Slot 2: " .. aKG2)
		player.statz.main.kg2.Value = aKG2
	end)
	section4:addDropdown("Bloodline Slot 3",kgs,function(aKG3)
		print("Bloodline Slot 3: " .. aKG3)
		notify("Bloodline Slot 3: " .. aKG3)
		player.statz.main.kg3.Value = aKG3
	end)
	section4:addDropdown("Bloodline Slot 4",kgs,function(aKG4)
		print("Bloodline Slot 4: " .. aKG4)
		notify("Bloodline Slot 4: " .. aKG4)
		player.statz.main.kg4.Value = aKG4
	end)

	section5:addButton("External Shindo Bloodline Changer",function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/NoobieDoge/DogeHub/Separated/ShindoBloodlineChanger"))()
	end)

	-- fifth page
	local page5 = NoobieDoge:addPage("Auto Farm", 5012544693)
	local section1 = page5:addSection("Dungeon Farm")
	local section2 = page5:addSection("Configurations")

	section1:addToggle("Auto Farm",false, function(bool)
		dfarm = bool
		local platform = Instance.new("Part", player.Character)
		platform.Anchored = true
		platform.Transparency = 1
		platform.Size = Vector3.new(6, 2, 6)

		local connection
		connection = game:GetService("RunService").RenderStepped:connect(function()
			if player.Character:FindFirstChild("HumanoidRootPart") then
				platform.Position = player.Character.HumanoidRootPart.CFrame * Vector3.new(0, -2.35, 0)
			else
				connection:Disconnect()
			end
		end)
		game:GetService('RunService').Stepped:connect(function()
			if dfarm ~= true then
				platform:Destroy()
			end
		end)
		if player.PlayerScripts:FindFirstChild("EFFECTS") and (player.statz.sword.throwable.Value == "explosivetag" or player.statz.sword.throwable.Value == "kunaibomb") then
			player.PlayerScripts.EFFECTS:Destroy()
			notify("Automatically removed effects and particles to reduce lag")
		end
	end)

	section1:addToggle("Legacy Farm Mode",false, function(bool)
		Legacy = bool
	end)

	section2:addSlider("Auto Farm Height", 16, -400, 400, function(value)
		AFHeight = value
	end)
	AFHeight = 16
	section2:addSlider("Auto Farm Distance", 10, -400, 400, function(value)
		AFDistance = value
	end)
	AFDistance = 10

	game:GetService('RunService').Stepped:connect(function()
		if dfarm then
			if not Legacy then
				for i,v in pairs(workspace.npc:GetChildren()) do
					if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
						repeat
							game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
							v.Humanoid.Health = 0
							player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, AFHeight, AFDistance)
							player.Character.combat.update:FireServer("key","e")
							player.Character.combat.update:FireServer("key","eend")
							player.Character.combat.update:FireServer("key","e")
							player.Character.combat.update:FireServer("key","eend")
							player.Character.combat.update:FireServer("key","e")
							player.Character.combat.update:FireServer("key","eend")
							player.Character.combat.update:FireServer("key","e")
							player.Character.combat.update:FireServer("key","eend")
							wait(.05000000000000001)
							game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
							v.Humanoid.Health = 0
						until not v:FindFirstChild("Humanoid") or not dfarm or not v
					end
				end
			else
				for i,v in pairs(workspace.npc:GetChildren()) do
					if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
						repeat
							game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
							player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
							player.Character.combat.update:FireServer("key","e")
							player.Character.combat.update:FireServer("key","eend")
							player.Character.combat.update:FireServer("mouse1",true)
							wait(.000000000000001)
							v.Humanoid.Health = 0
							game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
							player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
							player.Character.combat.update:FireServer("key","e")
							player.Character.combat.update:FireServer("key","eend")
							player.Character.combat.update:FireServer("mouse1",true)
							wait(.000000000000001)
							v.Humanoid.Health = 0
						until not v:FindFirstChild("Humanoid") or not dfarm or not v
					end
				end
			end
		end
	end)

	-- sixth page
	local page6 = NoobieDoge:addPage("Teleports", 5012544693)
	local section1 = page6:addSection("Server Teleports")

	local maps = {}
	for i,v in pairs(player.PlayerGui.Main.ingame.Menu.ArenaTab.List:GetChildren()) do
		if v:FindFirstChild("place") then
			table.insert(maps, v.Name)
		end
	end
	local mapselect
	section1:addDropdown("Map Selection",maps,function(MAPSed)
		print("Selected: " .. MAPSed)
		notify("Selected: " .. MAPSed)
		mapselect = MAPSed
	end)

	section1:addButton("Teleport",function()
		local args = {[1] = "rpgteleport",[2] = player.PlayerGui.Main.ingame.Menu.ArenaTab.List[mapselect].place.Value}
		player.startevent:FireServer(unpack(args))
	end)

	section1:addTextbox("Private Server Code", "DDrugs", function(PScodevalue, focusLost)
		print("Input", PScodevalue)
		if focusLost then
			notify("Private Server Code: " .. PScodevalue)
			PScodevalue1 = PScodevalue
		end
	end)

	section1:addButton("Private Server Teleport",function()
		local args = {[1] = "teleporttoprivate",[2] = PScodevalue1}
		player.startevent:FireServer(unpack(args))
	end)

	-- last page
	local pagelast = NoobieDoge:addPage("Settings", 5012544693)
	local settings = pagelast:addSection("Settings")
	local colors = pagelast:addSection("Colors")
	local credits = pagelast:addSection("Credits")

	settings:addButton("Upgrade Performance",function()
		UPPERFORMANCE()
		settings:updateButton("Upgrade Performance","Upgraded Performance")
	end)

	settings:addButton("Remove Effects / Particles",function()
		if player.PlayerScripts:FindFirstChild("EFFECTS") then
			player.PlayerScripts.EFFECTS:Destroy()
		end
		settings:updateButton("Remove Effects / Particles","Removed Effects / Particles")
	end)

	settings:addButton("Reset Script",function()

		local player = game.Players.LocalPlayer
		local ScreenGui = Instance.new("ScreenGui")
		local Frame = Instance.new("Frame")
		local TextLabel = Instance.new("TextLabel")
		local TextButton = Instance.new("TextButton")
		local TextButton2 = Instance.new("TextButton")


		ScreenGui.Parent = player:WaitForChild("PlayerGui")

		Frame.Parent = ScreenGui
		Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Frame.Position = UDim2.new(0.66, 0, 0.20, 0)
		Frame.Size = UDim2.new(0, 220, 0, 120)
		Frame.Active = true
		Frame.Draggable = true

		TextLabel.Parent = Frame
		TextLabel.BackgroundColor3 = Color3.fromRGB(255, 163, 26)
		TextLabel.Size = UDim2.new(0, 220, 0, 90)
		TextLabel.Font = Enum.Font.TitilliumWeb
		TextLabel.Text = "Are you sure that you want to reset script?"
		TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
		TextLabel.TextScaled = true
		TextLabel.TextSize = 35.000
		TextLabel.TextWrapped = true

		TextButton.Parent = Frame
		TextButton.BackgroundColor3 = Color3.fromRGB(41,41,41)
		TextButton.Position = UDim2.new(0, 0, 0.750000006, 0)
		TextButton.Size = UDim2.new(0, 110, 0, 60)
		TextButton.Font = Enum.Font.TitilliumWeb
		TextButton.Text = "Yes"
		TextButton.TextColor3 = Color3.fromRGB(200, 200, 200)
		TextButton.TextScaled = true
		TextButton.TextSize = 40.000
		TextButton.TextWrapped = true

		function ResetYES()
			for i,v in pairs(player.PlayerGui:GetChildren()) do
				if v.Name == "ScreenGui" then
					v:Destroy()
				end
			end
			wait()
			sliderpowa = false
			player.Character.Humanoid.WalkSpeed = originalwalkspeed
			player.Character.Humanoid.JumpPower = originaljumppower
			game:GetService("Workspace").Gravity = originalgravity
			CtrlClickTeePee = false
			noclips = false
			Pfly = false
			getgenv().dc = true
			unbreakableblockstoggle = false
			DisabledInCombat = false
			keel = false
			SpamGOBRRRRR = false
			ClickRATATA = false
			Q = false
			E = false
			R = false
			T = false
			Y = false
			F = false
			G = false
			H = false
			V = false
			B = false
			N = false
			Z = false
			C = false
			dfarm = false
			Legacy = false
			wait()
			for i,library in pairs(game:GetService("CoreGui"):GetChildren()) do
				if library.Name == "DogeHub V2" then
					library:Destroy()
				end
			end
			notify("Do not reset script too many times!")
			wait(.1)
			ShindoDungeons()
		end
		TextButton.MouseButton1Click:connect(ResetYES)

		TextButton2.Parent = Frame
		TextButton2.BackgroundColor3 = Color3.fromRGB(41,41,41)
		TextButton2.Position = UDim2.new(0.5, 0, 0.750000006, 0)
		TextButton2.Size = UDim2.new(0, 110, 0, 60)
		TextButton2.Font = Enum.Font.TitilliumWeb
		TextButton2.Text = "No"
		TextButton2.TextColor3 = Color3.fromRGB(200, 200, 200)
		TextButton2.TextScaled = true
		TextButton2.TextSize = 40.000
		TextButton2.TextWrapped = true

		function ResetNO()
			for i,v in pairs(player.PlayerGui:GetChildren()) do
				if v.Name == "ScreenGui" then
					v:Destroy()
				end
			end
		end
		TextButton2.MouseButton1Click:connect(ResetNO)
	end)

	settings:addButton("Rejoin Server",function()
		REJOINSERVER()
	end)

	settings:addKeybind("Toggle Keybind", Enum.KeyCode.RightControl, function()
	print("Activated Keybind")
	NoobieDoge:toggle()
	end, function()
	print("Changed Keybind")
	end)

	for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
		colors:addColorPicker(theme, color, function(color3)
			NoobieDoge:setTheme(theme, color3)
		end)
	end

	credits:addButton("Made by NoobieDoge")
	credits:addButton("https://discord.gg/P2CeRjvTDt")
	credits:addButton("Copy Discord Link",function()
		setclipboard("https://discord.gg/P2CeRjvTDt")
		wait()
		notify("Discord link copied to clipboard!")
	end)

	-- load
	NoobieDoge:SelectPage(NoobieDoge.pages[1], true)

end

--Shindo Boss Battle Map
local function ShindoBossBattle()
	repeat wait() until game:GetService("Workspace").npc.npc1.Head.mob.fram.name:FindFirstChild("info")
	--DogeHub V2 GUI
	local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/pornhub%20page%20format%20gui"))()
	local NoobieDoge = library.new("DogeHub V2", 5013109572)

	-- themes
	local themes = {
	Background = Color3.fromRGB(24, 24, 24),
	Glow = Color3.fromRGB(0, 0, 0),
	Accent = Color3.fromRGB(10, 10, 10),
	LightContrast = Color3.fromRGB(20, 20, 20),
	DarkContrast = Color3.fromRGB(14, 14, 14),
	TextColor = Color3.fromRGB(255, 255, 255)
	}

	-- first page
	local page1 = NoobieDoge:addPage("Universal", 5012544693)
	local section1 = page1:addSection("Walk Speed & Jump Power & Gravity")
	local section2 = page1:addSection("Other Functions")

	section1:addSlider("Walk Speed", originalwalkspeed, 0, 500, function(value)
		sliderspeed = value
	end)
	section1:addSlider("Jump Power", originaljumppower, 0, 500, function(value)
		sliderjump = value
	end)
	section1:addSlider("Gravity", originalgravity, 0, 500, function(value)
		slidergravity = value
	end)
	sliderspeed = 16
	sliderjump = 50
	slidergravity = 196.2

	section1:addToggle("Enabled", nil, function(value)
		sliderpowa = value
	end)
	game:GetService('RunService').Stepped:connect(function()
		if sliderpowa then
			player.Character.Humanoid.WalkSpeed = sliderspeed
			player.Character.Humanoid.JumpPower = sliderjump
			game:GetService("Workspace").Gravity = slidergravity
		end
	end)

	section1:addButton("Reset to Default", function()
		player.Character.Humanoid.WalkSpeed = originalwalkspeed
		player.Character.Humanoid.JumpPower = originaljumppower
		game:GetService("Workspace").Gravity = originalgravity
		section1:updateSlider("Walk Speed", "Walk Speed", originalwalkspeed, 0, 500)
		section1:updateSlider("Jump Power", "Jump Power", originaljumppower, 0, 500)
		section1:updateSlider("Gravity", "Gravity", originalgravity, 0, 500)
	end)

	section2:addButton("Add ESP GUI", function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Project%3A%20Bullshit%3A%20Rebooted"))()
	end)

	section2:addButton("Reset Character",function()
		player.Character:BreakJoints()
	end)

	section2:addToggle("Ctrl + Click TP",false,function(bool)
		CtrlClickTeePee = bool
		CTRLCLICKTP()
		if CtrlClickTeePee == true then
			notify("Press Ctrl + Click to Teleport")
		end
	end)

	local infjumptoggle = false
	section2:addToggle("Infinite Jump",false,function()
		infjumptoggle = not infjumptoggle
		notify("Infinite Jump: "..tostring(infjumptoggle),true)
		if infjumptoggle then
			getgenv().dc = false
			local infjump
			infJump = game:GetService("UserInputService").JumpRequest:Connect(function()
				if dc then infjump:Disconnect() return end
				player.Character.Humanoid:ChangeState("Jumping")
			end)
		else
			getgenv().dc = true
		end
	end)

	player.CharacterAdded:Connect(function()
		if infjumptoggle then
			wait(2.5)
			getgenv().dc = false
			local infjump
			infJump = game:GetService("UserInputService").JumpRequest:Connect(function()
				if dc then infjump:Disconnect() return end
				player.Character.Humanoid:ChangeState("Jumping")
			end)
		end
	end)

	section2:addToggle("Noclip", false, function(bool)
		noclips = bool
		game:GetService("RunService").Stepped:connect(function()
			if noclips then
				for i, v in pairs(player.Character:GetChildren()) do
					if v:IsA("BasePart") then
						v.CanCollide = false
					end
				end
			end
		end)
	end)

	section2:addSlider("Fly Speed", 101, 0, 999, function(value)
		sliderfly = value
	end)
	sliderfly = 101
	section2:addToggle("Press P to fly",false,function(bool)
		Pfly = bool
		repeat wait() 
		until player and player.Character:findFirstChild("Head") player.Character:findFirstChild("Humanoid") 
		local mouse = player:GetMouse() 
		repeat wait() until mouse
		local torso = player.Character.Head 
		local flying = false
		local deb = true 
		local ctrl = {f = 0, b = 0, l = 0, r = 0} 
		local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
		local maxspeed = sliderfly 
		local speed = 0 

		function Fly() 
			local bg = Instance.new("BodyGyro", torso) 
			bg.P = 9e4 
			bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
			bg.cframe = torso.CFrame 

			local bv = Instance.new("BodyVelocity", torso) 
			bv.velocity = Vector3.new(0,0.1,0) 
			bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
			repeat wait() 
				player.Character.Humanoid.PlatformStand = true 
				if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then 
					speed = speed+(sliderfly/1.7)+(speed/maxspeed) 
					if speed > maxspeed then 
						speed = maxspeed 
					end 

				elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then 
					speed = speed-(sliderfly/1.7)
					if speed < 0 then 
						speed = 0 
					end 
				end 
				if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then 
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
					lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
				elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then 
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
				else 
					bv.velocity = Vector3.new(0,0.1,0)
				end 
				bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) 
			until not flying or not Pfly
			ctrl = {f = 0, b = 0, l = 0, r = 0} 
			lastctrl = {f = 0, b = 0, l = 0, r = 0} 
			speed = 0 
			bg:Destroy() 
			bv:Destroy() 
			player.Character.Humanoid.PlatformStand = false 
		end 

		mouse.KeyDown:connect(function(key) 
			if key:lower() == "p" then 
				if flying then flying = false 
				else 
					flying = true 
					Fly() 
				end 
			elseif key:lower() == "w" then 
				ctrl.f = 1 
			elseif key:lower() == "s" then 
				ctrl.b = -1 
			elseif key:lower() == "a" then 
				ctrl.l = -1 
			elseif key:lower() == "d" then 
				ctrl.r = 1 
			end 
		end) 

		mouse.KeyUp:connect(function(key) 

			if key:lower() == "w" then 
				ctrl.f = 0 
			elseif key:lower() == "s" then 
				ctrl.b = 0 
			elseif key:lower() == "a" then 
				ctrl.l = 0 
			elseif key:lower() == "d" then 
				ctrl.r = 0 
			end 
		end)
		Fly()
		notify("Press P to fly: "..tostring(Pfly),true)
	end)

	player.CharacterAdded:Connect(function()
		if Pfly then
			repeat wait() 
			until player and player.Character:findFirstChild("Head") player.Character:findFirstChild("Humanoid") 
			local mouse = player:GetMouse() 
			repeat wait() until mouse
			local player = game.Players.LocalPlayer 
			local torso = player.Character.Head 
			local flying = false
			local deb = true 
			local ctrl = {f = 0, b = 0, l = 0, r = 0} 
			local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
			local maxspeed = sliderfly 
			local speed = 0 
			
			function Fly() 
				local bg = Instance.new("BodyGyro", torso) 
				bg.P = 9e4 
				bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
				bg.cframe = torso.CFrame 
	
				local bv = Instance.new("BodyVelocity", torso) 
				bv.velocity = Vector3.new(0,0.1,0) 
				bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
				repeat wait() 
					player.Character.Humanoid.PlatformStand = true 
					if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then 
						speed = speed+(sliderfly/1.7)+(speed/maxspeed) 
						if speed > maxspeed then 
							speed = maxspeed 
						end 
	
					elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then 
						speed = speed-(sliderfly/1.7)
						if speed < 0 then 
							speed = 0 
						end 
					end 
					if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then 
						bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
						lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
					elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then 
						bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
					else 
						bv.velocity = Vector3.new(0,0.1,0)
					end 
					bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) 
				until not flying or not Pfly
				ctrl = {f = 0, b = 0, l = 0, r = 0} 
				lastctrl = {f = 0, b = 0, l = 0, r = 0} 
				speed = 0 
				bg:Destroy() 
				bv:Destroy() 
				player.Character.Humanoid.PlatformStand = false 
			end 
	
			mouse.KeyDown:connect(function(key) 
				if key:lower() == "p" then 
					if flying then flying = false 
					else 
						flying = true 
						Fly() 
					end 
				elseif key:lower() == "w" then 
					ctrl.f = 1 
				elseif key:lower() == "s" then 
					ctrl.b = -1 
				elseif key:lower() == "a" then 
					ctrl.l = -1 
				elseif key:lower() == "d" then 
					ctrl.r = 1 
				end 
			end) 
	
			mouse.KeyUp:connect(function(key) 
	
				if key:lower() == "w" then 
					ctrl.f = 0 
				elseif key:lower() == "s" then 
					ctrl.b = 0 
				elseif key:lower() == "a" then 
					ctrl.l = 0 
				elseif key:lower() == "d" then 
					ctrl.r = 0 
				end 
			end)
			Fly()
		end
	end)

	-- second page
	local page2 = NoobieDoge:addPage("Misc", 5012544693)
	local section1 = page2:addSection("Game Functions")
	local mission = player.PlayerGui:WaitForChild("Main"):WaitForChild("ingame"):WaitForChild("Missionstory")

	section1:addButton("SuperHuman",function()
		section1:updateButton("SuperHuman","SuperHuman (Activated)")
		SUPERHUMAN()
	end)

	section1:addButton("Infinite MD",function()
		INFMDON()
	end)

	section1:addButton("God Mode",function()
		GODMODE()
	end)

	game:GetService('RunService').Stepped:connect(function()
		if not player.Character:FindFirstChild("fakehealth") then
			if game:GetService("CoreGui").DevConsoleMaster.DevConsoleWindow.Visible == true then
				notify("Just ignore the errors lol")
			end
		end
	end)

	local disableincombattoggle = true
	section1:addToggle("Disable In Combat",true,function(bool)
		disableincombattoggle = not disableincombattoggle
		if disableincombattoggle then
			DisabledInCombat = bool
			notify("Disabled In Combat")
		else
			notify("In Combat will be re-activated after a while")
		end
	end)
	DisabledInCombat = true

	game:GetService('RunService').Stepped:connect(function()
		if DisabledInCombat then
			player.incombat.Value = false
		end
	end)

	local unbreakableblockstoggle = false
	section1:addToggle("Unbreakable Blocks",false,function()
		unbreakableblockstoggle = not unbreakableblockstoggle
		notify("Unbreakable Blocks: "..tostring(unbreakableblockstoggle),true)
		if unbreakableblockstoggle then
			UNBREAKABLE_BLOCKS()
		else
			DISABLE_UNBREAKABLE_BLOCKS()
		end
	end)

	player.CharacterAdded:Connect(function()
		if unbreakableblockstoggle then
			wait(2.5)
			UNBREAKABLE_BLOCKS()
		end
	end)

	section1:addToggle("NPC Kill Aura",false,function(bool)
		keel = bool
		notify("NPC Kill Aura: "..tostring(keel),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if keel then
			NPCKEELAURA()
		end
	end)

	section1:addButton("Old No CD Method",function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/HDTerebi/Shindo-Life/main/No%20CD%20Method"))()
		wait()
		notify("Equip a beast mode, mode up, type '!spirit', press the ']' button, done!")
	end)

	-- third page
	local page3 = NoobieDoge:addPage("Spam Keys", 5012544693)
	local section1 = page3:addSection("Configurations")
	local section2 = page3:addSection("Keys")

	section1:addToggle("Enabled",false,function(bool)
		SpamGOBRRRRR = bool
		notify("Spam Keys: "..tostring(SpamGOBRRRRR),true)
	end)

	section2:addToggle("Spam Click",false,function(bool)
		ClickRATATA = bool
		notify("Spam Click: "..tostring(ClickRATATA),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if ClickRATATA and SpamGOBRRRRR then
			SpamClick()
		end
	end)
	section2:addToggle("Spam Q",false,function(bool)
		Q = bool
		notify("Spam Q: "..tostring(Q),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if Q and SpamGOBRRRRR then
			SpamQ()
		end
	end)
	section2:addToggle("Spam E",false, function(bool)
		E = bool
		notify("Spam E: "..tostring(E),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if E and SpamGOBRRRRR then
			SpamE()
		end
	end)
	section2:addToggle("Spam R",false, function(bool)
		R = bool
		notify("Spam R: "..tostring(R),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if R and SpamGOBRRRRR then
			SpamR()
		end
	end)
	section2:addToggle("Spam T",false, function(bool)
		T = bool
		notify("Spam T: "..tostring(T),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if T and SpamGOBRRRRR then
			SpamT()
		end
	end)
	section2:addToggle("Spam Y",false, function(bool)
		Y = bool
		notify("Spam Y: "..tostring(Y),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if Y and SpamGOBRRRRR then
			SpamY()
		end
	end)
	section2:addToggle("Spam F",false, function(bool)
		F = bool
		notify("Spam F: "..tostring(F),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if F and SpamGOBRRRRR then
			SpamF()
		end
	end)
	section2:addToggle("Spam G",false, function(bool)
		G = bool
		notify("Spam G: "..tostring(G),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if G and SpamGOBRRRRR then
			SpamG()
		end
	end)
	section2:addToggle("Spam H",false, function(bool)
		H = bool
		notify("Spam H: "..tostring(H),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if H and SpamGOBRRRRR then
			SpamH()
		end
	end)
	section2:addToggle("Spam V",false, function(bool)
		V = bool
		notify("Spam V: "..tostring(V),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if V and SpamGOBRRRRR then
			SpamV()
		end
	end)
	section2:addToggle("Spam B",false, function(bool)
		B = bool
		notify("Spam B: "..tostring(B),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if B and SpamGOBRRRRR then
			SpamB()
		end
	end)
	section2:addToggle("Spam N",false, function(bool)
		N = bool
		notify("Spam N: "..tostring(N),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if N and SpamGOBRRRRR then
			SpamN()
		end
	end)
	section2:addToggle("Spam Z",false, function(bool)
		Z = bool
		notify("Spam Z: "..tostring(Z),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if Z and SpamGOBRRRRR then
			SpamZ()
		end
	end)
	section2:addToggle("Spam C",false, function(bool)
		C = bool
		notify("Spam C: "..tostring(C),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if C and SpamGOBRRRRR then
			SpamC()
		end
	end)

	-- forth page
	local page4 = NoobieDoge:addPage("Customs", 5012544693)
	local section1 = page4:addSection("Display Ranks")
	local section2 = page4:addSection("Combat Art Animation")
	local section3 = page4:addSection("Element Changer")
	local section4 = page4:addSection("Bloodline Changer")
	local section5 = page4:addSection("Extra")

	section1:addDropdown("Custom Display Ranks",{"RELL", "F 0", "D 1", "D 2", "D 3", "C 1", "C 2", "C 3", "B 1", "B 2", "B 3", "A 1", "A 2", "A 3", "S 1", "S 2", "S 3", "V 1", "V 2", "V 3", "X 1", "X 2", "X 3", "Y 1", "Y 2", "Y 3", "Z 1", "Z 2", "Z 3", "MAX-69", "MAX-420", "MAX-666", "MAX-999", "GOD", "Dragon", "Hacker","???"},function(rektrank)
		player.leaderstats.Rank.Value =  (rektrank)
		notify("Rank Displayed: " .. rektrank)
	end)

	local elm = {}
	for i,v in pairs(game:GetService("Workspace").RAREKGS:GetChildren()) do
		if v.Name ~= "KGTEMPLATE" and game:GetService("ReplicatedStorage").alljutsu[v.Name]:FindFirstChild("ELEMENT") then
			table.insert(elm, v.Name)
		end
	end
	for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
		if v:FindFirstChild("ELEMENT") and not v:FindFirstChild("limited") then
			table.insert(elm, v.Name)
		end
	end

	local kgs = {}
	for i,v in pairs(game:GetService("Workspace").RAREKGS:GetChildren()) do
		if v.Name ~= "KGTEMPLATE" and game:GetService("ReplicatedStorage").alljutsu[v.Name]:FindFirstChild("KG") then
			table.insert(kgs, v.Name)
		end
	end
	for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
		if (v:FindFirstChild("KG") and not v:FindFirstChild("cantspin") and not v:FindFirstChild("limited")) then
			table.insert(kgs, v.Name)
		end
	end

	local ken = {}
	for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu.ALLKENSTYLES:GetChildren()) do
		if v:FindFirstChild("BREATHINGSTYLE") and not v:FindFirstChild("limited") then
			table.insert(ken, v.Name)
		end
	end

	local CAA = {}
	for i,v in pairs(player.PlayerGui.Main.ingame.Menu.CombatTab.Element1:GetChildren()) do
		if v:FindFirstChild("level") then
			table.insert(CAA, v.Name)
		end
	end
	section2:addDropdown("Combat Art",CAA,function(CAA1)
		print("Combat Art: " .. CAA1)
		notify("Combat Art: " .. CAA1)
		player.statz.fightingstyle.Value = CAA1
	end)

	section3:addDropdown("Element Slot 1",elm,function(aELM1)
		print("Element Slot 1: " .. aELM1)
		notify("Element Slot 1: " .. aELM1)
		player.statz.main.element1.Value = aELM1
	end)
	section3:addDropdown("Element Slot 2",elm,function(aELM2)
		print("Element Slot 2: " .. aELM2)
		notify("Element Slot 2: " .. aELM2)
		player.statz.main.element2.Value = aELM2
	end)
	section3:addDropdown("Element Slot 3",elm,function(aELM3)
		print("Element Slot 3: " .. aELM3)
		notify("Element Slot 3: " .. aELM3)
		player.statz.main.element3.Value = aELM3
	end)
	section3:addDropdown("Element Slot 4",elm,function(aELM4)
		print("Element Slot 4: " .. aELM4)
		notify("Element Slot 4: " .. aELM4)
		player.statz.main.element4.Value = aELM4
	end)

	section4:addDropdown("Bloodline Slot 1",kgs,function(aKG1)
		print("Bloodline Slot 1: " .. aKG1)
		notify("Bloodline Slot 1: " .. aKG1)
		player.statz.main.kg1.Value = aKG1
	end)
	section4:addDropdown("Bloodline Slot 2",kgs,function(aKG2)
		print("Bloodline Slot 2: " .. aKG2)
		notify("Bloodline Slot 2: " .. aKG2)
		player.statz.main.kg2.Value = aKG2
	end)
	section4:addDropdown("Bloodline Slot 3",kgs,function(aKG3)
		print("Bloodline Slot 3: " .. aKG3)
		notify("Bloodline Slot 3: " .. aKG3)
		player.statz.main.kg3.Value = aKG3
	end)
	section4:addDropdown("Bloodline Slot 4",kgs,function(aKG4)
		print("Bloodline Slot 4: " .. aKG4)
		notify("Bloodline Slot 4: " .. aKG4)
		player.statz.main.kg4.Value = aKG4
	end)

	section5:addButton("External Shindo Bloodline Changer",function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/NoobieDoge/DogeHub/Separated/ShindoBloodlineChanger"))()
	end)

	-- fifth page
	local page5 = NoobieDoge:addPage("Auto Farm", 5012544693)
	local section1 = page5:addSection("Boss Farm")
	local section2 = page5:addSection("Configurations")

	if game:GetService("Workspace").npc.npc1:FindFirstChild("megaboss") then
		section1:addToggle("Auto Gen 3 Tentacion",false, function(bool)
			gen3farm = bool
			--game:GetService("Workspace").ClientEffects:Destroy()
			local platform = Instance.new("Part", player.Character)
			platform.Anchored = true
			platform.Transparency = 1
			platform.Size = Vector3.new(6, 2, 6)

			local connection
			connection = game:GetService("RunService").RenderStepped:connect(function()
				if player.Character:FindFirstChild("HumanoidRootPart") then
					platform.Position = player.Character.HumanoidRootPart.CFrame * Vector3.new(0, -2.35, 0)
				else
					connection:Disconnect()
				end
			end)
			spawn(function ()
				while wait() do
					if gen3farm ~= true then
						platform:Destroy()
					end
				end
			end)
		end)
	else
		if game:GetService("Workspace").npc.npc1.Head.mob.fram.name.info.Text == "[Lv. 2000][Ken Mentor]" then
			section1:addToggle("Kenjutsu Mentor Farm",false, function(bool)
				dfarm = bool
				notify("Kenjutsu Mentor Farm: "..tostring(dfarm),true)
			end)
		else
			section1:addToggle("5/5",false, function(bool)
				dfarm = bool
				notify("5/5: "..tostring(dfarm),true)
			end)
		end
	end

	section2:addToggle("Legacy Farm Mode",false, function(bool)
		Legacy = bool
		notify("Legacy Farm Mode: "..tostring(Legacy),true)
	end)

	if game:GetService("Workspace").npc.npc1:FindFirstChild("megaboss") then
		section2:addToggle("NPC Kill Aura (Hard to trigger)",false, function(bool)
			keel2 = bool
			NPCKEELAURA()
			notify("NPC Kill Aura: "..tostring(keel2),true)
		end)
	else
		section2:addToggle("NPC Kill Aura",false, function(bool)
			keel2 = bool
			NPCKEELAURA()
			notify("NPC Kill Aura: "..tostring(keel2),true)
		end)
	end

	game:GetService('RunService').Stepped:connect(function()
		if Legacy then
			for i,v in pairs(workspace.npc:GetChildren()) do
				if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
					repeat
						game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
						player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame+Vector3.new(0,6,0)
						player.Character.combat.update:FireServer("key","e")
						player.Character.combat.update:FireServer("key","eend")
						player.Character.combat.update:FireServer("mouse1",true)
						wait(.000000000000001)
						v.Humanoid.Health = 0
						player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame+Vector3.new(0,7,.3)
						player.Character.combat.update:FireServer("key","e")
						player.Character.combat.update:FireServer("key","eend")
						player.Character.combat.update:FireServer("mouse1",true)
						wait(.000000000000001)
						v.Humanoid.Health = 0
					until not v:FindFirstChild("Humanoid") or not Legacy or not v
				end
			end
		end
	end)

	game:GetService('RunService').Stepped:connect(function()
		if dfarm then
			for i,v in pairs(workspace.npc:GetChildren()) do
				if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
					repeat wait() v.Humanoid.Health = 0
					game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
					player.Character.HumanoidRootPart.CFrame=(v.HumanoidRootPart.CFrame * CFrame.new(0, 7, .3))
					until not v:FindFirstChild("Humanoid") or not dfarm
				end
			end
		end
	end)

	game:GetService('RunService').Stepped:connect(function()
		if dfarm or Legacy or gen3farm then
			for i,v in pairs(workspace.npc:GetChildren()) do
				if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
					repeat v.Humanoid.Health = 0
					until v.Humanoid.Health == 0 or (not dfarm and not Legacy and not gen3farm)
				end
			end
		end
	end)

	game:GetService('RunService').Stepped:connect(function()
		if gen3farm then
			for i,v in pairs(workspace.npc:GetChildren()) do
				if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
					repeat wait() v.Humanoid.Health = 0
					game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
					if Legacy == false then
						player.Character.HumanoidRootPart.CFrame=(v.HumanoidRootPart.CFrame * CFrame.new(0, 75, .3))
					end
					until not v:FindFirstChild("Humanoid") or not gen3farm
				end
			end
		end
	end)

	-- sixth page
	local page6 = NoobieDoge:addPage("Teleports", 5012544693)
	local section1 = page6:addSection("Server Teleports")

	local maps = {}
	for i,v in pairs(player.PlayerGui.Main.ingame.Menu.ArenaTab.List:GetChildren()) do
		if v:FindFirstChild("place") then
			table.insert(maps, v.Name)
		end
	end
	local mapselect
	section1:addDropdown("Map Selection",maps,function(MAPSed)
		print("Selected: " .. MAPSed)
		notify("Selected: " .. MAPSed)
		mapselect = MAPSed
	end)

	section1:addButton("Teleport",function()
		local args = {[1] = "rpgteleport",[2] = player.PlayerGui.Main.ingame.Menu.ArenaTab.List[mapselect].place.Value}
		player.startevent:FireServer(unpack(args))
	end)

	section1:addTextbox("Private Server Code", "DDrugs", function(PScodevalue, focusLost)
		print("Input", PScodevalue)
		if focusLost then
			notify("Private Server Code: " .. PScodevalue)
			PScodevalue1 = PScodevalue
		end
	end)

	section1:addButton("Private Server Teleport",function()
		local args = {[1] = "teleporttoprivate",[2] = PScodevalue1}
		player.startevent:FireServer(unpack(args))
	end)

	-- last page
	local pagelast = NoobieDoge:addPage("Settings", 5012544693)
	local settings = pagelast:addSection("Settings")
	local colors = pagelast:addSection("Colors")
	local credits = pagelast:addSection("Credits")

	settings:addButton("Upgrade Performance",function()
		UPPERFORMANCE()
		settings:updateButton("Upgrade Performance","Upgraded Performance")
	end)

	settings:addButton("Remove Effects / Particles",function()
		if player.PlayerScripts:FindFirstChild("EFFECTS") then
			player.PlayerScripts.EFFECTS:Destroy()
		end
		settings:updateButton("Remove Effects / Particles","Removed Effects / Particles")
	end)

	settings:addButton("Reset Script",function()
		local player = game.Players.LocalPlayer
		local ScreenGuiK = Instance.new("ScreenGui")
		local Frame = Instance.new("ImageLabel")
		local TextLabelFrame = Instance.new("ImageLabel")
		local TextLabel = Instance.new("TextLabel")
		local TextButton = Instance.new("ImageButton")
		local TextButtonText = Instance.new("TextButton")
		local TextButton_2 = Instance.new("ImageButton")
		local TextButton_2Text = Instance.new("TextButton")

		ScreenGuiK.Parent = player:WaitForChild("PlayerGui")

		Frame.Parent = ScreenGuiK
		Frame.Name = "Main"
		Frame.BackgroundTransparency = 1
		Frame.Position = UDim2.new(0.66, 0, 0.055, 0)
		Frame.Size = UDim2.new(0, 220, 0, 120)
		Frame.Image = "rbxassetid://4641149554"
		Frame.ImageColor3 = Color3.fromRGB(24, 24, 24)
		Frame.ScaleType = Enum.ScaleType.Slice
		Frame.SliceCenter = Rect.new(4, 4, 296, 296)
		Frame.Active = true
		Frame.Draggable = true

		TextLabelFrame.Parent = Frame
		TextLabelFrame.Name = "TopBar"
		TextLabelFrame.BackgroundTransparency = 1
		TextLabelFrame.ClipsDescendants = true
		TextLabelFrame.Size = UDim2.new(1, 0, 0, 38)
		TextLabelFrame.ZIndex = 2
		TextLabelFrame.Image = "rbxassetid://4595286933"
		TextLabelFrame.ImageColor3 = Color3.fromRGB(255, 163, 26)
		TextLabelFrame.ScaleType = Enum.ScaleType.Slice
		TextLabelFrame.SliceCenter = Rect.new(4, 4, 296, 296)

		TextLabel.Parent = Frame
		TextLabel.BackgroundColor3 = Color3.fromRGB(255, 163, 26)
		TextLabel.Position = UDim2.new(0, 0, -0.1, 0)
		TextLabel.BackgroundTransparency = 1
		TextLabel.Size = UDim2.new(0, 220, 0, 60)
		TextLabel.ZIndex = 3
		TextLabel.Font = Enum.Font.TitilliumWeb
		TextLabel.Text = "Are you sure that you want to reset script?"
		TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
		TextLabel.TextScaled = false
		TextLabel.TextSize = 21.000
		TextLabel.TextWrapped = true

		TextButton.Parent = Frame
		TextButton.Name = "Button"
		TextButton.BackgroundTransparency = 1
		TextButton.Position = UDim2.new(0.075, 0, 0.500000006, 0)
		TextButton.BorderSizePixel = 0
		TextButton.Size = UDim2.new(0, 82.5, 0, 45)
		TextButton.ZIndex = 2
		TextButton.Image = "rbxassetid://5028857472"
		TextButton.ImageColor3 = Color3.fromRGB(41, 41, 41)
		TextButton.ScaleType = Enum.ScaleType.Slice
		TextButton.SliceCenter = Rect.new(2, 2, 298, 298)

		TextButtonText.Parent = TextButton
		TextButtonText.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
		TextButtonText.BackgroundTransparency = 1
		TextButtonText.ZIndex = 3
		TextButtonText.Size = UDim2.new(0, 82.5, 0, 45)
		TextButtonText.Font = Enum.Font.TitilliumWeb
		TextButtonText.Text = "Yes"
		TextButtonText.TextColor3 = Color3.fromRGB(200, 200, 200)
		TextButtonText.TextScaled = true
		TextButtonText.TextSize = 40.000
		TextButtonText.TextWrapped = true

		function ResetYES()
			for i,v in pairs(player.PlayerGui:GetChildren()) do
				if v.Name == "ScreenGui" then
					v:Destroy()
				end
			end
			wait()
			sliderpowa = false
			player.Character.Humanoid.WalkSpeed = originalwalkspeed
			player.Character.Humanoid.JumpPower = originaljumppower
			game:GetService("Workspace").Gravity = originalgravity
			CtrlClickTeePee = false
			noclips = false
			Pfly = false
			getgenv().dc = true
			DisabledInCombat = false
			unbreakableblockstoggle = false
			keel = false
			SpamGOBRRRRR = false
			ClickRATATA = false
			Q = false
			E = false
			R = false
			T = false
			Y = false
			F = false
			G = false
			H = false
			V = false
			B = false
			N = false
			Z = false
			C = false
			dfarm = false
			gen3farm = false
			Legacy = false
			keel2 = false
			wait()
			for i,library in pairs(game:GetService("CoreGui"):GetChildren()) do
				if library.Name == "DogeHub V2" then
					library:Destroy()
				end
			end
			notify("Do not reset script too many times!")
			wait(.1)
			ShindoBossBattle()
		end
		TextButton.MouseButton1Click:connect(ResetYES)
		TextButtonText.MouseButton1Click:connect(ResetYES)

		TextButton_2.Parent = Frame
		TextButton_2.Name = "Button"
		TextButton_2.BackgroundTransparency = 1
		TextButton_2.Position = UDim2.new(0.55, 0, 0.500000006, 0)
		TextButton_2.BorderSizePixel = 0
		TextButton_2.Size = UDim2.new(0, 82.5, 0, 45)
		TextButton_2.ZIndex = 2
		TextButton_2.Image = "rbxassetid://5028857472"
		TextButton_2.ImageColor3 = Color3.fromRGB(41, 41, 41)
		TextButton_2.ScaleType = Enum.ScaleType.Slice
		TextButton_2.SliceCenter = Rect.new(2, 2, 298, 298)

		TextButton_2Text.Parent = TextButton_2
		TextButton_2Text.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
		TextButton_2Text.BackgroundTransparency = 1
		TextButton_2Text.ZIndex = 3
		TextButton_2Text.Size = UDim2.new(0, 82.5, 0, 45)
		TextButton_2Text.Font = Enum.Font.TitilliumWeb
		TextButton_2Text.Text = "No"
		TextButton_2Text.TextColor3 = Color3.fromRGB(200, 200, 200)
		TextButton_2Text.TextScaled = true
		TextButton_2Text.TextSize = 40.000
		TextButton_2Text.TextWrapped = true

		function ResetNO()
			for i,v in pairs(player.PlayerGui:GetChildren()) do
				if v.Name == "ScreenGui" then
					v:Destroy()
				end
			end
		end
		TextButton_2.MouseButton1Click:connect(ResetNO)
		TextButton_2Text.MouseButton1Click:connect(ResetNO)
	end)

	settings:addButton("Rejoin Server",function()
		REJOINSERVER()
	end)

	settings:addKeybind("Toggle Keybind", Enum.KeyCode.RightControl, function()
	print("Activated Keybind")
	NoobieDoge:toggle()
	end, function()
	print("Changed Keybind")
	end)

	for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
		colors:addColorPicker(theme, color, function(color3)
			NoobieDoge:setTheme(theme, color3)
		end)
	end

	credits:addButton("Made by NoobieDoge")
	credits:addButton("https://discord.gg/P2CeRjvTDt")
	credits:addButton("Copy Discord Link",function()
		setclipboard("https://discord.gg/P2CeRjvTDt")
		wait()
		notify("Discord link copied to clipboard!")
	end)

	-- load
	NoobieDoge:SelectPage(NoobieDoge.pages[1], true)

end

--Shindo Event Boss
local function ShindoEventBoss()
	--DogeHub V2 GUI
	local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/pornhub%20page%20format%20gui"))()
	local NoobieDoge = library.new("DogeHub V2", 5013109572)

	-- themes
	local themes = {
	Background = Color3.fromRGB(24, 24, 24),
	Glow = Color3.fromRGB(0, 0, 0),
	Accent = Color3.fromRGB(10, 10, 10),
	LightContrast = Color3.fromRGB(20, 20, 20),
	DarkContrast = Color3.fromRGB(14, 14, 14),
	TextColor = Color3.fromRGB(255, 255, 255)
	}

	-- first page
	local page1 = NoobieDoge:addPage("Universal", 5012544693)
	local section1 = page1:addSection("Walk Speed & Jump Power & Gravity")
	local section2 = page1:addSection("Other Functions")

	section1:addSlider("Walk Speed", originalwalkspeed, 0, 500, function(value)
		sliderspeed = value
	end)
	section1:addSlider("Jump Power", originaljumppower, 0, 500, function(value)
		sliderjump = value
	end)
	section1:addSlider("Gravity", originalgravity, 0, 500, function(value)
		slidergravity = value
	end)
	sliderspeed = 16
	sliderjump = 50
	slidergravity = 196.2

	section1:addToggle("Enabled", nil, function(value)
		sliderpowa = value
	end)
	game:GetService('RunService').Stepped:connect(function()
		if sliderpowa then
			player.Character.Humanoid.WalkSpeed = sliderspeed
			player.Character.Humanoid.JumpPower = sliderjump
			game:GetService("Workspace").Gravity = slidergravity
		end
	end)

	section1:addButton("Reset to Default", function()
		player.Character.Humanoid.WalkSpeed = originalwalkspeed
		player.Character.Humanoid.JumpPower = originaljumppower
		game:GetService("Workspace").Gravity = originalgravity
		section1:updateSlider("Walk Speed", "Walk Speed", originalwalkspeed, 0, 500)
		section1:updateSlider("Jump Power", "Jump Power", originaljumppower, 0, 500)
		section1:updateSlider("Gravity", "Gravity", originalgravity, 0, 500)
	end)

	section2:addButton("Add ESP GUI", function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Project%3A%20Bullshit%3A%20Rebooted"))()
	end)

	section2:addButton("Reset Character",function()
		player.Character:BreakJoints()
	end)

	section2:addToggle("Ctrl + Click TP",false,function(bool)
		CtrlClickTeePee = bool
		CTRLCLICKTP()
		if CtrlClickTeePee == true then
			notify("Press Ctrl + Click to Teleport")
		end
	end)

	local infjumptoggle = false
	section2:addToggle("Infinite Jump",false,function()
		infjumptoggle = not infjumptoggle
		notify("Infinite Jump: "..tostring(infjumptoggle),true)
		if infjumptoggle then
			getgenv().dc = false
			local infjump
			infJump = game:GetService("UserInputService").JumpRequest:Connect(function()
				if dc then infjump:Disconnect() return end
				player.Character.Humanoid:ChangeState("Jumping")
			end)
		else
			getgenv().dc = true
		end
	end)

	player.CharacterAdded:Connect(function()
		if infjumptoggle then
			wait(2.5)
			getgenv().dc = false
			local infjump
			infJump = game:GetService("UserInputService").JumpRequest:Connect(function()
				if dc then infjump:Disconnect() return end
				player.Character.Humanoid:ChangeState("Jumping")
			end)
		end
	end)

	section2:addToggle("Noclip", false, function(bool)
		noclips = bool
		game:GetService("RunService").Stepped:connect(function()
			if noclips then
				for i, v in pairs(player.Character:GetChildren()) do
					if v:IsA("BasePart") then
						v.CanCollide = false
					end
				end
			end
		end)
	end)

	section2:addSlider("Fly Speed", 101, 0, 999, function(value)
		sliderfly = value
	end)
	sliderfly = 101
	section2:addToggle("Press P to fly",false,function(bool)
		Pfly = bool
		repeat wait() 
		until player and player.Character:findFirstChild("Head") player.Character:findFirstChild("Humanoid") 
		local mouse = player:GetMouse() 
		repeat wait() until mouse
		local torso = player.Character.Head 
		local flying = false
		local deb = true 
		local ctrl = {f = 0, b = 0, l = 0, r = 0} 
		local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
		local maxspeed = sliderfly 
		local speed = 0 

		function Fly() 
			local bg = Instance.new("BodyGyro", torso) 
			bg.P = 9e4 
			bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
			bg.cframe = torso.CFrame 

			local bv = Instance.new("BodyVelocity", torso) 
			bv.velocity = Vector3.new(0,0.1,0) 
			bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
			repeat wait() 
				player.Character.Humanoid.PlatformStand = true 
				if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then 
					speed = speed+(sliderfly/1.7)+(speed/maxspeed) 
					if speed > maxspeed then 
						speed = maxspeed 
					end 

				elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then 
					speed = speed-(sliderfly/1.7)
					if speed < 0 then 
						speed = 0 
					end 
				end 
				if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then 
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
					lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
				elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then 
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
				else 
					bv.velocity = Vector3.new(0,0.1,0)
				end 
				bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) 
			until not flying or not Pfly
			ctrl = {f = 0, b = 0, l = 0, r = 0} 
			lastctrl = {f = 0, b = 0, l = 0, r = 0} 
			speed = 0 
			bg:Destroy() 
			bv:Destroy() 
			player.Character.Humanoid.PlatformStand = false 
		end 

		mouse.KeyDown:connect(function(key) 
			if key:lower() == "p" then 
				if flying then flying = false 
				else 
					flying = true 
					Fly() 
				end 
			elseif key:lower() == "w" then 
				ctrl.f = 1 
			elseif key:lower() == "s" then 
				ctrl.b = -1 
			elseif key:lower() == "a" then 
				ctrl.l = -1 
			elseif key:lower() == "d" then 
				ctrl.r = 1 
			end 
		end) 

		mouse.KeyUp:connect(function(key) 

			if key:lower() == "w" then 
				ctrl.f = 0 
			elseif key:lower() == "s" then 
				ctrl.b = 0 
			elseif key:lower() == "a" then 
				ctrl.l = 0 
			elseif key:lower() == "d" then 
				ctrl.r = 0 
			end 
		end)
		Fly()
		notify("Press P to fly: "..tostring(Pfly),true)
	end)

	player.CharacterAdded:Connect(function()
		if Pfly then
			repeat wait() 
			until player and player.Character:findFirstChild("Head") player.Character:findFirstChild("Humanoid") 
			local mouse = player:GetMouse() 
			repeat wait() until mouse
			local player = game.Players.LocalPlayer 
			local torso = player.Character.Head 
			local flying = false
			local deb = true 
			local ctrl = {f = 0, b = 0, l = 0, r = 0} 
			local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
			local maxspeed = sliderfly 
			local speed = 0 
			
			function Fly() 
				local bg = Instance.new("BodyGyro", torso) 
				bg.P = 9e4 
				bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
				bg.cframe = torso.CFrame 
	
				local bv = Instance.new("BodyVelocity", torso) 
				bv.velocity = Vector3.new(0,0.1,0) 
				bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
				repeat wait() 
					player.Character.Humanoid.PlatformStand = true 
					if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then 
						speed = speed+(sliderfly/1.7)+(speed/maxspeed) 
						if speed > maxspeed then 
							speed = maxspeed 
						end 
	
					elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then 
						speed = speed-(sliderfly/1.7)
						if speed < 0 then 
							speed = 0 
						end 
					end 
					if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then 
						bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
						lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
					elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then 
						bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
					else 
						bv.velocity = Vector3.new(0,0.1,0)
					end 
					bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) 
				until not flying or not Pfly
				ctrl = {f = 0, b = 0, l = 0, r = 0} 
				lastctrl = {f = 0, b = 0, l = 0, r = 0} 
				speed = 0 
				bg:Destroy() 
				bv:Destroy() 
				player.Character.Humanoid.PlatformStand = false 
			end 
	
			mouse.KeyDown:connect(function(key) 
				if key:lower() == "p" then 
					if flying then flying = false 
					else 
						flying = true 
						Fly() 
					end 
				elseif key:lower() == "w" then 
					ctrl.f = 1 
				elseif key:lower() == "s" then 
					ctrl.b = -1 
				elseif key:lower() == "a" then 
					ctrl.l = -1 
				elseif key:lower() == "d" then 
					ctrl.r = 1 
				end 
			end) 
	
			mouse.KeyUp:connect(function(key) 
	
				if key:lower() == "w" then 
					ctrl.f = 0 
				elseif key:lower() == "s" then 
					ctrl.b = 0 
				elseif key:lower() == "a" then 
					ctrl.l = 0 
				elseif key:lower() == "d" then 
					ctrl.r = 0 
				end 
			end)
			Fly()
		end
	end)

	local playerlist = {}
	for i,v in pairs(game:GetService("Players"):GetPlayers()) do
		if v.Name ~= player.Name then
			table.insert(playerlist, v.Name)
		end
	end
	section2:addDropdown("Track selected player",playerlist,function(ripthatguy)
		notify("Player Selected: " .. ripthatguy)
		Target = ripthatguy
	end)

	section2:addToggle("Track Player",false,function(bool)
		trackthatguy = bool
		local platform = Instance.new("Part", player.Character)
		platform.Anchored = true
		platform.Transparency = 1
		platform.Size = Vector3.new(6, 1, 6)

		local connection
		connection = game:GetService("RunService").RenderStepped:connect(function()
			if player.Character:FindFirstChild("HumanoidRootPart") then
				platform.Position = player.Character.HumanoidRootPart.CFrame * Vector3.new(0, -1.8, 0)
			else
				connection:Disconnect()
			end
		end)
		game:GetService('RunService').Stepped:connect(function()
			if trackthatguy ~= true then
				platform:Destroy()
			end
		end)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if trackthatguy == true then
			player.Character.HumanoidRootPart.CFrame = game.Players[Target].Character.HumanoidRootPart.CFrame * CFrame.new(0, 5, 10)
			game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, game.Players[Target].Character.HumanoidRootPart.Position)
		end
	end)

	section2:addButton("Refesh Players List",function()
		section2:updateDropdown("Track selected player","Track selected player",playerlist)
	end)

