	local function FARM()
		if game:GetService("Players").LocalPlayer.statz.sword.throwable.Value == "BOSSDROPazimftg" or game:GetService("Players").LocalPlayer.statz.sword.throwable.Value == "kunaiwire" or game:GetService("Players").LocalPlayer.statz.sword.throwable.Value == "kunaiflash" then
			notify("Please change the throwable weapon!")
			autofarm = false
			boss = false
			megaboss = false
		else
			if LFM then
				for i,v in pairs(workspace.npc:GetChildren()) do
					local mobname = string.split(mission.bg.name.Text,"Defeat ")[1]
					if(string.find(mobname,"(s)")) then
						mobname = string.gsub(mobname,"(s)","")
					end
					if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and not v:FindFirstChild("fullsusanoo") and v.Head.CFrame.Y > -1000 then
						repeat
							pcall(function()
								v.Humanoid.Health = 0
								game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
								player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
								game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
								game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
								game.Players.LocalPlayer.Character.combat.update:FireServer("mouse1",true)
								wait(.000000000000001)
								v.Humanoid.Health = 0
							end)
						until not v:FindFirstChild("Humanoid") or mission.Visible == false or (not autofarm and not boss and not megaboss) or not v
					end
				end
			elseif SpamGOBRRRRR and SpamOnlyInMissions then
				for i,v in pairs(workspace.npc:GetChildren()) do
					local mobname = string.split(mission.bg.name.Text,"Defeat ")[1]
					if(string.find(mobname,"(s)")) then
						mobname = string.gsub(mobname,"(s)","")
					end
					if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and not v:FindFirstChild("fullsusanoo") and v.Head.CFrame.Y > -1000 then
						player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 15, 0)
						repeat wait()
							pcall(function()
								game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
								v.Humanoid.Health = 0
								pcall(function()
									player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, AFHeight, AFDistance)
									if E then SpamE() end
									if R then SpamR() end
									if T then SpamT() end
									if Y then SpamY() end
									if F then SpamF() end
									if G then SpamG() end
									if H then SpamH() end
									if V then SpamV() end
									if B then SpamB() end
									if N then SpamN() end
									if Z then SpamZ() end
									if C then SpamC() end
									if Q then SpamQ() end
									if ClickRATATA then SpamClick() end
									v.Humanoid.Health = 0
								end)
							end)
						until not v:FindFirstChild("Humanoid") or mission.Visible == false or (not autofarm and not boss and not megaboss) or not v
					end
				end
			else
				for i,v in pairs(workspace.npc:GetChildren()) do
					local mobname = string.split(mission.bg.name.Text,"Defeat ")[1]
					if(string.find(mobname,"(s)")) then
						mobname = string.gsub(mobname,"(s)","")
					end
					if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and not v:FindFirstChild("fullsusanoo") and v.Head.CFrame.Y > -1000 then
						player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 15, 0)
						repeat wait()
							pcall(function()
								game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
								v.Humanoid.Health = 0
								pcall(function()
									player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, AFHeight, AFDistance)
									game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
									game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
									game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
									game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
									game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
									game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
									game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
									game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
									v.Humanoid.Health = 0
								end)
							end)
						until not v:FindFirstChild("Humanoid") or mission.Visible == false or (not autofarm and not boss and not megaboss) or not v
					end
				end
			end
		end
	end

	local count
	local function DISP()
		count = 0
		for i,v in pairs(workspace.missiongivers:GetChildren()) do
			if v.Name == "" and v:FindFirstChild("Head") then
				if v:FindFirstChild("Head") and v.Head:FindFirstChild("givemission") and v.Head.givemission.Enabled and v.Head.givemission:FindFirstChild("color") and v.Head.givemission.color.Image == "http://www.roblox.com/asset/?id=5459241648" then
					count = count + 1
				end
			end
		end
	end

	spawn(function()
		while wait() do
			if autofarm or boss or megaboss then
				if mission.Visible == true then
					FARM()
					for i,v in pairs(workspace.npc:GetChildren()) do
						if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
							repeat 
								v.Humanoid.Health = 0 
								v.Humanoid.Health = 0
								v.Humanoid.Health = 0
								v.Humanoid.Health = 0
								v.Humanoid.Health = 0
							until (not autofarm and not boss and not megaboss) or v.Humanoid.Health == 0
						end
					end
				else
					if megaboss and boss and autofarm then
						BEASTFARM()
						wait(.1)
						BOSSFARM()
						wait(.1)
						DISP()
						wait(.1)
						QUEST()
					elseif boss and megaboss then
						BEASTFARM()
						wait(.1)
						BOSSFARM()
					elseif megaboss and autofarm then
						BEASTFARM()
						wait(.1)
						DISP()
						wait(.1)
						QUEST()
					elseif boss and autofarm then
						BOSSFARM()
						wait(.1)
						DISP()
						wait(.1)
						QUEST()
					elseif megaboss then
						BEASTFARM()
					elseif boss then
						BOSSFARM()
					elseif autofarm then
						DISP()
						wait(.1)
						QUEST()
					end
				end
			end
		end
	end)

	spawn(function()
		while wait() do
			if autofarm then
				if count == 0 then
					if QUEST() or FARM() or BOSSFARM() then
						wait(5.5)
					else
						repeat wait()
							notify("Currently no missions avaliable")
							wait(2)
						until count > 0 or QUEST or FARM or BOSSFARM() or not autofarm
					end
				end
			end
		end
	end)

	local PRES = player.statz.prestige.maxlvlpres
	local RANK = player.statz.prestige.rank
	local RANKNUM = player.statz.prestige.number

	spawn(function()
		while wait() do
			if RANKUPP and player.statz.lvl:FindFirstChild("lvl").Value == 1000 then
				repeat wait()
					game.Players.LocalPlayer.startevent:FireServer("rankup")
				until player.statz.lvl:FindFirstChild("lvl").Value == 1 or not RANKUPP
			end
		end
	end)

