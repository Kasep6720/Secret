local CtrlClickTeePee
function CTRLCLICKTP()
	local UIS = game:GetService("UserInputService")
	local Player = game.Players.LocalPlayer
	local Mouse = Player:GetMouse()

	function GetCharacter()
	return player.Character
	end

	function Teleport(pos)
		local Char = GetCharacter()
		if Char then
			Char:MoveTo(pos)
		end
	end

	UIS.InputBegan:Connect(function(input)
		if CtrlClickTeePee == true then
			if input.UserInputType == Enum.UserInputType.MouseButton1 and UIS:IsKeyDown(Enum.KeyCode.LeftControl) then
				Teleport(Mouse.Hit.p)
			end
		end
	end)
end
function GODMODE()
	if player.Character:FindFirstChild("fakehealth") then
		if player.statz.mastery.health.Value > 1750 then
			notify("Your character will be broken after activating God Mode")
			local UrOriginalHP = player.statz.mastery.health.Value*60
			repeat wait() until player.Character.fakehealth.Value ~= UrOriginalHP
			player.Character.fakehealth:Destroy()
		else
			player.Character.fakehealth:Destroy()
		end
		wait(2.1)
		notify("Do not charge chi or mode up")
	else
		notify("God Mode is already activated")
	end
end
function SUPERHUMAN()
	local chanceofaudioquote = math.random(1, 100)
	if chanceofaudioquote <= 10 then
		local s = Instance.new("Sound")
		s.Name = "Sound" s.SoundId = "http://www.roblox.com/asset/?id=1169755927" s.Volume = 20 s.Looped = false s.archivable = false s.Parent = game.Workspace wait(.3) s:play()
	end
	_G.Doge = true;
	_G.dogespeed = 80;

	local mt = getrawmetatable(game)
	local old = mt.__newindex
	setreadonly(mt, false)

	mt.__newindex = newcclosure(function(t,k,v)
		pcall(function()
			if t == player.Character.Humanoid and k == "WalkSpeed" and _G.Doge then
				v = _G.dogespeed;
			end
		end)
		return old(t,k,v)
	end)
	notify("SuperHuman activated")
	wait(1)
end
function UNBREAKABLE_BLOCKS()
	local chanceofaudioquote = math.random(1, 100)
	if chanceofaudioquote <= 25 then
		local s = Instance.new("Sound")
		s.Name = "Sound" s.SoundId = "http://www.roblox.com/asset/?id=10163532591" s.Volume = 20 s.Looped = false s.archivable = false s.Parent = game.Workspace wait(.3) s:play()
	else
		local chanceofnotifyquote = math.random(1, 100)
		if chanceofnotifyquote <= 20 then
			notify("Making the mother of all omelettes here, Jack")
			wait(1.2)
			notify("Can't fret over every egg.")
		end
	end
	for i,v in pairs(game.workspace:GetChildren()) do
		if v.Name == player.Name and v:FindFirstChild("blockbreaker") then
			v.blockbreaker.Parent = game:GetService("Workspace")[player.Name].gunfolder
		end
	end
end
function DISABLE_UNBREAKABLE_BLOCKS()
	for i,v in pairs(game.workspace:GetChildren()) do
		if v.Name == player.Name and v:FindFirstChild("gunfolder") and v.gunfolder:FindFirstChild("blockbreaker") then
			v.gunfolder.blockbreaker.Parent = game:GetService("Workspace")[player.Name]
		end
	end
end
function NPCKEELAURA()
	for i,v in pairs(workspace.npc:GetChildren()) do
		if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
			repeat
				v.Humanoid.Health = 0
			until v.Humanoid.Health == 0 or (not keel and not keel2)
		end
	end
end
function INFMDON()
	local mode = player.Character.combat.mode
	local copy = mode:Clone()
	copy.Parent = mode.Parent
	mode:Destroy()
	notify("Do not charge chi til max while inf MD is on!")
end
function SAVEDATA()
	local player = game.Players.LocalPlayer
	local ScreenGuiK = Instance.new("ScreenGui")
	local FrameK = Instance.new("ImageLabel")
	local TextLabelFrame = Instance.new("ImageLabel")
	local TextLabel = Instance.new("TextLabel")
	local TextButton = Instance.new("ImageButton")
	local TextButtonText = Instance.new("TextButton")
	local TextButton_2 = Instance.new("ImageButton")
	local TextButton_2Text = Instance.new("TextButton")

	ScreenGuiK.Parent = player:WaitForChild("PlayerGui")

	FrameK.Parent = ScreenGuiK
	FrameK.Name = "Main"
	FrameK.BackgroundTransparency = 1
	FrameK.Position = UDim2.new(0.005, 0, 0.7, 0)
	FrameK.Size = UDim2.new(0, 220, 0, 120)
	FrameK.Image = "rbxassetid://4641149554"
	FrameK.ImageColor3 = Color3.fromRGB(24, 24, 24)
	FrameK.ScaleType = Enum.ScaleType.Slice
	FrameK.SliceCenter = Rect.new(4, 4, 296, 296)

	TextLabelFrame.Parent = FrameK
