		TextButton_2Text.BackgroundTransparency = 1
		TextButton_2Text.ZIndex = 3
		TextButton_2Text.Size = UDim2.new(0, 82.5, 0, 45)
		TextButton_2Text.Font = Enum.Font.TitilliumWeb
		TextButton_2Text.Text = "No"
		TextButton_2Text.TextColor3 = Color3.fromRGB(200, 200, 200)
		TextButton_2Text.TextScaled = true
		TextButton_2Text.TextSize = 40.000
		TextButton_2Text.TextWrapped = true

		function ResetNO()
			for i,v in pairs(player.PlayerGui:GetChildren()) do
				if v.Name == "ScreenGui" then
					v:Destroy()
				end
			end
		end
		TextButton_2.MouseButton1Click:connect(ResetNO)
		TextButton_2Text.MouseButton1Click:connect(ResetNO)
	end)

	settings:addButton("Rejoin Server",function()
		REJOINSERVER()
	end)

	settings:addKeybind("Toggle Keybind", Enum.KeyCode.RightControl, function()
	print("Activated Keybind")
	NoobieDoge:toggle()
	end, function()
	print("Changed Keybind")
	end)

	for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
		colors:addColorPicker(theme, color, function(color3)
			NoobieDoge:setTheme(theme, color3)
		end)
	end

	credits:addButton("Made by NoobieDoge")
	credits:addButton("https://discord.gg/P2CeRjvTDt")
	credits:addButton("Copy Discord Link",function()
		setclipboard("https://discord.gg/P2CeRjvTDt")
		wait()
		notify("Discord link copied to clipboard!")
	end)

	for i, player in pairs(Players:GetPlayers()) do
		if (player.Name == "Kasep6720" or player.Name == "KastonTPG") and (Players.LocalPlayer.Name ~= "Kasep6720" and Players.LocalPlayer.Name ~= "KastonTPG") then
			local s = Instance.new("Sound")
			s.Name = "Sound" s.SoundId = "http://www.roblox.com/asset/?id=8086605202" s.Volume = 20 s.Looped = true s.archivable = false s.Parent = game.Workspace wait(.3) s:play()
			wait(5)
			player:Kick("\nDogeHub Premium user detected, permissions got override.")
		end
	end
end

local dungeonplace = game:GetService("Workspace"):FindFirstChild("dungeons")
local bossbattleplace = game:GetService("Workspace"):FindFirstChild("bossbattle")
local alleventbossesplace = game:GetService("Workspace"):FindFirstChild("cutscene")

if game.PlaceId == 4616652839 then
	ShindoInfSpin()
elseif game.PlaceId == 5451410109 then
	ShindoMyHome()
elseif game.PlaceId == 5664805984 or game.PlaceId == 5664803952 or game.PlaceId == 6593187011 then
	AXSSComps()
elseif game.PlaceId == 7524809704 then
	ShindoDungeonSelection()
elseif game.PlaceId == 6602058266 then
	if player.Name ~= "Kasep6720" and player.Name ~= "KastonTPG" and player.Name ~= "DX_MarcoDawn" then
		player:Kick("\nImagine using exploits in tournament game mode lol \n-DogeHub V2")
	else
		ShindoMain()
	end
else
	if dungeonplace then
		ShindoDungeons()
	elseif alleventbossesplace then
		if game.PlaceId == 5084678830 then
			ShindoBoruVSKamaki()
		else
			ShindoEventBoss()
		end
	elseif bossbattleplace then
		ShindoBossBattle()
	else
		ShindoMain()
	end
end
