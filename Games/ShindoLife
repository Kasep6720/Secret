		StartSpinElm1 = bool
		if StartSpinElm1 == true then
			local spins = player.statz.spins.Value
			game:GetService('RunService').Stepped:connect(function()
				if spins > resetspinswhenthisnumberreached then
					spins = player.statz.spins.Value
					elmvalue1 = player.statz.main.element1.Value
					elmvalue2 = player.statz.main.element2.Value
					elmvalue3 = player.statz.main.element3.Value
					elmvalue4 = player.statz.main.element4.Value
					if StartSpinElm1 == true then
						if elmvalue1 ~= a1 and elmvalue1 ~= a2 and elmvalue1 ~= a3 and elmvalue1 ~= a4 and elmvalue1 ~= a5 and elmvalue2 ~= a1 and elmvalue2 ~= a2 and elmvalue2 ~= a3 and elmvalue2 ~= a4 and elmvalue2 ~= a5 and elmvalue3 ~= a1 and elmvalue3 ~= a2 and elmvalue3 ~= a3 and elmvalue3 ~= a4 and elmvalue3 ~= a5 and elmvalue4 ~= a1 and elmvalue4 ~= a2 and elmvalue4 ~= a3 and elmvalue4 ~= a4 and elmvalue4 ~= a5 then
							player.startevent:FireServer("spin", "element1")
							print("Rolled: " .. elmvalue1)
							wait(1)
						elseif elmvalue1 ~= a1 and elmvalue1 ~= a2 and elmvalue1 ~= a3 and elmvalue1 ~= a4 and elmvalue1 ~= a5 then
							if Autosave == true then
								datasavedandyoucanredeemcodes = true
							end
						else
							print("You have got: " .. elmvalue1)
							notify("You have got: " .. elmvalue1)
							if Autosave == true then
								datasavedandyoucanredeemcodes = true
							end
						end
					end
				else
					if Autorejoin == true then
						--player.statz.spins:Destroy()
						player.startevent:FireServer("band", "\128")
						wait()
						game:GetService('TeleportService'):Teleport(game.PlaceId, player)
					end
				end
			end)
		else
		end
	end)

	section2:addToggle("Spin Slot 2",false,function(bool)
		StartSpinElm2 = bool
		if StartSpinElm2 == true then
			local spins = player.statz.spins.Value
			game:GetService('RunService').Stepped:connect(function()
				if spins > resetspinswhenthisnumberreached then
					spins = player.statz.spins.Value
					elmvalue1 = player.statz.main.element1.Value
					elmvalue2 = player.statz.main.element2.Value
					elmvalue3 = player.statz.main.element3.Value
					elmvalue4 = player.statz.main.element4.Value
