		setclipboard("https://discord.gg/P2CeRjvTDt")
		wait()
		notify("Discord link copied to clipboard!")
	end)

	-- load
	NoobieDoge:SelectPage(NoobieDoge.pages[1], true)

end

--Shindo Main
local function ShindoMain()
	--DogeHub V2 GUI
	local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/pornhub%20page%20format%20gui"))()
	local NoobieDoge = library.new("DogeHub V2", 5013109572)

	-- themes
	local themes = {
	Background = Color3.fromRGB(24, 24, 24),
	Glow = Color3.fromRGB(0, 0, 0),
	Accent = Color3.fromRGB(10, 10, 10),
	LightContrast = Color3.fromRGB(20, 20, 20),
	DarkContrast = Color3.fromRGB(14, 14, 14),  
	TextColor = Color3.fromRGB(255, 255, 255)
	}

	-- first page
	local page1 = NoobieDoge:addPage("Universal", 5012544693)
	local section1 = page1:addSection("Walk Speed & Jump Power")
	local section2 = page1:addSection("Other Functions")

	section1:addSlider("Walk Speed", 16, 0, 400, function(value)
		sliderspeed = value
	end)
	section1:addSlider("Jump Power", 50, 0, 400, function(value)
		sliderjump = value
	end)

	section1:addToggle("Enabled", nil, function(value)
		sliderpowa = value
	end)
	game:GetService('RunService').Stepped:connect(function()
		if sliderpowa then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = sliderspeed
			game.Players.LocalPlayer.Character.Humanoid.JumpPower = sliderjump
		end
	end)

	section2:addButton("Add ESP GUI", function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Project%3A%20Bullshit%3A%20Rebooted"))()
	end)

	section2:addButton("Reset Character",function()
		game.Players.LocalPlayer.Character:BreakJoints()
	end)

	section2:addButton("Enable Infinite Jump",function()
		getgenv().dc = false
		local infjump
		infJump = game:GetService("UserInputService").JumpRequest:Connect(function()
			if dc then infjump:Disconnect() return end
			game.Players.LocalPlayer.Character.Humanoid:ChangeState("Jumping")
		end)
		notify("Enabled Infinute Jump")
	end)

	section2:addButton("Disable Infinite Jump",function()
		getgenv().dc = true
		notify("Disabled Infinite Jump")
	end)

	section2:addSlider("Fly Speed", 101, 0, 500, function(value)
		sliderfly = value
	end)
	sliderfly = 101
	section2:addToggle("Press P to fly",false,function(Pfly)
		repeat wait() 
		until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:findFirstChild("Head") and game.Players.LocalPlayer.Character:findFirstChild("Humanoid") 
		local mouse = game.Players.LocalPlayer:GetMouse() 
		repeat wait() until mouse
		local plr = game.Players.LocalPlayer 
		local torso = plr.Character.Head 
		local flying = false
		local deb = true 
		local ctrl = {f = 0, b = 0, l = 0, r = 0} 
		local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
		local maxspeed = sliderfly 
		local speed = 0 

		function Fly() 
			local bg = Instance.new("BodyGyro", torso) 
			bg.P = 9e4 
			bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
			bg.cframe = torso.CFrame 

			local bv = Instance.new("BodyVelocity", torso) 
			bv.velocity = Vector3.new(0,0.1,0) 
			bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
			repeat wait() 
				plr.Character.Humanoid.PlatformStand = true 
				if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then 
					speed = speed+(sliderfly/1.7)+(speed/maxspeed) 
					if speed > maxspeed then 
						speed = maxspeed 
					end 

				elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then 
					speed = speed-(sliderfly/1.7)
					if speed < 0 then 
						speed = 0 
					end 
				end 
				if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then 
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
					lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} 
				elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then 
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed 
				else 
					bv.velocity = Vector3.new(0,0.1,0)
				end 
				bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) 
			until not flying 
			ctrl = {f = 0, b = 0, l = 0, r = 0} 
			lastctrl = {f = 0, b = 0, l = 0, r = 0} 
			speed = 0 
			bg:Destroy() 
			bv:Destroy() 
			plr.Character.Humanoid.PlatformStand = false 
		end 

		mouse.KeyDown:connect(function(key) 
			if key:lower() == "p" then 
				if flying then flying = false 
				else 
					flying = true 
					Fly() 
				end 
			elseif key:lower() == "w" then 
				ctrl.f = 1 
			elseif key:lower() == "s" then 
				ctrl.b = -1 
			elseif key:lower() == "a" then 
				ctrl.l = -1 
			elseif key:lower() == "d" then 
				ctrl.r = 1 
			end 
		end) 

		mouse.KeyUp:connect(function(key) 

			if key:lower() == "w" then 
				ctrl.f = 0 
			elseif key:lower() == "s" then 
				ctrl.b = 0 
			elseif key:lower() == "a" then 
				ctrl.l = 0 
			elseif key:lower() == "d" then 
				ctrl.r = 0 
			end 
		end)
		Fly()
		notify("Press P to fly: "..tostring(Pfly),true)
	end)

	local playerlist = {}
	for i,v in pairs(game:GetService("Players"):GetPlayers()) do
		if v.Name ~= game.Players.LocalPlayer.Name then
			table.insert(playerlist, v.Name)
		end
	end
	section2:addDropdown("Track selected player",playerlist,function(ripthatguy)
		notify("Player Selected: " .. ripthatguy)
		T = ripthatguy
	end)

	section2:addToggle("Track Player",false,function(bool)
		trackthatguy = bool
		local platform = Instance.new("Part", player.Character)
		platform.Anchored = true
		platform.Transparency = 1
		platform.Size = Vector3.new(6, 1, 6)

		local connection
		connection = game:GetService("RunService").RenderStepped:connect(function()
			if player.Character:FindFirstChild("HumanoidRootPart") then
				platform.Position = player.Character.HumanoidRootPart.CFrame * Vector3.new(0, -1.8, 0)
			else
				connection:Disconnect()
			end
		end)
		game:GetService('RunService').Stepped:connect(function()
			if trackthatguy ~= true then
				platform:Destroy()
			end
		end)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if trackthatguy == true then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[T].Character.HumanoidRootPart.CFrame * CFrame.new(0, 5, 10)
			game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, game.Players[T].Character.HumanoidRootPart.Position)
		end
	end)

	-- second page
	local page2 = NoobieDoge:addPage("Misc", 5012544693)
	local section1 = page2:addSection("Game Functions")
	local section2 = page2:addSection("Customs")
	local section3 = page2:addSection("Kenjutsu Spin")

	section1:addButton("SuperHuman",function()
		SUPERHUMAN()
	end)

	section1:addButton("Unbreakable Blocks",function()
		UNBREAKABLE_BLOCKS()
	end)

	section1:addButton("Infinite MD",function()
		INFMDON()
	end)

	section1:addToggle("NPC Kill Aura",false,function(bool)
		keel = bool
		notify("NPC Kill Aura: "..tostring(keel),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if keel then
			NPCKEELAURA()
		end
	end)

	local scrollfarm
	section1:addToggle("Scroll Snipe",false, function(bool)
		scrollfarm = bool
	end)

	spawn(function()
		while wait() do
			if scrollfarm then
				repeat wait()
					SCROLLFARM()
					SCROLLFARM1()
				until not scrollfarm
			end
		end
	end)

	section1:addButton("Old No CD Method",function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/HDTerebi/Shindo-Life/main/No%20CD%20Method"))()
		wait()
		notify("Equip a beast mode, mode up, type '!spirit', press the ']' button, done!")
	end)

	section2:addDropdown("Custom Display Ranks",{"RELL", "F 0", "D 1", "D 2", "D 3", "C 1", "C 2", "C 3", "B 1", "B 2", "B 3", "A 1", "A 2", "A 3", "S 1", "S 2", "S 3", "V 1", "V 2", "V 3", "X 1", "X 2", "X 3", "Y 1", "Y 2", "Y 3", "Z 1", "Z 2", "Z 3", "MAX-69", "MAX-420", "MAX-666", "MAX-999", "GOD", "Dragon", "Hacker","???"},function(rektrank)
		game:GetService("Players").LocalPlayer.leaderstats.Rank.Value =  (rektrank)
		notify("Rank Displayed: " .. rektrank)
	end)

	section2:addButton("Bloodline Changer",function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/NoobieDoge/DogeHub/Separated/ShindoBloodlineChanger"))()
	end)

	section2:addButton("RC Shop Changer",function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Shindo%20Loadout%20Changer/Page%20RC%20Shop%20Changer"))()
	end)

	section3:addButton("Reset Spins NOW",function()
		game:GetService("Players").LocalPlayer.startevent:FireServer("band", "\128")
		wait()
		game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
	end)

	section3:addButton("Save Spins Data",function()
		local player = game.Players.LocalPlayer
		local ScreenGuiK = Instance.new("ScreenGui")
		local FrameK = Instance.new("Frame")
		local TextLabel = Instance.new("TextLabel")
		local TextButton = Instance.new("TextButton")
		local TextButton_2 = Instance.new("TextButton")

		ScreenGuiK.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

		FrameK.Parent = ScreenGuiK
		FrameK.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		FrameK.Position = UDim2.new(0.005, 0, 0.7, 0)
		FrameK.Size = UDim2.new(0, 200, 0, 120)

		TextLabel.Parent = FrameK
		TextLabel.BackgroundColor3 = Color3.fromRGB(255, 163, 26)
		TextLabel.Size = UDim2.new(0, 220, 0, 60)
		TextLabel.Font = Enum.Font.TitilliumWeb
		TextLabel.Text = "Are you sure you want to keep the current spins data?"
		TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
		TextLabel.TextScaled = false
		TextLabel.TextSize = 25.000
		TextLabel.TextWrapped = true

		TextButton.Parent = FrameK
		TextButton.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
		TextButton.Position = UDim2.new(0.55, 0, 0.500000006, 0)
		TextButton.Size = UDim2.new(0, 110, 0, 60)
		TextButton.Font = Enum.Font.TitilliumWeb
		TextButton.Text = "Yes"
		TextButton.TextColor3 = Color3.fromRGB(200, 200, 200)
		TextButton.TextScaled = true
		TextButton.TextSize = 40.000
		TextButton.TextWrapped = true

		function OKKeep()
			for i,v in pairs(player.PlayerGui:GetChildren()) do
				if v.Name == "ScreenGui" then
					v:Destroy()
				end
			end
			game:GetService("Players").LocalPlayer.startevent:FireServer("band", "Eye")
			wait()
			datasavedandyoucanredeemcodes = true
		end
		TextButton.MouseButton1Click:connect(OKKeep)

		TextButton_2.Parent = FrameK
		TextButton_2.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
		TextButton_2.Position = UDim2.new(0, 0, 0.500000006, 0)
		TextButton_2.Size = UDim2.new(0, 110, 0, 60)
		TextButton_2.Font = Enum.Font.TitilliumWeb
		TextButton_2.Text = "No"
		TextButton_2.TextColor3 = Color3.fromRGB(200, 200, 200)
		TextButton_2.TextScaled = true
		TextButton_2.TextSize = 40.000
		TextButton_2.TextWrapped = true

		function cancelKeep()
			for i,v in pairs(player.PlayerGui:GetChildren()) do
				if v.Name == "ScreenGui" then
					v:Destroy()
				end
			end
		end
		TextButton_2.MouseButton1Click:connect(cancelKeep)	
	end)

	section3:addToggle("Auto Rejoin",true,function(bool)
		Autorejoin = bool
	end)

	section3:addToggle("Auto Save",false,function(bool)
		Autosave = bool
	end)

	if game:GetService("Players").LocalPlayer.statz.unlocked:FindFirstChild("kenjutsuslot2") then
		local kenselect1
			section3:addDropdown("Kenjutsu Choice 1",ken,function(KEN1)
			print("Selected: " .. KEN1)
			notify("Selected: " .. KEN1)
			FrameK.Visible = true
			kenselect1 = KEN1
		end)
		local kenselect2
			section3:addDropdown("Kenjutsu Choice 2",ken,function(KEN2)
			print("Selected: " .. KEN2)
			notify("Selected: " .. KEN2)
			FrameK.Visible = true
			kenselect2 = KEN2
		end)
		local kenselect3
			section3:addDropdown("Kenjutsu Choice 3",ken,function(KEN3)
			print("Selected: " .. KEN3)
			notify("Selected: " .. KEN3)
			FrameK.Visible = true
			kenselect3 = KEN3
		end)
		local kenselect4
			section3:addDropdown("Kenjutsu Choice 4",ken,function(KEN4)
			print("Selected: " .. KEN4)
			notify("Selected: " .. KEN4)
			FrameK.Visible = true
			kenselect4 = KEN4
		end)
		local kenselect5
			section3:addDropdown("Kenjutsu Choice 3",ken,function(KEN5)
			print("Selected: " .. KEN5)
			notify("Selected: " .. KEN5)
			FrameK.Visible = true
			kenselect5 = KEN5
		end)

		section3:addToggle("Spin Slot 1",false,function(bool)
			game:GetService("Players").LocalPlayer.startevent:FireServer("band", "\128")
			StartSpinKen = bool
			if StartSpinKen == true then
				local spins = game.Players.LocalPlayer.statz.spins.Value
				game:GetService('RunService').Stepped:connect(function()
					if spins > 9 then
						spins = game.Players.LocalPlayer.statz.spins.Value
						kenvalue = game.Players.LocalPlayer.statz.breathingstyle.Value
						kenvalue2 = game.Players.LocalPlayer.statz.breathingstyle2.Value
						if StartSpinKen == true then
							if kenvalue ~= kenselect1 and kenvalue ~= kenselect2 and kenvalue ~= kenselect3 and kenvalue ~= kenselect4 and kenvalue ~= kenselect5 and kenvalue2 ~= kenselect1 and kenvalue2 ~= kenselect2 and kenvalue2 ~= kenselect3 and kenvalue2 ~= kenselect4 and kenvalue2 ~= kenselect5 then
								local args = {[1] = "spinBREATHING"}
								game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
								print("Rolled: " .. kenvalue)
								wait(1)
							elseif kenvalue2 ~= kenselect1 and kenvalue2 ~= kenselect2 and kenvalue2 ~= kenselect3 and kenvalue2 ~= kenselect4 and kenvalue2 ~= kenselect5 then
								if Autosave == true then
									datasavedandyoucanredeemcodes = true
								end
							else
								print("You have got: " .. kenvalue)
								notify("You have got " .. kenvalue .. " in slot 1")
								if Autosave == true then
									datasavedandyoucanredeemcodes = true
								end
							end
						end
					else
						if Autorejoin == true then
							--player.statz.spins:Destroy()
							game:GetService("Players").LocalPlayer.startevent:FireServer("band", "\128")
							wait()
							game:GetService('TeleportService'):Teleport(game.PlaceId, player)
						end
					end
				end)
			else 
			end
		end)

		section3:addToggle("Spin Slot 2",false,function(bool)
			game:GetService("Players").LocalPlayer.startevent:FireServer("band", "\128")
			StartSpinKen2 = bool
			if StartSpinKen2 == true then
				local spins = game.Players.LocalPlayer.statz.spins.Value
				game:GetService('RunService').Stepped:connect(function()
					if spins > 9 then
						spins = game.Players.LocalPlayer.statz.spins.Value
						kenvalue = game.Players.LocalPlayer.statz.breathingstyle.Value
						kenvalue2 = game.Players.LocalPlayer.statz.breathingstyle2.Value
						if StartSpinKen2 == true then
							if kenvalue ~= kenselect1 and kenvalue ~= kenselect2 and kenvalue ~= kenselect3 and kenvalue ~= kenselect4 and kenvalue ~= kenselect5 and kenvalue2 ~= kenselect1 and kenvalue2 ~= kenselect2 and kenvalue2 ~= kenselect3 and kenvalue2 ~= kenselect4 and kenvalue2 ~= kenselect5 then
								local args = {[1] = "spinBREATHING",[2] = "breathingstyle2"}
								game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
								print("Rolled: " .. kenvalue2)
								wait(1)
							elseif kenvalue ~= kenselect1 and kenvalue ~= kenselect2 and kenvalue ~= kenselect3 and kenvalue ~= kenselect4 and kenvalue ~= kenselect5 then
								if Autosave == true then
									datasavedandyoucanredeemcodes = true
								end
							else
								print("You have got: " .. kenvalue2)
								notify("You have got " .. kenvalue2 .. " in slot 2")
								if Autosave == true then
									datasavedandyoucanredeemcodes = true
								end
							end
						end
					else
						if Autorejoin == true then
							--player.statz.spins:Destroy()
							game:GetService("Players").LocalPlayer.startevent:FireServer("band", "\128")
							wait()
							game:GetService('TeleportService'):Teleport(game.PlaceId, player)
						end
					end
				end)
			else 
			end
		end)
	else
		local kenselect1
			section3:addDropdown("Kenjutsu Choice 1",ken,function(KEN1)
			print("Selected: " .. KEN1)
			notify("Selected: " .. KEN1)
			kenselect1 = KEN1
		end)
		local kenselect2
			section3:addDropdown("Kenjutsu Choice 2",ken,function(KEN2)
			print("Selected: " .. KEN2)
			notify("Selected: " .. KEN2)
			kenselect2 = KEN2
		end)
		local kenselect3
			section3:addDropdown("Kenjutsu Choice 3",ken,function(KEN3)
			print("Selected: " .. KEN3)
			notify("Selected: " .. KEN3)
			kenselect3 = KEN3
		end)

		section3:addToggle("Spin Kenjutsu",false,function(bool)
			game:GetService("Players").LocalPlayer.startevent:FireServer("band", "\128")
			StartSpinKen = bool
			if StartSpinKen == true then
				local spins = game.Players.LocalPlayer.statz.spins.Value
				game:GetService('RunService').Stepped:connect(function()
					if spins > 0 then
						spins = game.Players.LocalPlayer.statz.spins.Value
						kenvalue = game.Players.LocalPlayer.statz.breathingstyle.Value
						if StartSpinKen == true then
							if kenvalue ~= kenselect1 and kenvalue ~= kenselect2 and kenvalue ~= kenselect3 then
								local args = {[1] = "spinBREATHING"}
								game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
								print("Rolled: " .. kenvalue)
								notify("Rolled: " .. kenvalue)
								wait(1)
							else
								print("You have got: " .. kenvalue)
								notify("You have got: " .. kenvalue)
								if Autosave == true then
									datasavedandyoucanredeemcodes = true
								end
							end
						end
					else
						if Autorejoin == true then
							--player.statz.spins:Destroy()
							game:GetService("Players").LocalPlayer.startevent:FireServer("band", "\128")
							wait()
							game:GetService('TeleportService'):Teleport(game.PlaceId, player)
						end
					end
				end)
			else 
			end
		end)
	end

	game:GetService('RunService').Stepped:connect(function()
		if datasavedandyoucanredeemcodes == true then
			StartSpinKen = false
			StartSpinKen2 = false
			datasavedandyoucanredeemcodes = false
			game:GetService("Players").LocalPlayer.startevent:FireServer("band", "Eye")
			notify("You will be teleported to My Home in order to save data")
			wait(1)
			local args = {[1] = "arenateleport",[2] = 5451410109}
			game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
		end
	end)

