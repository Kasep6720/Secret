	local function QUEST()
		for i,v in pairs(workspace.missiongivers:GetChildren()) do
			if v.Name == "" and v:FindFirstChild("Head") then
				local TALK = v:FindFirstChild("Talk")
				if v:FindFirstChild("Head") and v.Head:FindFirstChild("givemission") and v.Head.givemission.Enabled and v.Talk.typ.Value == "defeat" and v.Head.givemission:FindFirstChild("color").Image == "http://www.roblox.com/asset/?id=5459241648" then
					if v:WaitForChild("Talk"):WaitForChild("mobname") then
						local getmission = v:FindFirstChild("HumanoidRootPart")
						local clienttalk = v:FindFirstChild("CLIENTTALK")
						repeat
							player.Character.HumanoidRootPart.CFrame=getmission.CFrame+Vector3.new(0,3,0)
							clienttalk:FireServer()
							clienttalk:FireServer("accept")
							wait()
						until mission.Visible or mission.title.text == "[Objective: Mission]" or not mobs
					end
				end
			end
		end
	end

	local function BOSSFARM()
		if random_boss then
			if game.PlaceId == 7923764447 then
				for i,v in pairs(workspace.sengokubossfight.missions:GetChildren()) do
					if v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
						player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
						local args = {[1] = player}
						v.missiongiver.CLIENTTALK:FireServer(unpack(args))
						wait()
					end
				end
			end
			for i,v in pairs(workspace.bossdropmission.missions:GetChildren()) do
				if v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
					player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
					local args = {[1] = player}
					v.missiongiver.CLIENTTALK:FireServer(unpack(args))
					wait()
				end
			end
		elseif specific_boss then
			if game.PlaceId == 7923764447 then
				for i,v in pairs(workspace.sengokubossfight.missions:GetChildren()) do
					if v.Name == selected_boss and v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
						player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
						local args = {[1] = player}
						v.missiongiver.CLIENTTALK:FireServer(unpack(args))
						wait()
					end
				end
			end
			for i,v in pairs(workspace.bossdropmission.missions:GetChildren()) do
				if v.Name == selected_boss and v:FindFirstChild("missiongiver") and v.missiongiver:FindFirstChild("timer") and v.missiongiver.timer:FindFirstChild("time") and v.missiongiver.timer.time:FindFirstChild("time") and (v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
					player.Character.HumanoidRootPart.CFrame=v.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
					local args = {[1] = player}
					v.missiongiver.CLIENTTALK:FireServer(unpack(args))
					wait()
				end
			end
		else
			wait(10)
			notify("Bruh.")
		end
	end

	local function BEASTFARM()
		if random_beasts then
			for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
				if v:FindFirstChild("missions") and v.Name ~= "bossdropmission" and v.Name ~= "sengokubossfight" then
					for i,v1 in pairs(game:GetService("Workspace")[v.Name].missions:GetChildren()) do
						if v1:FindFirstChild("missiongiver") and v1.missiongiver:FindFirstChild("timer") and v1.missiongiver.timer:FindFirstChild("time") and v1.missiongiver.timer.time:FindFirstChild("time") and (v1.missiongiver.timer.time.time.Text == "00:00" or v1.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
							player.Character.HumanoidRootPart.CFrame=v1.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
							local args = {[1] = player}
							v1.missiongiver.CLIENTTALK:FireServer(unpack(args))
							wait()
						end
					end
				end
			end
		elseif specific_beast then
			for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
				if v:FindFirstChild("missions") and v.Name ~= "bossdropmission" and v.Name ~= "sengokubossfight" then
					for i,v1 in pairs(game:GetService("Workspace")[v.Name].missions:GetChildren()) do
						if v1.Name == selected_beast and v1:FindFirstChild("missiongiver") and v1.missiongiver:FindFirstChild("timer") and v1.missiongiver.timer:FindFirstChild("time") and v1.missiongiver.timer.time:FindFirstChild("time") and (v1.missiongiver.timer.time.time.Text == "00:00" or v1.missiongiver.timer.time.time.Text == "59:59") then --and v.missiongiver.timer.time.time.Text == "59:59" then --or v.missiongiver.timer.time.time.Text == "00:00" or v.missiongiver.timer.time.time.Text == "59:59" then
							player.Character.HumanoidRootPart.CFrame=v1.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
							local args = {[1] = player}
							v1.missiongiver.CLIENTTALK:FireServer(unpack(args))
							wait()
						end
					end
				end
			end
		else
			wait(10)
			notify("Bruh.")
		end
	end

	local function FARM()
		if player.statz.sword.throwable.Value == "BOSSDROPazimftg" or player.statz.sword.throwable.Value == "kunaiwire" or player.statz.sword.throwable.Value == "kunaiflash" then
			if player.statz.unlocked:FindFirstChild("painrodthrow") then
				local args = {[1] = "equipweapon",[2] = game:GetService("ReplicatedStorage").saber.powers.painrodthrow}
				player.startevent:FireServer(unpack(args))
				notify("Automatically changed your throwable weapon")
			else
				if player.statz.sword.throwable.Value == "BOSSDROPazimftg" then
					local args = {[1] = "equipweapon",[2] = game:GetService("ReplicatedStorage").saber.powers.BOSSDROPazimftg}
					player.startevent:FireServer(unpack(args))
				elseif player.statz.sword.throwable.Value == "kunaiwire" then
					local args = {[1] = "equipweapon",[2] = game:GetService("ReplicatedStorage").saber.powers.kunaiwire}
					player.startevent:FireServer(unpack(args))
				elseif player.statz.sword.throwable.Value == "kunaiflash" then
					local args = {[1] = "equipweapon",[2] = game:GetService("ReplicatedStorage").saber.powers.kunaiflash}
					player.startevent:FireServer(unpack(args))
				end
				notify("Automatically unequipped your throwable weapon")
			end
		else
			if LFM then
				for i,v in pairs(workspace.npc:GetChildren()) do
					local mobname = string.split(mission.bg.name.Text,"Defeat ")[1]
					if(string.find(mobname,"(s)")) then
						mobname = string.gsub(mobname,"(s)","")
					end
					if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and not v:FindFirstChild("fullsusanoo") and v.Head.CFrame.Y > -1000 then
						repeat
							pcall(function()
								v.Humanoid.Health = 0
								game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
								if player.statz.fightingstyle.Value == "" or player.statz.fightingstyle.Value == "boxing" or player.statz.fightingstyle.Value == "jeetkunedo" or player.statz.fightingstyle.Value == "muaythai" or player.statz.fightingstyle.Value == "mma" then
									player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 0, 7)
								else
									player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
								end
								player.Character.combat.update:FireServer("key","e")
								player.Character.combat.update:FireServer("key","eend")
								player.Character.combat.update:FireServer("mouse1",true)
								wait(.000000000000001)
								v.Humanoid.Health = 0
							end)
						until not v:FindFirstChild("Humanoid") or mission.Visible == false or (not autofarm and not boss and not megaboss) or not v
					end
				end
			elseif SpamGOBRRRRR and SpamOnlyInMissions then
				for i,v in pairs(workspace.npc:GetChildren()) do
					local mobname = string.split(mission.bg.name.Text,"Defeat ")[1]
					if(string.find(mobname,"(s)")) then
						mobname = string.gsub(mobname,"(s)","")
					end
					if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and not v:FindFirstChild("fullsusanoo") and v.Head.CFrame.Y > -1000 then
						player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 15, 0)
						repeat wait()
							pcall(function()
								game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
								v.Humanoid.Health = 0
								pcall(function()
									player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, AFHeight, AFDistance)
									if E then SpamE() end
									if R then SpamR() end
									if T then SpamT() end
									if Y then SpamY() end
									if F then SpamF() end
									if G then SpamG() end
									if H then SpamH() end
									if V then SpamV() end
									if B then SpamB() end
									if N then SpamN() end
									if Z then SpamZ() end
									if C then SpamC() end
									if Q then SpamQ() end
									if ClickRATATA then SpamClick() end
									v.Humanoid.Health = 0
								end)
							end)
						until not v:FindFirstChild("Humanoid") or mission.Visible == false or (not autofarm and not boss and not megaboss) or not v
					end
				end
			else
				for i,v in pairs(workspace.npc:GetChildren()) do
					local mobname = string.split(mission.bg.name.Text,"Defeat ")[1]
					if(string.find(mobname,"(s)")) then
						mobname = string.gsub(mobname,"(s)","")
					end
					if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and not v:FindFirstChild("fullsusanoo") and v.Head.CFrame.Y > -1000 then
						player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 15, 0)
						repeat wait()
							pcall(function()
								game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
								v.Humanoid.Health = 0
								pcall(function()
									player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, AFHeight, AFDistance)
									player.Character.combat.update:FireServer("key","e")
									player.Character.combat.update:FireServer("key","eend")
									player.Character.combat.update:FireServer("key","e")
									player.Character.combat.update:FireServer("key","eend")
									player.Character.combat.update:FireServer("key","e")
									player.Character.combat.update:FireServer("key","eend")
									player.Character.combat.update:FireServer("key","e")
									player.Character.combat.update:FireServer("key","eend")
									v.Humanoid.Health = 0
								end)
							end)
						until not v:FindFirstChild("Humanoid") or mission.Visible == false or (not autofarm and not boss and not megaboss) or not v
					end
				end
			end
		end
	end

	local count
	local function DISP()
		count = 0
		for i,v in pairs(workspace.missiongivers:GetChildren()) do
			if v.Name == "" and v:FindFirstChild("Head") then
				if v:FindFirstChild("Head") and v.Head:FindFirstChild("givemission") and v.Head.givemission.Enabled and v.Head.givemission:FindFirstChild("color") and v.Head.givemission.color.Image == "http://www.roblox.com/asset/?id=5459241648" then
					count = count + 1
				end
			end
		end
	end

	spawn(function()
		while wait() do
			if mobs or boss or megaboss then
				mission = player.PlayerGui:WaitForChild("Main"):WaitForChild("ingame"):WaitForChild("Missionstory")
				if (mission.Visible == true and mission.title.text == "[Objective: Mission]") then
					FARM()
					for i,v in pairs(workspace.npc:GetChildren()) do
						if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
							repeat
								v.Humanoid.Health = 0
								v.Humanoid.Health = 0
								v.Humanoid.Health = 0
								v.Humanoid.Health = 0
								v.Humanoid.Health = 0
							until (not mobs and not boss and not megaboss) or v.Humanoid.Health == 0
						end
					end
				else
					if megaboss and boss and mobs then
						BEASTFARM()
						wait(.1)
						BOSSFARM()
						wait(.1)
						DISP()
						wait(.1)
						QUEST()
					elseif boss and megaboss then
						BEASTFARM()
						wait(.1)
						BOSSFARM()
					elseif megaboss and mobs then
						BEASTFARM()
						wait(.1)
						DISP()
						wait(.1)
						QUEST()
					elseif boss and mobs then
						BOSSFARM()
						wait(.1)
						DISP()
						wait(.1)
						QUEST()
					elseif megaboss then
						BEASTFARM()
					elseif boss then
						BOSSFARM()
					elseif mobs then
						DISP()
						wait(.1)
						QUEST()
					end
				end
			end
		end
	end)

	spawn(function()
		while wait() do
			if mobs then
				if count == 0 then
					if QUEST() or FARM() or BOSSFARM() then
						wait(5.5)
					else
						repeat wait()
							notify("Currently no green scroll quests avaliable")
							wait(2)
						until count > 0 or QUEST() or FARM() or BOSSFARM() or not mobs
					end
				end
			end
		end
	end)

	local PRES = player.statz.prestige.maxlvlpres
	local RANK = player.statz.prestige.rank
	local RANKNUM = player.statz.prestige.number

	spawn(function()
		while wait() do
			if RANKUPP then
				if NUMTHENSTOPRANKUPP and player.statz.spins.Value > (stopautorankwhenthisnumberreached - 1) then
					RANKUPP = false
					section5:updateToggle("Auto Rank", false)
					notify("Spins amount has currently reached " .. stopautorankwhenthisnumberreached)
				else
					if MAXRANKUPP then
						if RANKUPPBYRC then
							if player.PlayerGui.notifui:FindFirstChild("keymove") and player.PlayerGui.notifui.keymove.Tex.text == "Not Enough RELL Coin." then
								wait()
								notify("Out of RELL Coins")
								wait()
								RANKUPP = false
								section5:updateToggle("Auto Rank", false)
								RANKUPPBYRC = false
								section6:updateToggle("Rank Up with RELL Coins", false)
								--patched --loadstring(game:HttpGet('https://raw.githubusercontent.com/KylnDantas/InfRellCoinsSL/main/file.lua',true))()
							else
								player.startevent:FireServer("maxlvlpres2")
							end
						end
						if RANKUPPBYLVL then
							if player.statz.lvl:FindFirstChild("lvl").Value == 1000 then
								repeat wait()
									player.startevent:FireServer("maxlvlpres")
								until player.statz.lvl:FindFirstChild("lvl").Value == 1 or not RANKUPP or not MAXRANKUPP
							end
						else
							notify("Bruh.")
						end
					else
						if RANKUPPBYRC then
							if player.PlayerGui.notifui:FindFirstChild("keymove") and player.PlayerGui.notifui.keymove.Tex.text == "Not Enough RELL Coin." then
								wait()
								notify("Out of RELL Coins")
								wait()
								RANKUPP = false
								section5:updateToggle("Auto Rank", false)
								RANKUPPBYRC = false
								section6:updateToggle("Rank Up with RELL Coins", false)
								--patched --loadstring(game:HttpGet('https://raw.githubusercontent.com/KylnDantas/InfRellCoinsSL/main/file.lua',true))()
							else
								player.startevent:FireServer("maxlvlpres2")
							end
						end
						if RANKUPPBYLVL then
							if player.statz.lvl:FindFirstChild("lvl").Value == 1000 then
								repeat wait()
									player.startevent:FireServer("rankup")
								until player.statz.lvl:FindFirstChild("lvl").Value == 1 or not RANKUPP
							end
						else
							notify("Bruh.")
						end
					end
				end
			end
		end
	end)

