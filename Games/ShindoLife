				workspace.sensei.missions:FindFirstChild("Jiso Seishin").missiongiver.CLIENTTALK:FireServer(unpack(args))
				player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Jiso Seishin"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(5,0,-1.7)
			else
				notify("Jiso Seishin Mentor is not in this map!")
			end
		end)
		section1:addButton("Tp to Shindai Mentor",function()
			if game.PlaceId == 5451401540 then
				local args = {[1] = game:GetService("Players").LocalPlayer}
				workspace.sensei.missions:FindFirstChild("Shindai Akuma").missiongiver.CLIENTTALK:FireServer(unpack(args))
				player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Shindai Akuma"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-4,0)
			else
				notify("Shindai Akuma Mentor is not in this map!")
			end
		end)
		section1:addButton("Tp to Ryuji Mentor",function()
			if game.PlaceId == 5431071837 then
				local args = {[1] = game:GetService("Players").LocalPlayer}
				workspace.sensei.missions:FindFirstChild("Ryuji Kenichi").missiongiver.CLIENTTALK:FireServer(unpack(args))
				player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").sensei.missions["Ryuji Kenichi"].missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,4,3)
			else
				notify("Ryuji Kenichi Mentor is not in this map!")
			end
		end)
		section1:addButton("Tp to Kenjutsu Mentor",function()
			if game.PlaceId == 4601350656 then
				for v = 1,8 do
					wait()
					game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, game:GetService("Workspace")["Ken Mentor"].HumanoidRootPart.Position)
				end
				player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace")["Ken Mentor"].HumanoidRootPart.CFrame+Vector3.new(-6,4,0)
			else
				notify("Kenjutsu Mentor is not in this map!")
			end
		end)

