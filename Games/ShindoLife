	section4:addToggle("Spin Race",false,function(bool)
		StartSpinRace = bool
		if StartSpinRace == true then
			local rcamount = player.PlayerGui.Main.Ryo2.amt.text,""
			if(string.find(rcamount,"(K)")) then
				rcamount = (string.gsub(rcamount,"(K)",""))
				rcamount = tonumber(rcamount)*1000
				print(rcamount)
			else
				rcamount = string.gsub(rcamount)	
				print(rcamount)
			end
			game:GetService('RunService').Stepped:connect(function()
				if rcamount > 20000 then
					rcamount = player.PlayerGui.Main.Ryo2.amt.text,""
					if(string.find(rcamount,"(K)")) then
						rcamount = (string.gsub(rcamount,"(K)",""))
						rcamount = tonumber(rcamount)*1000
						print(rcamount)
					else
						rcamount = string.gsub(rcamount)	
						print(rcamount)
					end
					racevalue = player.statz.race.Value
					if StartSpinRace == true then
						if racevalue ~= raceselect1 and racevalue ~= raceselect2 and racevalue ~= raceselect3 then
							local args = {[1]="spinrace"}
							game:GetService("Players").LocalPlayer.startevent:FireServer(unpack(args))
							print("Rolled: " .. racevalue)
							notify("Rolled: " .. racevalue)
							wait(1)
						else
							print("You have got: " .. racevalue)
							notify("You have got: " .. racevalue)
						end
					end
				end
			end)
		else
		end
	end)
		
