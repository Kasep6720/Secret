			game.Players.LocalPlayer.Character.combat.update:FireServer("takemovement2")
		end
	end)
	section2:addToggle("Daily Boss Quests",false, function(bool)
		DailyBoss = bool
		notify("Daily Boss Quests: "..tostring(DailyBoss),true)
	end)
	game:GetService('RunService').Stepped:connect(function()
		if DailyBoss then
			for i,v in pairs(game.Players.LocalPlayer.statz.mission:GetChildren()) do
				if v.Value == "fightboss" then
					if game.Workspace:FindFirstChild("bossdropmission") then
						for i,v0 in pairs(workspace.bossdropmission.missions:GetChildren()) do
							if v0.Name == v.objective.Value and v0:FindFirstChild("missiongiver") then
								if mission.Visible == true then
									for i,v in pairs(workspace.npc:GetChildren()) do
										if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and not v:FindFirstChild("fullsusanoo") and v.Head.CFrame.Y > -1000 then
											repeat
												pcall(function()
													v.Humanoid.Health = 0
													game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
													player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 0, 10)
													game.Players.LocalPlayer.Character.combat.update:FireServer("key","e")
													game.Players.LocalPlayer.Character.combat.update:FireServer("key","eend")
													game.Players.LocalPlayer.Character.combat.update:FireServer("mouse1",true)
													wait(.000000000000001)
													v.Humanoid.Health = 0
												end)
											until not v:FindFirstChild("Humanoid") or mission.Visible == false or not autooofarm or not v
										end
									end
								else
									for i,v1 in pairs(workspace.bossdropmission.missions:GetChildren()) do
										if v1.Name == v.objective.Value and (v1.missiongiver.timer.time.time.Text == "00:00" or v1.missiongiver.timer.time.time.Text == "59:59") then
											player.Character.HumanoidRootPart.CFrame=v1.missiongiver.HumanoidRootPart.CFrame+Vector3.new(0,-0.8,0)
											local args = {[1] = game:GetService("Players").LocalPlayer}
											v1.missiongiver.CLIENTTALK:FireServer(unpack(args))
											wait()
										end
									end
								end
							else
								notify("The boss doesn't spawn in this map!")
								wait()
								DailyBoss = false
							end
						end
					else
						notify("The boss doesn't spawn in this map!")
						wait()
						DailyBoss = false
					end		
				end
			end
		end
	end)

	section3:addButton("Tp to logs",function()
		if not game.Workspace.npc:FindFirstChild("logtraining") then
			notify("This map does not have any logs!")
		else
			for i,v in pairs(workspace.npc:GetChildren()) do
				if v.Name == "logtraining" and v.Head.CFrame.Y > -1000 then
					game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
					player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 2, 10)
				end
			end
		end
	end)

	section3:addButton("Tp to duel pads",function()
		if game:GetService("Workspace").map2:FindFirstChild("duel") and game:GetService("Workspace").map2.duel.duel1:FindFirstChild("pad") then
			for i,v in pairs(workspace.map2.duel.duel1:GetChildren()) do
				if v.Name == "pad" then
					player.Character.HumanoidRootPart.CFrame=v.CFrame * CFrame.new(0, 2, 0)
				end
			end
		else
			notify("This map does not have any duel pads!")
		end
	end)

