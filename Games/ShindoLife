--Shindo Inf Spin
local function ShindoInfSpin()
	--Kenjutsu GUI
	local player = game.Players.LocalPlayer
	local ScreenGui4Ken = Instance.new("ScreenGui")
	local FrameK = Instance.new("Frame")
	local TextLabel = Instance.new("TextLabel")
	local TextLabel2 = Instance.new("TextLabel")
	local TextLabel3 = Instance.new("TextLabel")

	ScreenGui4Ken.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

	FrameK.Parent = ScreenGui4Ken
	FrameK.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	FrameK.Position = UDim2.new(0.82, 0, 0.4, 0)
	FrameK.Size = UDim2.new(0, 220, 0, 120)
	FrameK.Active = true
	FrameK.Draggable = true
	FrameK.Visible = false

	TextLabel.Parent = FrameK
	TextLabel.BackgroundColor3 = Color3.fromRGB(255, 163, 26)
	TextLabel.Size = UDim2.new(0, 220, 0, 30)
	TextLabel.Font = Enum.Font.TitilliumWeb
	TextLabel.Text = "Current Kenjutsu:"
	TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel.TextScaled = false
	TextLabel.TextSize = 25.000
	TextLabel.TextWrapped = true

	TextLabel2.Parent = FrameK
	TextLabel2.BackgroundColor3 = Color3.fromRGB(41, 41, 41)
	TextLabel2.Size = UDim2.new(0, 220, 0, 45)
	TextLabel2.Position = UDim2.new(0, 0, 0.25, 0)
	TextLabel2.Font = Enum.Font.TitilliumWeb
	TextLabel2.Text = "Kenjutsu Slot 1:" .. " " .. game.Players.LocalPlayer.statz.breathingstyle.Value
	TextLabel2.TextColor3 = Color3.fromRGB(999,999,999)
	TextLabel2.TextScaled = true
	TextLabel2.TextSize = 25.000
	TextLabel2.TextWrapped = true

	TextLabel3.Parent = FrameK
	TextLabel3.BackgroundColor3 = Color3.fromRGB(41, 41, 41)
	TextLabel3.Size = UDim2.new(0, 220, 0, 45)
	TextLabel3.Position = UDim2.new(0, 0, 0.615, 0)
	TextLabel3.Font = Enum.Font.TitilliumWeb
	TextLabel3.Text = "Kenjutsu Slot 2:" .. " " .. game.Players.LocalPlayer.statz.breathingstyle2.Value
	TextLabel3.TextColor3 = Color3.fromRGB(999,999,999)
	TextLabel3.TextScaled = true
	TextLabel3.TextSize = 25.000
	TextLabel3.TextWrapped = true

	game:GetService('RunService').Stepped:connect(function()
		TextLabel2.Text = "Kenjutsu Slot 1:" .. " " .. game.Players.LocalPlayer.statz.breathingstyle.Value
		TextLabel3.Text = "Kenjutsu Slot 2:" .. " " .. game.Players.LocalPlayer.statz.breathingstyle2.Value
	end)

	--DogeHub V2 GUI
	local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/pornhub%20page%20format%20gui"))()
	local NoobieDoge = library.new("DogeHub V2", 5013109572)

	-- themes
	local themes = {
	Background = Color3.fromRGB(24, 24, 24),
	Glow = Color3.fromRGB(0, 0, 0),
	Accent = Color3.fromRGB(10, 10, 10),
	LightContrast = Color3.fromRGB(20, 20, 20),
	DarkContrast = Color3.fromRGB(14, 14, 14),  
	TextColor = Color3.fromRGB(255, 255, 255)
	}

	-- first page
	local page1 = NoobieDoge:addPage("Infinite Spin", 5012544693)
	local section1 = page1:addSection("Instructions")
	local section2 = page1:addSection("Utility Functions")
	local section3 = page1:addSection("Inf Spin Functions")
	local section4 = page1:addSection("Inf Spin Settings")
	
	section1:addButton("Instructions",function()
		local player = game.Players.LocalPlayer
		local ScreenGuiK = Instance.new("ScreenGui")
		local FrameK = Instance.new("Frame")
		local ScrollingFrame = Instance.new("ScrollingFrame")
		local TextLabel = Instance.new("TextLabel")
		local TextButton = Instance.new("TextButton")

		ScreenGuiK.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

		FrameK.Parent = ScreenGuiK
		FrameK.BackgroundColor3 = Color3.fromRGB(255, 163, 26)
		FrameK.Position = UDim2.new(0.005, 0, 0.7, 0)
		FrameK.Size = UDim2.new(0, 200, 0, 180)
		FrameK.Active = true
		FrameK.Draggable = true

		ScrollingFrame.Parent = FrameK
		ScrollingFrame.BackgroundColor3 = Color3.new(0.27, 0.27, 0.27)
		ScrollingFrame.BackgroundTransparency = 0
		ScrollingFrame.BorderSizePixel = 0
		ScrollingFrame.Position = UDim2.new(0.025, 0, 0.15, 0)
		ScrollingFrame.Size = UDim2.new(0, 192, 0, 150)
		ScrollingFrame.CanvasSize = UDim2.new(0, 0, 5, 0)
		ScrollingFrame.ScrollBarThickness = 5

		TextLabel.Parent = ScrollingFrame
		TextLabel.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
		TextLabel.Size = UDim2.new(0, 190, 0, 900)
		TextLabel.Font = Enum.Font.TitilliumWeb
		TextLabel.Text = [[To use this inf spins script:

		1.Choose the bloodline or element or kenjutsu that you want to get, you can choose no more than 5 choices for bloodlines and elements and no more than 3 choices for kenjutsu (5 choices when purchased 2nd slot).
		
		2.Press spin the slots that you want to have the bloodline or element or kenjutsu.

		3.Wait patiently and DO NOT change anything until you got the stuff that you want.
		
		4.When you got the bloodline or element or kenjutsu that you want, you may press save data then leave this server to save it.
		
		5.I've already put all spinnable limiteds on top of the dropdown, please don't be stupid and try to spin a limited bloodline in element slots or whatever.
		
		6.DO NOT redeem codes before pressing save data

		7.If you are unsatisfied about the results, you may always press Reset Spins NOW to reset, unless you pressed save spins data or turned on auto save.

		8.Press Right Ctrl to show/hide GUI]]
		TextLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
		TextLabel.TextScaled = true
		TextLabel.TextSize = 20.000
		TextLabel.TextWrapped = true
		TextLabel.TextXAlignment = Enum.TextXAlignment.Left
		TextLabel.TextYAlignment = Enum.TextYAlignment.Top

		TextButton.Parent = FrameK
		TextButton.BackgroundColor3 = Color3.new(0.255, 0.163, 0.26)
		TextButton.BackgroundTransparency = 1
		TextButton.BorderSizePixel = 0
		TextButton.Position = UDim2.new(0.85, 0, 0, 0)
		TextButton.Size = UDim2.new(0, 31, 0, 20)
		TextButton.Font = Enum.Font.Cartoon
		TextButton.Text = "X"
		TextButton.TextColor3 = Color3.new(0, 0, 0)
		TextButton.TextScaled = true
		TextButton.TextSize = 14
		TextButton.TextWrapped = true
		TextButton.MouseButton1Click:connect(function()
			for i,v in pairs(player.PlayerGui:GetChildren()) do
				if v.Name == "ScreenGui" then
					v:Destroy()
				end
			end
		end)
	end)

	section2:addButton("RC Shop Changer",function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Functions/Shindo%20Loadout%20Changer/Page%20RC%20Shop%20Changer"))()
	end)

	section3:addButton("Reset Spins NOW",function()	
		game:GetService("Players").LocalPlayer.startevent:FireServer("band", "\128")
		wait()
		game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
	end)

	section3:addButton("Save Spins Data",function()
		local player = game.Players.LocalPlayer
		local ScreenGuiK = Instance.new("ScreenGui")
		local FrameK = Instance.new("Frame")
		local TextLabel = Instance.new("TextLabel")
		local TextButton = Instance.new("TextButton")
		local TextButton_2 = Instance.new("TextButton")

		ScreenGuiK.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

		FrameK.Parent = ScreenGuiK
		FrameK.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		FrameK.Position = UDim2.new(0.005, 0, 0.7, 0)
		FrameK.Size = UDim2.new(0, 200, 0, 120)

		TextLabel.Parent = FrameK
		TextLabel.BackgroundColor3 = Color3.fromRGB(255, 163, 26)
		TextLabel.Size = UDim2.new(0, 220, 0, 60)
		TextLabel.Font = Enum.Font.TitilliumWeb
		TextLabel.Text = "Are you sure you want to keep the current spins data?"
		TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
		TextLabel.TextScaled = false
		TextLabel.TextSize = 25.000
		TextLabel.TextWrapped = true

		TextButton.Parent = FrameK
		TextButton.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
		TextButton.Position = UDim2.new(0.55, 0, 0.500000006, 0)
		TextButton.Size = UDim2.new(0, 110, 0, 60)
		TextButton.Font = Enum.Font.TitilliumWeb
		TextButton.Text = "Yes"
		TextButton.TextColor3 = Color3.fromRGB(200, 200, 200)
		TextButton.TextScaled = true
		TextButton.TextSize = 40.000
		TextButton.TextWrapped = true

		function OKKeep()
			for i,v in pairs(player.PlayerGui:GetChildren()) do
				if v.Name == "ScreenGui" then
					v:Destroy()
				end
			end
			game:GetService("Players").LocalPlayer.startevent:FireServer("band", "Eye")
			wait()
			datasavedandyoucanredeemcodes = true
		end
		TextButton.MouseButton1Click:connect(OKKeep)

		TextButton_2.Parent = FrameK
		TextButton_2.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
		TextButton_2.Position = UDim2.new(0, 0, 0.500000006, 0)
		TextButton_2.Size = UDim2.new(0, 110, 0, 60)
		TextButton_2.Font = Enum.Font.TitilliumWeb
		TextButton_2.Text = "No"
		TextButton_2.TextColor3 = Color3.fromRGB(200, 200, 200)
		TextButton_2.TextScaled = true
		TextButton_2.TextSize = 40.000
		TextButton_2.TextWrapped = true

		function cancelKeep()
			for i,v in pairs(player.PlayerGui:GetChildren()) do
				if v.Name == "ScreenGui" then
					v:Destroy()
				end
			end
		end
		TextButton_2.MouseButton1Click:connect(cancelKeep)	
	end)

	section4:addToggle("Auto Rejoin",true,function(bool)
		Autorejoin = bool
	end)

	section4:addToggle("Auto Save",false,function(bool)
		Autosave = bool
	end)

	local elm = {}
	for i,v in pairs(game:GetService("Workspace").RAREKGS:GetChildren()) do
		if v.tim.Value ~= "N/A" then
			table.insert(elm, v.Name)
		end
	end
	for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
		if v:FindFirstChild("ELEMENT") and not v:FindFirstChild("limited") then
			table.insert(elm, v.Name)
		end
	end

	local kgs = {}
	for i,v in pairs(game:GetService("Workspace").RAREKGS:GetChildren()) do
		if v.tim.Value ~= "N/A" then
			table.insert(kgs, v.Name)
		end
	end
	for i,v in pairs(game:GetService("ReplicatedStorage").alljutsu:GetChildren()) do
		if (v:FindFirstChild("KG") and not v:FindFirstChild("cantspin") and not v:FindFirstChild("limited")) then
			table.insert(kgs, v.Name)
		end
	end

	-- second page
	local page2 = NoobieDoge:addPage("Elements", 5012544693)
	local section1 = page2:addSection("Choose wanted elements")
	local section2 = page2:addSection("Start Spinning")

	local a1
	section1:addDropdown("Element Choice 1",elm,function(KG1)
		print("Selected: " .. KG1)
		notify("1st Element Selected: " .. KG1)
		a1 = KG1
	end)
	local a2
