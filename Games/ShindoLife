				if spins > resetspinswhenthisnumberreached then
					spins = player.statz.spins.Value
					kgvalue1 = player.statz.main.kg1.Value
					kgvalue2 = player.statz.main.kg2.Value
					kgvalue3 = player.statz.main.kg3.Value
					kgvalue4 = player.statz.main.kg4.Value
					if AutoSpinBL1 == true then
						if kgvalue1 ~= A1 and kgvalue1 ~= A2 and kgvalue1 ~= A3 and kgvalue1 ~= A4 and kgvalue1 ~= A5 and kgvalue2 ~= A1 and kgvalue2 ~= A2 and kgvalue2 ~= A3 and kgvalue2 ~= A4 and kgvalue2 ~= A5 and kgvalue3 ~= A1 and kgvalue3 ~= A2 and kgvalue3 ~= A3 and kgvalue3 ~= A4 and kgvalue3 ~= A5 and kgvalue4 ~= A1 and kgvalue4 ~= A2 and kgvalue4 ~= A3 and kgvalue4 ~= A4 and kgvalue4 ~= A5 then
							player.startevent:FireServer("spin", "kg1")
							print("Rolled: " .. kgvalue1)
							wait(1)
						elseif kgvalue1 ~= A1 and kgvalue1 ~= A2 and kgvalue1 ~= A3 and kgvalue1 ~= A4 and kgvalue1 ~= A5 then
							if Autosave == true then
								datasavedandyoucanredeemcodes = true
							end
