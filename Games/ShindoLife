	-- forth page
	local page4 = NoobieDoge:addPage("Auto Farm", 5012544693)
	local section1 = page4:addSection("Auto Farm")
	local section2 = page4:addSection("Farming Configurations")

	section1:addToggle("Auto Farm",false,function(bool)
		EventBossUded = bool
		local platform = Instance.new("Part", player.Character)
		platform.Anchored = true
		platform.Transparency = 1
		platform.Size = Vector3.new(6, 1, 6)

		local connection
		connection = game:GetService("RunService").RenderStepped:connect(function()
			if player.Character:FindFirstChild("HumanoidRootPart") then
				platform.Position = player.Character.HumanoidRootPart.CFrame * Vector3.new(0, -1.8, 0)
			else
				connection:Disconnect()
			end
		end)
		game:GetService('RunService').Stepped:connect(function()
			if EventBossUded ~= true then
				platform:Destroy()
			end
		end)
	end)

	section1:addToggle("Camera lock to boss",false,function(bool)
		Aimyom = bool
	end)

	game:GetService('RunService').Stepped:connect(function()
		if Aimyom then
			for i,v in pairs(workspace.npc:GetChildren()) do
				if v.ClassName == "Model" and v:FindFirstChild("timemegaboss") and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
					game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0).Position)
				end
			end
		end
	end)

	section2:addToggle("Location: High",false,function(bool)
		High = bool
	end)

	section2:addToggle("Location: Mid",true, function(bool)
		Mid = bool
	end)

	section2:addToggle("Location: Low",false, function(bool)
		Low = bool
	end)

	section2:addToggle("Location: Really Low",false, function(bool)
		RLow = bool
	end)
	Mid = true

	game:GetService('RunService').Stepped:connect(function()
		if EventBossUded then
			if High or Mid or Low or RLow then
				for i,v in pairs(workspace.npc:GetChildren()) do
					if v.ClassName == "Model" and v:FindFirstChild("timemegaboss") and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
						repeat wait()
							if v.rest.Value == true then
								if High then
									player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 100, 50)
								elseif Mid then
									player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 50, 30)
								elseif Low then
									player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 15, 14)
								elseif RLow then	
									player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 8, 5)
								end
							else
								player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame * CFrame.new(0, 220, 0)
							end
						until not EventBossUded
					end
				end
			else
				notify("Please select a farming position!")
			end
		end
	end)

