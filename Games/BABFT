	end)
	Target = readfile(Universal .. "/ripthatguy.txt")

	if readfile(Universal .. "/trackthatguy.txt") == "true" then
		local trackthatguy = true
		section2:addToggle("Track Player",true,function(bool)
			trackthatguy = bool
			if trackthatguy == true then
				writefile(Universal .. "/trackthatguy.txt","true")
			else
				writefile(Universal .. "/trackthatguy.txt","false")
			end
			local platform = Instance.new("Part", player.Character)
			platform.Anchored = true
			platform.Transparency = 1
			platform.Size = Vector3.new(6, 1, 6)

			local connection
			connection = game:GetService("RunService").RenderStepped:connect(function()
				if player.Character:FindFirstChild("HumanoidRootPart") and game.Players:FindFirstChild(Target) then
					platform.Position = player.Character.HumanoidRootPart.CFrame * Vector3.new(0, -1.8, 0)
				else
					connection:Disconnect()
				end
			end)
			game:GetService('RunService').Stepped:connect(function()
				if trackthatguy ~= true then
					platform:Destroy()
				end
			end)
		end)
		trackthatguy = true
	else
		local trackthatguy = false
		section2:addToggle("Track Player",false,function(bool)
			trackthatguy = bool
			if trackthatguy == true then
				writefile(Universal .. "/trackthatguy.txt","true")
			else
				writefile(Universal .. "/trackthatguy.txt","false")
			end
			local platform = Instance.new("Part", player.Character)
			platform.Anchored = true
			platform.Transparency = 1
			platform.Size = Vector3.new(6, 1, 6)

			local connection
			connection = game:GetService("RunService").RenderStepped:connect(function()
				if player.Character:FindFirstChild("HumanoidRootPart") and game.Players:FindFirstChild(Target) then
					platform.Position = player.Character.HumanoidRootPart.CFrame * Vector3.new(0, -1.8, 0)
				else
					connection:Disconnect()
				end
			end)
			game:GetService('RunService').Stepped:connect(function()
				if trackthatguy ~= true then
					platform:Destroy()
				end
			end)
		end)
	end

	game:GetService('RunService').Stepped:connect(function()
		if trackthatguy == true or readfile(Universal .. "/trackthatguy.txt") == "true" then
			if player.Character:FindFirstChild("HumanoidRootPart") and game.Players:FindFirstChild(Target) then
				player.Character.HumanoidRootPart.CFrame = game.Players[Target].Character.HumanoidRootPart.CFrame * CFrame.new(0, 5, 10)
				game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, game.Players[Target].Character.HumanoidRootPart.Position)
			end
		end
	end)
	
	-- second page
	local page2 = NoobieDoge:addPage("Misc", 6052531413)
	local section1 = page2:addSection("Utilities")

	if readfile(DHV2BABFT .. "/Icanswimlol.txt") == "true" then
		section1:addToggle("Water GodMode", true, function(bool)
			Icanswimlol = bool
			if Icanswimlol == true then
				writefile(DHV2BABFT .. "/Icanswimlol.txt","true")
			else
				writefile(DHV2BABFT .. "/Icanswimlol.txt","false")
			end
		end)
		Icanswimlol = true
	else
		section1:addToggle("Suck Out Water", false, function(bool)
			Icanswimlol = bool
			if Icanswimlol == true then
				writefile(DHV2BABFT .. "/Icanswimlol.txt","true")
			else
				writefile(DHV2BABFT .. "/Icanswimlol.txt","false")
			end
		end)
	end
	game:GetService("RunService").Stepped:connect(function()
		pcall(function()
			if Icanswimlol then
				if player.Character:FindFirstChild("WaterDetector") then
					player.Character.WaterDetector:remove()
				end
				if Workspace:FindFirstChild("Water") then
					Workspace.Water.Shape = Enum.PartType.Ball
				end
				for i,v in pairs(workspace.BoatStages.NormalStages:GetChildren()) do
					if v:FindFirstChild("Water") then
						for i,v1 in pairs(v:GetChildren()) do
							if v1.Name == "Water" then
								v1.Shape = Enum.PartType.Ball
							end
						end
					end
				end
				for i,v in pairs(workspace.BoatStages.OtherStages:GetChildren()) do
					if v:FindFirstChild("Water") then
						v.Water.Shape = Enum.PartType.Ball
					end
					if v.Name == "GearStage" then
						if v:FindFirstChild("TerrainWall1") and v.TerrainWall1:FindFirstChild("Water") then
							v.TerrainWall1.Water.Shape = Enum.PartType.Ball
						end
						if v:FindFirstChild("TerrainWall2") and v.TerrainWall2:FindFirstChild("Water") then
							v.TerrainWall2.Water.Shape = Enum.PartType.Ball
						end
					end
				end
			else
				if Workspace:FindFirstChild("Water") then
					Workspace.Water.Shape = Enum.PartType.Block
				end
				for i,v in pairs(workspace.BoatStages.NormalStages:GetChildren()) do
					if v:FindFirstChild("Water") then
						for i,v1 in pairs(v:GetChildren()) do
							if v1.Name == "Water" then
								v1.Shape = Enum.PartType.Block
							end
						end
					end
				end
				for i,v in pairs(workspace.BoatStages.OtherStages:GetChildren()) do
					if v:FindFirstChild("Water") then
						v.Water.Shape = Enum.PartType.Block
					end
					if v.Name == "GearStage" then
						if v:FindFirstChild("TerrainWall1") and v.TerrainWall1:FindFirstChild("Water") then
							v.TerrainWall1.Water.Shape = Enum.PartType.Block
						end
						if v:FindFirstChild("TerrainWall2") and v.TerrainWall2:FindFirstChild("Water") then
							v.TerrainWall2.Water.Shape = Enum.PartType.Block
						end
					end
				end
			end
		end)
	end)

	-- third page
	local page3 = NoobieDoge:addPage("Auto Farm", 6052531413)
	local section1 = page3:addSection("Gold Farm")
	local section2 = page3:addSection("Gold Farm Settings")
	local section3 = page3:addSection("Items Farm")

	if readfile(DHV2BABFT .. "/LETSFARM.txt") == "true" then
		section1:addToggle("Auto Farm",true,function(bool)
			LETSFARM = bool
			if LETSFARM == true then
				writefile(DHV2BABFT .. "/LETSFARM.txt","true")
			else
				writefile(DHV2BABFT .. "/LETSFARM.txt","false")
			end
			cancelled = not toggled
			if toggled then
				if not infarm then
					farm()
				end
			else
				player.Character:WaitForChild("Humanoid").Health = 0
			end
			notify("Auto Farm: "..tostring(LETSFARM),true)
		end)
		cancelled = not toggled
		if toggled then
			if not infarm then
				farm()
			end
		else
			player.Character:WaitForChild("Humanoid").Health = 0
		end
		LETSFARM = true
	else
