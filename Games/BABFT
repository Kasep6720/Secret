	section3:addDropdown(readfile(DHV2BABFT .. "/ABIitem.txt"),items,function(CHOICE)
		game:GetService("Players").LocalPlayer.PlayerGui.ShopGui.MainFrame.TabFrame.ShopFrame.ScrollingFrameChests.Frame2XXX.Visible = true
		ABIitem = CHOICE
		writefile(DHV2BABFT .. "/ABIitem.txt",tostring(ABIitem))
		notify("Item Selected: " .. ABIitem)
	end)
	section3:addSlider("Item Amount", tonumber(readfile(DHV2BABFT .. "/ABIamount.txt")), 1, 1000, function(value)
		ABIamount = value
		writefile(DHV2BABFT .. "/ABIamount.txt",tostring(ABIamount))
	end)
	ABIamount = tonumber(readfile(DHV2BABFT .. "/ABIamount.txt"))

	spawn(function ()
		while wait(1) do
			if ABI then
				local args = {[1] = ABIitem,[2] = ABIamount}
				workspace.ItemBoughtFromShop:InvokeServer(unpack(args))
			end
		end
	end)

	local function farm()
		local platform = Instance.new("Part", player.Character)
		platform.Anchored = true
		platform.Transparency = 1
		platform.Size = Vector3.new(6, 1, 6)

		local connection
		connection = game:GetService("RunService").RenderStepped:connect(function()
			if player.Character:FindFirstChild("HumanoidRootPart") then
				platform.Position = player.Character.HumanoidRootPart.CFrame * Vector3.new(0, -3.5, 0)
			else
				connection:Disconnect()
			end
		end)
		infarm = true

		task.spawn(function()
			if not NOGOLDBLOCK then
				task.wait(2.9)
				firetouchinterest(player.Character.HumanoidRootPart, workspace.BoatStages.NormalStages.TheEnd.GoldenChest.Trigger, 0)
			end
		end)
		
		for i=1,10 do
			player.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages["CaveStage" .. i].DarknessPart.CFrame + Vector3.new(0, -30, 0)
			task.wait(1)
			if workspace.BoatStages.NormalStages:FindFirstChild("CaveStage" .. i + 1) then
				player.Character.HumanoidRootPart.CFrame = workspace.BoatStages.NormalStages["CaveStage" .. i + 1].DarknessPart.CFrame + Vector3.new(0, -30, -20)
			end
			task.wait(0.5)
		end
		if NOGOLDBLOCK then
			player.Character:WaitForChild("Humanoid").Health = 0
		else
			player.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").BoatStages.NormalStages.TheEnd.GoldenChest.Trigger.CFrame
		end

		infarm = false
	end

	local Timer = 0
	spawn(function()
		while wait() do
			if LETSFARM then
				if Timer < 61 then
					wait(1)
					Timer = Timer + 1
					print("Time: " .. Timer)
				else
					player.Character:WaitForChild("Humanoid").Health = 0
					notify("Automatically reset character due to treasure chest is glitched")
					Timer = 0
				end
			end
		end
	end)

	player.CharacterAdded:Connect(function()
		if LETSFARM then
			workspace.ClaimRiverResultsGold:FireServer()
			task.wait(4)
			Timer = 0
			farm()
		end
	end)

	-- forth page
	local page4 = NoobieDoge:addPage("Teleports", 6052531413)
	local section1 = page4:addSection("Teams")

	section1:addButton("Black Team",function()
		wait()
		player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").BlackTeam.SpawnPath.Union.CFrame+Vector3.new(0,4,0)
	end)
	section1:addButton("Blue Team",function()
		wait()
		player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace")["Really blueTeam"].SpawnPath.Union.CFrame+Vector3.new(0,4,0)
	end)
	section1:addButton("Green Team",function()
		wait()
		player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").CamoTeam.SpawnPath.Union.CFrame+Vector3.new(0,4,0)
	end)
	section1:addButton("Magenta Team",function()
		wait()
		player.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").MagentaTeam.SpawnPath.Union.CFrame+Vector3.new(0,4,0)
	end)
	section1:addButton("Red Team",function()
		wait()
