								end)
							until vd:FindFirstChild("HP").Value == 0 or FarmSirShark == false or (vd:FindFirstChild("HP").Value == 0 and game:GetService("Workspace").Objects:FindFirstChild("Lord of Sands") and game:GetService("Workspace").Objects:FindFirstChild("Lord of Sands"):FindFirstChildOfClass("Part") and (game:GetService("Workspace").Objects:FindFirstChild("Lord of Sands").HP.Value ~= 0))
						end
					end
				end
			end
		end)
	end
	
	function FARMMT()
		pcall(function()
			if game:GetService("Workspace").Objects:FindFirstChild("Minotaur") and game:GetService("Workspace").Objects:FindFirstChild("Minotaur"):FindFirstChildOfClass("Part") and game:GetService("Workspace").Objects:FindFirstChild("Minotaur"):FindFirstChild("HP") and game:GetService("Workspace").Objects:FindFirstChild("Minotaur").HP.Value ~= 0 then
				local function getNearest()
				local nearest, dist = nil, 99999
				for i, v in pairs(game:GetService("Workspace").Objects:GetChildren()) do
					if v.Name == "Minotaur" and (v:FindFirstChild("BOTTOM") or v:FindFirstChild("HRP") or v:FindFirstChild("Part")) and v:FindFirstChild("HP") and v:FindFirstChild("HP").Value ~= 0 then
						local m =(player.Character.Head.Position-v:FindFirstChildOfClass("Part").Position).magnitude
							if(m<dist)then
								dist = m
								nearest = v
							end
						end
					end
				return nearest
				end
				local target = getNearest()
				repeat wait()
					pcall(function()	
						if (target:FindFirstChildOfClass("Part") or target:FindFirstChildOfClass("MeshPart")) and FarmMinotaur then
							for i,wand in pairs(player.Backpack:GetChildren()) do
								if wand:FindFirstChild("Handle") and wand:FindFirstChild("SPAWNER") then
									player.Character.Humanoid:EquipTool(wand)
								end
							end
							for i, v1 in pairs(player.Character:GetChildren()) do
								if v1:IsA("BasePart") then
									if FarmSirShark or noclips then
										v1.CanCollide = false
									else
										v1.CanCollide = true
									end
								end
							end
							if(target~=nil)then
								for i, v1 in pairs(target:GetChildren()) do
									if target:FindFirstChild("Location") then
										if v1.ClassName == "Part" and v1.Name ~= "BOTTOM" then
											if FarmTween then
												toTarget(player.Character.HumanoidRootPart.Position,v1.Position,v1.CFrame * CFrame.new(0, 4, 5.5))
											elseif FarmTP then
												player.Character.HumanoidRootPart.CFrame = v1.CFrame * CFrame.new(0, 4, 5.5)
											else
												wait(10)
												notify("Bruh.")
											end
											game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v1.Position)
										end
									else
										if v1.ClassName == "Part" and v1.Name == "BOTTOM" then
											if FarmTween then
												toTarget(player.Character.HumanoidRootPart.Position,v1.Position,v1.CFrame * CFrame.new(0, 4, 5.5))
											elseif FarmTP then
												player.Character.HumanoidRootPart.CFrame = v1.CFrame * CFrame.new(0, 4, 5.5)
											else
												wait(10)
												notify("Bruh.")
											end
											game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v1.Position)
										end
									end
								end
								AUCLICK1 = true
							end
						else
							AUCLICK1 = false
						end
						player.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
					end)
				until target == nil or target:FindFirstChild("HP").Value == 0 or FarmMinotaur == false 
			else
				if FarmMinotaur then
					for i, vGS in pairs(game.Workspace.GemSpawns:GetChildren()) do
						for i, v in pairs(vGS:GetChildren()) do
							if v.Name == "Gem" and v:FindFirstChild("PROXIMITY") and v:FindFirstChild("Sp") then
								if FarmTween and FarmMinotaur then
									toTarget(player.Character.HumanoidRootPart.Position,v.Position,v.CFrame * CFrame.new(0, 0, 0))
									wait(.5)
									game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.Position)
									VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
									VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
									wait(.1)
								elseif FarmTP and FarmMinotaur then
									player.Character.HumanoidRootPart.CFrame = v.CFrame * CFrame.new(0, 0, 0)
									wait(.5)
									game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.Position)
									VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
									VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
									wait(.1)
								end
							end
						end
					end
					if FarmMinotaur then
						player.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(-355, 9.5, 1048))
						wait()
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						wait(.1)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						wait()
						player.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(-359, 12, 1030))
						wait()
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						wait(.1)
						player.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(-372, 12, 1021))
						wait()
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						wait(.1)
						player.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(-387, 9.5, 1018.7))
						wait()
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						wait(.1)
						player.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(-398, 12, 1032))
						wait()
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						wait(.1)
						player.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(-397, 12, 1047))
						wait()
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						wait(.1)
						player.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(-356.8, 11.7, 1048))
						wait()
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						wait(.1)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
						VirtualInputManager:SendKeyEvent(true,"E",false,uwu)
					end
				end
			end
		end)
	end
	
	AUCLICK1 = false
	spawn(function()
		while wait() do
			if AUCLICK1 then
				if game:GetService("CoreGui")["DogeHub V2"].Main.Size == UDim2.new(0, 511, 0, 0) then
					VirtualInputManager:SendMouseButtonEvent(869, 458, 0, true, game, 1)
					VirtualInputManager:SendMouseButtonEvent(869, 458, 0, false, game, 1)
					wait(1)
				end
			end
		end
	end)

	spawn(function()
		while wait() do
			if AutoFarm or FarmLordOfSands or FarmSirShark or FarmMinotaur then
				if AutoSellResources and player.PlayerGui.MainUI.TopSide.Backpack.Meter.BackgroundColor3 == Color3.fromRGB(255, 0, 0) then
					for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
						if v:FindFirstChild("Baracks") and v.Baracks:FindFirstChild("Slot 5") and v:FindFirstChild("PLAYERNAME") and v.PLAYERNAME:FindFirstChild("SurfaceGui") and v.PLAYERNAME.SurfaceGui:FindFirstChild("TextLabel") and v.PLAYERNAME.SurfaceGui.TextLabel.Text == player.Name then
							repeat wait()
								player.Character.HumanoidRootPart.CFrame = v.Baracks["Slot 5"].CFrame * CFrame.new(0, 8, 0)
							until player.PlayerGui.MainUI.TopSide.Backpack.Meter.BackgroundColor3 == Color3.fromRGB(0, 255, 0)
						end
					end
				else
					if AutoFarm then
						FARM()
					end
					if FarmLordOfSands then
						FARMLOS()
					end
					if FarmSirShark then
						FARMSS()
					end
					if FarmMinotaur then
						FARMMT()
					end
				end
			else
				AUCLICK1 = false
			end
		end
	end)

	player.CharacterAdded:Connect(function()
		if AutoFarm or FarmLordOfSands or FarmSirShark or FarmMinotaur then
			wait(1)
			for i,wand in pairs(player.Backpack:GetChildren()) do
				if wand:FindFirstChild("Handle") and wand:FindFirstChild("SPAWNER") then
					player.Character.Humanoid:EquipTool(wand)
				end
			end
			wait(.25)
			if not game.Workspace[player.Name]:FindFirstChildOfClass("Tool") then
				notify("You don't have a wand!")
				wait(1)
				player.Character:BreakJoints()
			end
		end
	end)

	spawn(function()
		while wait() do
			if AutoSellResources and (not AutoFarm or not FarmLordOfSands or not FarmSirShark or not FarmMinotaur) then
				if player.PlayerGui.MainUI.TopSide.Backpack.Meter.BackgroundColor3 == Color3.fromRGB(255, 0, 0) then
					for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
						if v:FindFirstChild("Baracks") and v.Baracks:FindFirstChild("Slot 5") and v:FindFirstChild("PLAYERNAME") and v.PLAYERNAME:FindFirstChild("SurfaceGui") and v.PLAYERNAME.SurfaceGui:FindFirstChild("TextLabel") and v.PLAYERNAME.SurfaceGui.TextLabel.Text == player.Name then
							repeat wait()
								player.Character:FindFirstChild("HumanoidRootPart").CFrame = v.Baracks["Slot 5"].CFrame * CFrame.new(0, 8, 0)
							until player.PlayerGui.MainUI.TopSide.Backpack.Meter.BackgroundColor3 == Color3.fromRGB(0, 255, 0)
						end
					end
				end
			end
		end
	end)

