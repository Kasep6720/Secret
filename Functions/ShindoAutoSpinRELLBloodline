repeat wait() until game:IsLoaded()
game.Workspace:WaitForChild("Camera"):WaitForChild("startup")
local player = game.Players.LocalPlayer
local VirtualUser=game:service'VirtualUser'
game:service'Players'.LocalPlayer.Idled:connect(function()
	warn("anti-afk")
	VirtualUser:CaptureController()
	VirtualUser:ClickButton2(Vector2.new())
end)
if workspace:FindFirstChild("CCoff") then
	game:GetService("Workspace").CCoff:Destroy()
end
local spins = player.statz.spins.Value

--spin slot 1
game:GetService('RunService').Stepped:connect(function()
	if spins > 0 then
		spins = player.statz.spins.Value
		kgvalue1 = player.statz.main.kg1.Value
		kgvalue2 = player.statz.main.kg2.Value
		kgvalue3 = player.statz.main.kg3.Value
		kgvalue4 = player.statz.main.kg4.Value
		if kgvalue1 ~= "RELL" and kgvalue2 ~= "RELL" and kgvalue3 ~= "RELL" and kgvalue4 ~= "RELL" then
			player.startevent:FireServer("spin", "kg1")
			print("Rolled: " .. kgvalue1)
			wait(1)
		elseif kgvalue1 == "RELL" then
			print("You have got: " .. kgvalue1)
			notify("You have got: " .. kgvalue1)
			player.startevent:FireServer("band", "Eye")
			wait(1)
			game:GetService('TeleportService'):Teleport(game.PlaceId, player)
		end
	else
		player.startevent:FireServer("band", "\128")
		wait()
		game:GetService('TeleportService'):Teleport(game.PlaceId, player)
	end
end)

--spin slot 2
game:GetService('RunService').Stepped:connect(function()
	if spins > 0 then
		spins = player.statz.spins.Value
		kgvalue1 = player.statz.main.kg1.Value
		kgvalue2 = player.statz.main.kg2.Value
		kgvalue3 = player.statz.main.kg3.Value
		kgvalue4 = player.statz.main.kg4.Value
		if kgvalue1 ~= "RELL" and kgvalue2 ~= "RELL" and kgvalue3 ~= "RELL" and kgvalue4 ~= "RELL" then
			player.startevent:FireServer("spin", "kg2")
			print("Rolled: " .. kgvalue2)
			wait(1)
		elseif kgvalue2 == "RELL" then
			print("You have got: " .. kgvalue2)
			notify("You have got: " .. kgvalue2)
			player.startevent:FireServer("band", "Eye")
			wait(1)
			game:GetService('TeleportService'):Teleport(game.PlaceId, player)
		end
	else
		player.startevent:FireServer("band", "\128")
		wait()
		game:GetService('TeleportService'):Teleport(game.PlaceId, player)
	end
end)

if player.gamepasses:FindFirstChild("genkai3") then
	--spin slot 3
	game:GetService('RunService').Stepped:connect(function()
		if spins > 0 then
			spins = player.statz.spins.Value
			kgvalue1 = player.statz.main.kg1.Value
			kgvalue2 = player.statz.main.kg2.Value
			kgvalue3 = player.statz.main.kg3.Value
			kgvalue4 = player.statz.main.kg4.Value
			if kgvalue1 ~= "RELL" and kgvalue2 ~= "RELL" and kgvalue3 ~= "RELL" and kgvalue4 ~= "RELL" then
				player.startevent:FireServer("spin", "kg3")
				print("Rolled: " .. kgvalue3)
				wait(1)
			elseif kgvalue3 == "RELL" then
				print("You have got: " .. kgvalue3)
				notify("You have got: " .. kgvalue3)
				player.startevent:FireServer("band", "Eye")
				wait(1)
				game:GetService('TeleportService'):Teleport(game.PlaceId, player)
			end
		else
			player.startevent:FireServer("band", "\128")
			wait()
			game:GetService('TeleportService'):Teleport(game.PlaceId, player)
		end
	end)
end

if player.gamepasses:FindFirstChild("genkai3") then
	--spin slot 4
	game:GetService('RunService').Stepped:connect(function()
		if spins > 0 then
			spins = player.statz.spins.Value
			kgvalue1 = player.statz.main.kg1.Value
			kgvalue2 = player.statz.main.kg2.Value
			kgvalue3 = player.statz.main.kg3.Value
