window1:Toggle("Camera lock to boss",false,function(bool)
	Aimyom = bool
end)

spawn(function()
	while wait() do
		if Aimyom then
			for i,v in pairs(workspace.npc:GetChildren()) do
				if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
					game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, v.HumanoidRootPart.Position)
				end
			end
		end
	end
end)

window1:Label("Character Functions", Color3.fromRGB(127, 143, 166))

window1:Button("SuperHuman",function()
	_G.Doge = true;
	_G.dogespeed = 80;

	local mt = getrawmetatable(game)
	local old = mt.__newindex
	setreadonly(mt, false)

	mt.__newindex = newcclosure(function(t,k,v)
		pcall(function()
			if t == game:GetService("Players").LocalPlayer.Character.Humanoid and k == "WalkSpeed" and _G.Doge then
				v = _G.dogespeed;
			end
		end)
		return old(t,k,v)
	end)
end)

window1:Button("Auto Charge Chi", function(bool)
	local chakra = string.split(game.Players.LocalPlayer.PlayerGui.Main.ingamearena.Bar.cha.Text,"CHI: ")[2]
	c = chakra:gsub("CH%: ","")
	local cha
	local function chakracheck()
		chakra = string.split(game.Players.LocalPlayer.PlayerGui.Main.ingamearena.Bar.cha.Text,"CHI: ")[2]
		c = chakra:gsub("CH%: ","")
		cha = c
		if game.Players.LocalPlayer.Character.Humanoid.WalkSpeed == 0 then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
		end
	end
	halfchi = chakra/1.5
	print("CHI will recharge at "..halfchi)
	spawn(function() 
		while wait() do
			if game.Players.LocalPlayer.Character.Humanoid.WalkSpeed == 0 then
				game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
			end
			chakracheck()
		end
	end)
	spawn(function() 
		while wait() do
			if tonumber(cha) < halfchi then
				game.Players.LocalPlayer.Character.combat.update:FireServer("key","c")
				wait(3)
			else
				game.Players.LocalPlayer.Character.combat.update:FireServer("key","cend")
			end
		end
	end)
end)

spawn(function()
	while wait() do
		if autofarm then
			if  player.currentmission.Value == nil then
				for i,v in pairs(workspace.npc:GetChildren()) do
					pcall(function()
						autoofarm = false
						autooofarm = false
						if v.ClassName == "Model" and v:FindFirstChild("npctype") and string.find(v.Name, "npc") and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Head.CFrame.Y > -1000 then
							repeat wait()
								player.Character.HumanoidRootPart.CFrame=v.HumanoidRootPart.CFrame+Vector3.new(0,190,0)
							until --[[v.Humanoid.Health == 0 or]] not autofarm
						end
					end)
				end
			end
		end
	end
end)

