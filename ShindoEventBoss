window3:Label("Settings", Color3.fromRGB(127, 143, 166))

window3:Button("Upgrade Performance",function()
	local decalsyeeted = true -- Leaving this on makes games look shitty but the fps goes up by at least 20.
	local g = game
	local w = g.Workspace
	local l = g.Lighting
	local t = w.Terrain
	t.WaterWaveSize = 0
	t.WaterWaveSpeed = 0
	t.WaterReflectance = 0
	t.WaterTransparency = 0
	l.GlobalShadows = false
	l.FogEnd = 9e9
	l.Brightness = 0
	settings().Rendering.QualityLevel = "Level01"
	for i,v in pairs(g:GetDescendants()) do
		if v:IsA("Part") or v:IsA("Union") or v:IsA("MeshPart") then
			v.Material = "Plastic"
			v.Reflectance = 0
		elseif v:IsA("Decal") and decalsyeeted then 
			v.Transparency = 1
		elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then 
			v.Lifetime = NumberRange.new(0)
		end
	end
end)

window3:Button("Remove Effects / Particles",function()
	game:GetService("Players").LocalPlayer.PlayerScripts.EFFECTS:Destroy()
end)

window3:Button("Switch to Dungeon script",function()

	local player = game.Players.LocalPlayer
	local ScreenGui = Instance.new("ScreenGui")
	local Frame = Instance.new("Frame")
	local TextLabel = Instance.new("TextLabel")
	local TextButton = Instance.new("TextButton")
	local TextButton2 = Instance.new("TextButton")

	ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

	Frame.Parent = ScreenGui
	Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Frame.Position = UDim2.new(0.37, 0, 0.425, 0)
	Frame.Size = UDim2.new(0, 220, 0, 120)

	TextLabel.Parent = Frame
	TextLabel.BackgroundColor3 = Color3.fromRGB(255, 163, 26)
	TextLabel.Size = UDim2.new(0, 220, 0, 90)
	TextLabel.Font = Enum.Font.TitilliumWeb
	TextLabel.Text = "Are you sure that you want to switch to dungeon script?"
	TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel.TextScaled = true
	TextLabel.TextSize = 35.000
	TextLabel.TextWrapped = true

	TextButton.Parent = Frame
	TextButton.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
	TextButton.Position = UDim2.new(0, 0, 0.750000006, 0)
	TextButton.Size = UDim2.new(0, 110, 0, 60)
	TextButton.Font = Enum.Font.TitilliumWeb
	TextButton.Text = "Yes"
	TextButton.TextColor3 = Color3.fromRGB(200, 200, 200)
	TextButton.TextScaled = true
	TextButton.TextSize = 40.000
	TextButton.TextWrapped = true

	TextButton2.Parent = Frame
	TextButton2.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
	TextButton2.Position = UDim2.new(0.5, 0, 0.750000006, 0)
	TextButton2.Size = UDim2.new(0, 110, 0, 60)
	TextButton2.Font = Enum.Font.TitilliumWeb
	TextButton2.Text = "No"
	TextButton2.TextColor3 = Color3.fromRGB(200, 200, 200)
	TextButton2.TextScaled = true
	TextButton2.TextSize = 40.000
	TextButton2.TextWrapped = true

	function OK21()
		for i,v in pairs(player.PlayerGui:GetChildren()) do
			if v.Name == "ScreenGui" then
				v:Destroy()
			end
		end
	end
	TextButton2.MouseButton1Click:connect(OK21)

	function OK2()
		for i,v in pairs(player.PlayerGui:GetChildren()) do
			if v.Name == "ScreenGui" then
				v:Destroy()
			end 
		end
		wait()
		Q = false 
		E = false 
		R = false 
		T = false 
		Y = false 
		F = false 
		G = false 
		H = false 
		V = false  
		B = false 
		N = false
		keel = false
		autofarm = false
		autoofarm = false
		autooofarm = false
		Aimyom = false
		scrollfarm = false
		Pnorm = false
		wait()
		library:Destroy()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Kasep6720/Secret/Shindo/ShindoDF"))()
	end
	TextButton.MouseButton1Click:connect(OK2)

end)

