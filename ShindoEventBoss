local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()
local w = library:CreateWindow("Boss Farm - Shindo Life")
_G.ToggleColor = Color3.fromRGB(117, 50, 168)
_G.ButtonColor = Color3.fromRGB(117, 50, 168)
_G.ButtonTextColor = Color3.fromRGB(255, 255, 255)
--AUTOFARM
getgenv().speed = 1000000
local b = w:CreateFolder("AutoFarm")
local autofarm
b:Toggle("Track Boss",function(bool)
	autofarm = bool
end)
--[[b:Toggle("No Clip", function(bool)
